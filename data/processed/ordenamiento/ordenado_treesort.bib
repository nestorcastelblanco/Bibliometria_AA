  title = {Harnessing the Potential of Artificial Intelligence in Language Learning: is AI Threat or Opportunity?},
  year = {2024},
  isbn = {9798400709036},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3644713.3644751},
  doi = {10.1145/3644713.3644751},
  abstract = {The current article evaluates the influence of Artificial Intelligence (AI) on the field of education, namely a foreign language learning. Artificial Intelligence comprises of the development of computers and other technological artifacts, which possess intelligence similar to humans characterized by cognitive ability, learning capabilities, problem-solving skills and the like. The study has discovered that AI has been widely integrated into education in various forms, at first as computer-related technologies, and later as web-designed platforms intended for teaching and learning. AI interference into education, especially into language teaching has enabled educators to carry out a number of administrative tasks, such as reviewing and grading students’ assignments, preparing different activities for lessons more effectively and within short time. In addition, AI has been embedded into miscellaneous chatbots and online learning platforms, offering wide range of study materials and making learners be more autonomous as well as self-confident. On the other hand, AI leverage in language learning has led to several issues in students’ overall performance which will be discussed more deeply in the article.},
  booktitle = {Proceedings of the 7th International Conference on Future Networks and Distributed Systems},
  pages = {292–297},
  numpages = {6},
  location = {Dubai, United Arab Emirates},
}

@inproceedings{10.1145/3657604.3664721,
  author = {Lin, Jionghao and Koedinger, Kenneth R.},
  title = {HAROR: A System for Highlighting and Rephrasing Open-Ended Responses},
  year = {2024},
  isbn = {9798400706332},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
