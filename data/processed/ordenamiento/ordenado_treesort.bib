  title = {The 3rd Workshop on Personal Intelligence with Generative AI},
  year = {2025},
  isbn = {9798400713316},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3701716.3717524},
  doi = {10.1145/3701716.3717524},
  abstract = {The emergence of large language models (LLMs) has ushered in a new era of generative AI, exhibiting broad human-level capabilities and achieving meaningful progress toward Artificial General Intelligence (AGI). To realize AGI that truly benefits each individual, one next key direction for generative AI is to develop personal intelligence -understanding individuals and enhancing personal thinking, planning, and life experiences through personalization. Although evolving generative AI toward personal intelligence is emerging, it has attracted considerable interest from both research institutions and industry, even becoming a strategic direction for many companies. This workshop aims to provide a platform for discussing innovative ideas that facilitate personal intelligence with generative AI, 1) envisioning the future of personal intelligence powered by generative AI, 2) item establishing benchmarks for personal intelligence tasks, 3) advancing user modeling/personalization techniques for generative models/agents, 4) enhancing real-world applications, and 4) ensuring privacy and trustworthiness. This workshop aims to deepen understanding, accelerate progress, and support the transformative development of personal intelligence. The website for the workshop is: https://generative-rec.github.io/workshop-www25.},
  booktitle = {Companion Proceedings of the ACM on Web Conference 2025},
  pages = {2377–2380},
  numpages = {4},
  keywords = {agent personalization, generative ai, large language models, llm personalization, personalization},
  location = {Sydney NSW, Australia},
}

