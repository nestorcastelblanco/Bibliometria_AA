@inbook{10.1145/3676641.3716267,
author = {Gu, Yufeng and Khadem, Alireza and Umesh, Sumanth and Liang, Ning and Servot, Xavier and Mutlu, Onur and Iyer, Ravi and Das, Reetuparna},
title = {PIM Is All You Need: A CXL-Enabled GPU-Free System for Large Language Model Inference},
year = {2025},
isbn = {9798400710797},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3676641.3716267},
abstract = {Large Language Model (LLM) inference uses an autoregressive manner to generate one token at a time, which exhibits notably lower operational intensity compared to earlier Machine Learning (ML) models such as encoder-only transformers and Convolutional Neural Networks. At the same time, LLMs possess large parameter sizes and use key-value caches to store context information. Modern LLMs support context windows with up to 1 million tokens to generate versatile text, audio, and video content. A large key-value cache unique to each prompt requires a large memory capacity, limiting the inference batch size. Both low operational intensity and limited batch size necessitate a high memory bandwidth. However, contemporary hardware systems for ML model deployment, such as GPUs and TPUs, are primarily optimized for compute throughput. This mismatch challenges the efficient deployment of advanced LLMs and makes users to pay for expensive compute resources that are poorly utilized for the memory-bound LLM inference tasks.We propose CENT, a CXL-ENabled GPU-Free sysTem for LLM inference, which harnesses CXL memory expansion capabilities to accommodate substantial LLM sizes, and utilizes near-bank processing units to deliver high memory bandwidth, eliminating the need for expensive GPUs. CENT exploits a scalable CXL network to support peer-to-peer and collective communication primitives across CXL devices. We implement various parallelism strategies to distribute LLMs across these devices. Compared to GPU baselines with maximum supported batch sizes and similar average power, CENT achieves 2.3x higher throughput and consumes 2.3x less energy. CENT reduces the Total Cost of Ownership (TCO), generating 5.2x more tokens per dollar than GPUs.},
booktitle = {Proceedings of the 30th ACM International Conference on Architectural Support for Programming Languages and Operating Systems, Volume 2},
pages = {862–881},
numpages = {20}
}

@inbook{10.1145/3756423.3756511,
author = {Ng, Tin Hon},
title = {The Role of Artificial Intelligence in 3D Printing Systems},
year = {2025},
isbn = {9798400714351},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3756423.3756511},
abstract = {This paper explores the role of artificial intelligence (AI) in optimizing the 3D printing industry. AI is developing fast, with a large number of applications shown, such as polishing the statement. The rapid development of AI has allowed its integration into various sectors, including 3D printing. While the applications of AI in many industrial systems have been identified by existing studies, the applications of this emerging technology in another promising technology – 3D printing – have been largely unexplored. This points out the necessity of conducting researches on this issue to facilitate the further development of both AI and 3D printing technologies. This study examines the application of AI across different phases of 3D printing, such as design, usage, production, and maintenance. AI enhances the efficiency of 3D printing by providing generative design alternatives, automating parameter settings, optimizing process controls, and improving product quality through real-time monitoring. AI's ability to store and analyze vast amounts of data enables better decision-making, predictive maintenance, and customized production, reducing costs and enhancing production capabilities. However, AI implementation in 3D printing also faces challenges, including system complexity, data privacy concerns, and the need for extensive sensor networks. As AI technologies continue to evolve, they promise further advancements in flexibility, precision, and cost-efficiency, potentially revolutionizing the 3D printing industry. This paper provides a comprehensive analysis of AI's applications in 3D printing, offering insights into how AI can drive future innovation in additive manufacturing.},
booktitle = {Proceedings of the 2025 International Conference on Artificial Intelligence and Smart Manufacturing},
pages = {531–535},
numpages = {5}
}

@inproceedings{10.1145/3641519.3657489,
author = {Gu, Zeqi and Yang, Ethan and Davis, Abe},
title = {Filter-Guided Diffusion for Controllable Image Generation},
year = {2024},
isbn = {9798400705250},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3641519.3657489},
doi = {10.1145/3641519.3657489},
abstract = {Recent advances in diffusion-based generative models have shown incredible promise for zero shot image-to-image translation and editing. Most of these approaches work by combining or replacing network-specific features used in the generation of new images with those taken from the inversion of some guide image. Methods of this type are considered the current state-of-the-art in training-free approaches, but have some notable limitations: they tend to be costly in runtime and memory, and often depend on deterministic sampling that limits variation in generated results. We propose Filter-Guided Diffusion (FGD), an alternative approach that leverages fast filtering operations during the diffusion process to support finer control over the strength and frequencies of guidance and can work with non-deterministic samplers to produce greater variety. With its efficiency, FGD can be sampled over multiple seeds and hyperparameters in less time than a single run of other SOTA methods to produce superior results based on structural and semantic metrics. We conduct extensive quantitative and qualitative experiments to evaluate the performance of FGD in translation tasks and also demonstrate its potential in localized editing when used with masks.},
booktitle = {ACM SIGGRAPH 2024 Conference Papers},
articleno = {101},
numpages = {10},
keywords = {Diffusion Models, Generative Artificial Intelligence, Image Synthesis, Style Transfer},
location = {Denver, CO, USA},
series = {SIGGRAPH '24}
}

