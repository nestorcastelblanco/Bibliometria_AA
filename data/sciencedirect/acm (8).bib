author = {Gao, Guanghui and Deng, Dapeng and Shi, Gang and Guo, Yining and Ning, Zhaole},
title = {Research on Methodology of Intelligent Traffic Accident Detection based on Enhanced YOLOv8 Algorithm},
year = {2024},
