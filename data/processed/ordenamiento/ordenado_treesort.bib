@inproceedings{10.1145/3660043.3660176,
  author = {Liu, Qianwen},
  title = {Online Education Big Data Encryption Algorithm System Based on Artificial Intelligence},
  year = {2024},
  isbn = {9798400716157},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3660043.3660176},
  doi = {10.1145/3660043.3660176},
  abstract = {With the rapid development of online education, a large amount of educational data is transmitted and stored on the network. This data includes sensitive information about students’ personal information and learning records, so protecting the security of this data becomes critical. The traditional AES encryption algorithm has been widely used, but as computing power increases, the risk of cracking it also increases. Therefore, this article studies an online education big data encryption algorithm based on artificial intelligence (AI)-assisted optimization, focusing on AI technology to help analyze and process large-scale online education data, and the use of AI technology to optimize traditional Advanced Encryption Standard (AES) encryption algorithm. The results of the two sets of simulation experiments are as follows: the average cracking success rate after optimization is 0.028\%, which is one-fifth of that before optimization, and the encryption time is reduced by about 37 seconds on average. This research can enhance users' trust in data privacy, thereby promoting users to participate more actively in online learning activities.},
  booktitle = {Proceedings of the 2023 International Conference on Information Education and Artificial Intelligence},
  pages = {744–749},
  numpages = {6},
  location = {Xiamen, China},
}

@inproceedings{10.1145/3654522.3654607,
