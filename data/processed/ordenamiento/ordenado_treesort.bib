@article{10.5555/3744154.3744166,
  author = {Suh, Sang and Ravelo, Jesus and Strogalev, Nikita},
  title = {Impact of Artificial Intelligence on Student's Education},
  year = {2025},
  issue_date = {April 2025},
  publisher = {Consortium for Computing Sciences in Colleges},
  address = {Evansville, IN, USA},
  volume = {40},
  number = {7},
  issn = {1937-4771},
  abstract = {Over the past decade, Artificial Intelligence (AI) has slowly but surely found its way into almost every corner of our educational system forever changing how we learn and interact with information. This paper looks into how AI is transforming students everyday academic lives, from getting quick answers to challenging questions to receiving personalized support unique to their learning styles. We focus a lot on AI-powered chatbots—these tools not only offer round-the-clock academic help, but also help students with administrative tasks, like figuring out class schedules or financial aid information. By reviewing various studies, we see that AI often leads to better engagement and improved learning outcomes. In this study, we use surveys and data analysis to capture students' thoughts about AI in their day-to-day studies. Our findings suggest that while AI holds a lot of promise for making education more accessible and effective, we need to be careful and thoughtful about how we roll it out so all students can really benefit.},
  journal = {J. Comput. Sci. Coll.},
  pages = {80–90},
}

