@article{10.1145/3721846,
author = {Bhargava, Hemant K. and Brown, Susan and Ghose, Anindya and Gupta, Alok and Leidner, Dorothy and Wu, D. J.},
title = {Exploring Generative AI’s Impact on Research: Perspectives from Senior Scholars in Management Information Systems},
year = {2025},
issue_date = {June 2025},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
volume = {16},
number = {2},
issn = {2158-656X},
url = {https://doi.org/10.1145/3721846},
doi = {10.1145/3721846},
abstract = {This commentary reflects on insights from a panel discussion at the 2024 Annual MIS Academic Leadership Conference, where six senior MIS scholars discussed the impact of Generative AI on scholarly research and peer review. The discussion underscored the importance of responsible use, transparency, and ethical standards, as well as the irreplaceable role of human judgment in maintaining research integrity. This commentary explores the potential of Generative AI as a collaborative tool across various stages of the research lifecycle, highlighting the "human-in-the-loop" approach to harness AI's capabilities while preserving essential human insight. This commentary synthesizes the senior scholars’ perspectives on the responsible integration of Generative AI, emphasizing opportunities to enhance research efficiency and foster interdisciplinary collaboration, while advocating for policies that ensure AI supports—rather than substitutes—human intellectual contributions in academic research.},
journal = {ACM Trans. Manage. Inf. Syst.},
month = may,
articleno = {19},
numpages = {9},
keywords = {Generative Artificial Intelligence (GenAI), Large Language Models (LLMs), research integrity, peer review}
}

@inproceedings{10.1145/3759928.3759934,
author = {Lv, Qiyou and Chen, Zehong and Luo, Xuliang},
title = {Intelligent system for illegal behavior safety supervision based on improved YoLov5 and generative visual big model},
year = {2025},
isbn = {9798400715884},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3759928.3759934},
doi = {10.1145/3759928.3759934},
abstract = {There are a large number of violations of safety regulations at the construction site of the power grid. These behaviors do not comply with safety supervision norms and can cause significant safety hazards. In response to the problem of difficulty in capturing and alerting such violations, we have designed an intelligent recognition system for violations based on enhanced YOLOv5 and generative visual big models to automatically detect violations and potential safety hazards at the construction site of the power grid. We have constructed a private dataset of violations at the construction site of the power grid. In our improved YOLOv5 model, we first added a spatial channel attention mechanism to the Neck section of Yolov5 to enhance its ability to extract spatial features and distinguish between different channel features, enabling the network to better distinguish different targets. In addition, we have added a spatial attention mechanism after the SPP of the backbone network, which enables the network to fully utilize spatial information, extract more prominent features, and improve the accuracy and recall of the model. In addition, to compensate for the lack of negative samples in the dataset, we use a generative visual model to generate a large number of negative sample images to enhance the dataset and address the issue of insufficient negative samples. We have designed a real-time safety supervision intelligent system for detecting violations on construction sites based on the above model and method, using IoT devices such as on-site real-time cameras, intermediate data servers, and routers. We conducted extensive experiments on real construction site datasets, and the results showed that our method has significantly improved compared to the original model. The MAP of the system is 94.0\%, indicating that the model has strong detection ability and robustness in identifying personnel and safety tools in power grid construction site scenarios.},
booktitle = {Proceedings of the 2nd International Conference on Image Processing, Machine Learning, and Pattern Recognition},
pages = {32–38},
numpages = {7},
keywords = {Generative Visual Large Model, Illegal behavior, Improved YoLov5, Security Supervision},
location = {
},
series = {IPMLP '25}
}

@article{10.1145/3747355,
author = {Johnson, Jeffrey and Denning, Peter and Odlyzko, Andrew and Walker, Martin and Delic, Kemal and Yaffe, Phil and Picton, Phil},
title = {Artificial Intelligence: Foundational Technologies of Artificial Intelligence},
year = {2025},
issue_date = {July 2025},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
volume = {2025},
number = {July},
url = {https://doi.org/10.1145/3747355},
doi = {10.1145/3747355},
abstract = {More than the 70 years since its emergence in the early 1950s, artificial intelligence (AI) is performing cognitive tasks traditionally considered the unique province of humans. This progress did not occur in a vacuum. AI emerged against a rich background of technologies from computer science and ideas about intelligence and learning from philosophy, psychology, logic, game theory, and cognitive science. We sketch out the enabling technologies for AI. They include search, reasoning, neural networks, natural language processing, signal processing and computer graphics, programming and conventional software engineering, human-computer interaction, communications, and specialized hardware that provides supercomputing power. Beyond these technologies is the notion of Artificial General Intelligence that has or exceeds the capabilities of the human brain. Currently this is completely aspirational and is not expected to be possible before 2025, if ever. Artificial Intelligence is based on a variety of technologies, none of which seek to emulate human intelligence.},
journal = {Ubiquity},
month = jul,
articleno = {2},
numpages = {10}
}

