@inproceedings{10.1145/3706599.3720238,
  author = {Wu, Zi-Xin and Lin, Li and Chuang, Ling-I and Shen, Jian and Lai, Ruei Ci and Liang, Rung-Huei and Cheng, Yu-Ting and Chen, Chien-Hsiung},
  title = {Dream Relay: Exploring Dream Sharing with Generative AI},
  year = {2025},
  isbn = {9798400713958},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3706599.3720238},
  doi = {10.1145/3706599.3720238},
  abstract = {To better understand people’s inner worlds, the HCI community has explored ways to understand dream experience through generative AI (GAI). This paper presented “dream relay,” a novel approach to sharing dreams by integrating GAI. In the relay, participants used a dream probe to record their dreams, which were then visualized by GAI and shared with the next person as stimuli. We conducted a probe-based, group autoethnography study to explore how dream sharing was influenced in the way of relay and with GAI. Results showed how participants recorded their dreams and perceived the AI-generated dream visuals. This study also highlighted the value of GAI in sparking imagination and alleviating personal privacy concerns, which increased the willingness to share. Finally, we discussed the human-AI collaboration in reshaping dreams, as well as the role of GAI in facilitating social interaction.},
  booktitle = {Proceedings of the Extended Abstracts of the CHI Conference on Human Factors in Computing Systems},
  articleno = {202},
  numpages = {7},
  keywords = {Dreams, Dream sharing, Dream visualization, Generative AI, Dream privacy},
  location = {},
}

@inproceedings{10.1145/3708359.3712078,
  author = {Zhao, Yijun and Li, Zhengke and Wang, Yicheng and Cai, Xueyan and Zhou, Xiaojing and Yan, Yifan and Jin, Kecheng and Ding, Shiying and Shao, Yilin and Cao, Jiacheng and Wang, Pinhao and Yao, Cheng and Tao, Ye and Wang, Guanyun},
  title = {DreamDirector: Designing a Generative AI System to Aid Therapists in Treating Clients' Nightmares},
  year = {2025},
  isbn = {9798400713064},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3708359.3712078},
  doi = {10.1145/3708359.3712078},
  abstract = {Nightmares can adversely affect individuals’ mental health and well-being, necessitating timely psychological intervention. Current nightmare therapy has set high requirements for therapists, appeared abstract to clients, and showed poor interaction between them, due to its extensive information input, lack of sensory stimulation, and exclusive reliance on one-on-one conversation. We propose DreamDirector, a visual-interactive and narrative generative system powered by generative AI. Based on Imagery Rehearsal Therapy (IRT) and Nightmare Deconstruction and Reprocessing (NDR), the system can (1) recollect the nightmare scene, (2) interpret the dream with LLM, (3) reprocess the nightmare by generating therapeutic dream visuals using AI painting alongside meditative texts, and feedback with a picture book. Finally, we verified the usability of this system in terms of efficiency enhancement and interaction promotion through a user study with 2 therapists and 16 clients. The results revealed emotional relief among clients, with a positive and impressive attitude toward visual interaction.},
  booktitle = {Proceedings of the 30th International Conference on Intelligent User Interfaces},
  pages = {553–578},
  numpages = {26},
  keywords = {Nightmare therapy, System assist therapist, System design, Generative AI},
  location = {},
}

