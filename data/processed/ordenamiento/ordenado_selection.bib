 pages = {62–67},
 publisher = {Association for Computing Machinery},
 series = {ICCMT '25},
 title = {Research on Two-way Empowerment Mode and Path Optimization of Primary and Secondary School Teachers' Deep Participation in Normal University Training Based on Artificial Intelligence},
 url = {https://doi.org/10.1145/3757749.3757760},
 year = {2025}
}

@inproceedings{10.1145/3757749.3757768,
 abstract = {Cervical cancer is one of the most common malignant tumors among women worldwide. Early diagnosis and precise treatment are crucial for improving patient survival rates. In recent years, the rapid development of artificial intelligence (AI) in the medical field has shown great potential, particularly in the diagnosis, treatment, and prognostic assessment of cancer. This article focuses on the application of AI in cervical cancer, discussing the current status of cervical cancer, specific AI applications, challenges faced, and future prospects. AI has demonstrated significant potential in the diagnosis, treatment, and prognostic assessment of cervical cancer, promising to enhance diagnostic and therapeutic levels and promote the development of precision medicine. However, challenges such as model interpretability and data-related issues remain. Through comprehensive analysis, this article clarifies the importance and future directions of AI in cervical cancer research, thereby advancing the development of personalized medicine.},
