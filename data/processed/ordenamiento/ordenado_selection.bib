 publisher = {Association for Computing Machinery},
 series = {ICAIE '24},
 title = {Future Education: Freedom and Comprehensive Human Development in the Age of Artificial Intelligence},
 url = {https://doi.org/10.1145/3722237.3722374},
 year = {2025}
}

@inproceedings{10.1145/3722237.3722376,
 abstract = {This paper discusses the role of AI in various academic fields and its influence on the development of competitive talent. Reviewing existing literature on AI applications in science, engineering, arts, and more, it brings forth unique contributions to progress in each area. This paper will research cross-national disparities in the development of AI talent, how educational policies, as well as industrial demands, impact training. A mixed-method approach is used in the analysis, with data and case studies from various countries to understand AI talent development. AI applications vary across countries and fields, exposing gaps in policy and education. Advanced countries invest in AI healthcare research to take the lead in sophisticated diagnostic systems. It is in developing countries where AI helps most: increased productivity in agriculture. The priority and resources will be dictated by local needs. Education and its resources and initiatives are in place for talent development, guiding policymakers and industry leaders on how to develop AI. This will develop the expertise necessary for advancing global AI technology. For example, in the area of education, enough resources can be put in place to ensure that students are abreast of state-of-the-art facilities and knowledge. Interdisciplinary initiatives, such as the integration of computer science with psychology or sociology, can yield a comprehensive understanding of the interplay between artificial intelligence and human behavior as well as social structures. By acknowledging these factors, it is essential to formulate strategies that foster an ecosystem conducive to the growth of AI expertise and propel international advancements in AI technology.},
