year = {2025},
isbn = {9798400715624},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3756580.3756634},
doi = {10.1145/3756580.3756634},
abstract = {This study explores the systematic integration of Artificial Intelligence Generated Content (AIGC) in university English language teaching, focusing on the core challenge of optimising the teaching closure loop. Using a mixed research method (questionnaire survey, classroom experiment and content analysis), AIGC was found to have significant efficacy in multimodal resource generation, personalised feedback and intercultural scene simulation. The study proposes a three-dimensional collaborative teaching framework: teacher-AIGC-learners, which reveals that the core value of AIGC is to liberate teachers from mechanical labour and achieve precise teaching interventions. Meanwhile, the study faces the ethical dilemmas of academic integrity and data privacy, and proposes a governance framework that combines technological constraints, institutional norms, and humanistic education. The experimental data show that AIGC assistance increases students' writing complexity by 27.9\% and decreases their speaking anxiety index by 34\%, but there are also problems such as weakened critical thinking due to technological dependence. This study provides a reusable pedagogical model and risk coping strategy for the digital transformation of education.},
booktitle = {Proceedings of the 2025 6th International Conference on Education, Knowledge and Information Management},
pages = {334–342},
numpages = {9},
keywords = {adaptive learning, artificial intelligence-generated content (AIGC), ethical governance, hybrid research, ternary collaborative framework, university English language teaching},
location = {
},
series = {ICEKIM '25}
}

@inbook{10.1145/3756423.3756561,
author = {Fan, Ronghui and Chen, Dandan},
title = {Theoretical framework and simulation experiment analysis of material organization performance prediction driven by artificial intelligence},
year = {2025},
isbn = {9798400714351},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3756423.3756561},
abstract = {With the rapid development of artificial intelligence technology, more and more fields are leveraging its powerful data processing and pattern recognition capabilities for research, and materials science is no exception. Driven by artificial intelligence, this study proposes a material microstructure-performance prediction model. By conducting simulation experiments, it explores the relationship between the material's microstructure and its macroscopic properties. The model predicts and analyzes the mechanical properties, thermal properties, and corrosion resistance of typical metal materials such as steel, aluminum alloys, and titanium alloys, verifying its high accuracy and reliability. The artificial intelligence-driven model can effectively fit test data with high prediction accuracy, and the errors fall within a reasonable range. The combination of artificial intelligence with simulated annealing and genetic algorithms further optimizes the performance of the prediction model.},
booktitle = {Proceedings of the 2025 International Conference on Artificial Intelligence and Smart Manufacturing},
pages = {838–842},
numpages = {5}
}

