@inproceedings{10.1145/3677892.3677964,
  author = {Wang, Qifan},
  title = {Research on the Development Status of the Combination of Biology, Chemistry, Geology and Artificial Intelligence},
  year = {2024},
  isbn = {9798400709838},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3677892.3677964},
  doi = {10.1145/3677892.3677964},
  abstract = {With the development of technology, AI technology is gradually on the rise. Moreover, it has established close connections with various other fields. Biology, chemistry, geology, and so on are all closely related to the environment we live in. Therefore, exploring the extent of the connections between these three fields and AI is beneficial for us to have a clearer understanding of the applications of AI technology in these areas. To investigate the connections between AI artificial intelligence and these three disciplines, we crawled the top 10,000 most cited articles in each field from the Semantic Scholar [8] website, along with their corresponding paper IDs and author IDs, to prepare data for the subsequent citation networks and co-authorship networks. After obtaining the data, we created citation networks and co-authorship networks for each field, resulting in a total of six images, and analyzed various features of each graph. Finally, through comparing the data and images, we found that the complexity of the citation network is much higher than that of the co-authorship network. In the co-authorship network, the connection between the field of biology and AI is relatively independent, while chemistry and geology are more closely linked. In the citation network, geology exhibits two relatively independent parts, whereas biology and chemistry are more closely connected. This indicates that the application of biology to AI is multifaceted and independent, while geology has two major directions in its application to AI. Therefore, we can conclude that collaborators in biology have diverse research styles in their approach to AI, resulting in a more comprehensive application of AI.},
  booktitle = {Proceedings of the 2024 International Conference on Digital Society and Artificial Intelligence},
  pages = {468–473},
  numpages = {6},
  location = {Qingdao, China},
}

