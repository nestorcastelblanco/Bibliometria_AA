@inproceedings{10.1145/3766918.3766919,
author = {Wang, Qi and Jin, Zongzhen and Hou, Fei and Wang, Nana and Zhang, Yuchen},
title = {Risk perception Influences User satisfaction on AI-Powered Contract Generation Technologies},
year = {2025},
isbn = {9798400716027},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3766918.3766919},
doi = {10.1145/3766918.3766919},
abstract = {This study aims to explore the impact of risk perception on user satisfaction when using AI-driven contract generation technologies. As artificial intelligence(AI) continues to be widely applied in the fields of law and contract management, users' awareness of potential risks significantly affects their usage experience. The research indicates that concerns regarding data privacy and legal compliance often create discomfort among users, which in turn reduces their satisfaction levels. Additionally, the study highlights a strong demand for platform reasonable risk point alerts and user support, emphasizing the importance of enhancing user trust when promoting AI contract generation technologies. Although this study has limitations, including a small sample size and potential self-report bias, it provides a new perspective for understanding the relationship between risk perception and user satisfaction. Based on the findings, it is recommended that future research adopt diverse sampling methods and qualitative approaches to gain deeper insights into this field, considering the characteristics and needs of different user groups. Ultimately, establishing more transparent communication mechanisms and providing effective user support will be key strategies for enhancing user satisfaction, thereby facilitating the broader application and long-term development of AI technologies within the legal domain.},
booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence for Business},
pages = {1–6},
numpages = {6},
keywords = {Artificial Intelligence, Risk Perception, User Experience},
location = {
},
series = {GAIB '25}
}

