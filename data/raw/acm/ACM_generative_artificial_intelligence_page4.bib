@inproceedings{10.1145/3766918.3766954,
author = {Ye, Xiyuan and Li, Yantao},
title = {Corporate Investment Return Prediction Model Using Long Short-Term Memory Networks},
year = {2025},
isbn = {9798400716027},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3766918.3766954},
doi = {10.1145/3766918.3766954},
abstract = {In the context of digital transformation, enterprises increasingly rely on high-quality data mining techniques to enhance the scientific basis and foresight of investment decisions. Traditional statistical and machine learning methods face limitations in modeling the temporal dependencies inherent in financial and market data, making it difficult to capture long-term trends effectively. To address this challenge, this study proposes a corporate return on investment (ROI) prediction model based on Long Short-Term Memory (LSTM) networks. The model integrates corporate financial indicators, market behavior data, and macroeconomic variables to construct a deep learning framework for multivariate time series analysis. A sliding window mechanism and unified data preprocessing pipeline are developed to improve the model's capability of learning complex dynamic patterns. Experimental results demonstrate that the proposed model significantly outperforms benchmark methods such as ARIMA and SVR in terms of mean squared error, mean absolute error, and trend prediction accuracy, showing superior stability and long-term forecasting ability. This research confirms the feasibility of applying deep learning techniques to enterprise financial modeling and provides theoretical support and methodological reference for building intelligent investment research systems.},
booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence for Business},
pages = {221–225},
numpages = {5},
keywords = {LSTM time series model 2, ROI prediction 1, corporate financial modeling 3, deep learning applications 5, macroeconomic data integration 4},
location = {
},
series = {GAIB '25}
}

