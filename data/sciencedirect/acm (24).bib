abstract = {This article analyses the current research status of artificial intelligence in the field of ceramic art at home and abroad, as well as the problems and reasons of traditional ceramic art, and thoroughly studies the techniques of Chinese fine brushwork flower and bird painting, and innovatively integrates it into the creation of underglaze colourful ceramic works. At the same time, artificial intelligence technology was introduced, focusing on how to use artificial intelligence to help traditional ceramic art creation to improve its artistic expression and production efficiency. This paper uses convolutional neural network (CNN) as the infrastructure of the aesthetic evaluation model of underglaze colored ceramic art; selects several underglaze colored ceramic art design schemes generated based on generative adversarial network (GAN) to show their innovative design in modeling, decoration and color; color prediction and design scheme classification and sorting by support vector machine (SVM) and random forest (RF) machine learning algorithms. Combining the functions of artificial intelligence such as image recognition, data analysis and generation, and design, we actively explore the path of innovative development of traditional ceramic art in the context of the new era, and strive to achieve the deep integration of traditional craftsmanship and modern technology, thereby injecting new vitality into the inheritance and development of ceramic art and promoting it to a new stage of development.},
booktitle = {Proceedings of the 2nd Guangdong-Hong Kong-Macao Greater Bay Area International Conference on Digital Economy and Artificial Intelligence},
pages = {1771–1777},
numpages = {7},
keywords = {artificial intelligence, intangible cultural heritage, machine learning algorithms, the techniques of Chinese fine brushwork flower and bird painting, underglaze five-color ceramics},
location = {
},
series = {DEAI '25}
}

