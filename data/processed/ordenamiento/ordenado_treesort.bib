@article{10.1177/26339137231222481,
  author = {Karl J Friston and Maxwell JD Ramstead and Alex B Kiefer and Alexander Tschantz and Christopher L Buckley and Mahault Albarracin and Riddhi J Pitliya and Conor Heins and Brennan Klein and Beren Millidge et al.},
