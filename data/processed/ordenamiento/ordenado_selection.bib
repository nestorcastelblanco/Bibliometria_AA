 publisher = {Association for Computing Machinery},
 series = {A-TEST 2024},
 title = {Use of ChatGPT as an Assistant in the End-to-End Test Script Generation for Android Apps},
 url = {https://doi.org/10.1145/3678719.3685691},
 year = {2024}
}

@inproceedings{10.1145/3678726.3678735,
 abstract = {In the digital content design field, the integration of Artificial Intelligence Generated Content (AIGC) with brand design has attracted widespread attention. This study explores the "Human-Machine Collaboration" model between AIGC and designers, as well as the incorporation of Design Thinking (DT) to understand its impact on brand appeals and design processes. The results show that using AIGC can quickly adjust design directions, create brand images that meet expectations, and enhance design efficiency and creative diversity. The study notes some limitations: excessive reliance on AIGC may introduce technical challenges. This research suggests that an appropriate combination of AIGC and designer expertise can form a human-machine collaboration model, saving time while stimulating innovation. Designers utilizing tools to balance creativity and efficiency are adapting to new technologies, exploring how to use more convenient tools in design methods to drive design innovation.},
