@inbook{10.1145/3728725.3728808,
author = {Chen, Siqi and Zhang, Wenzhen and Chen, Xinye and Ji, Yuchao and Liu, Xinyu and Yu, Fan and Xu, Huazhen},
title = {Plant disease detection technology based on hybrid convolutional attention mechanism},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728808},
abstract = {As the scale of agricultural production continues to expand, plant diseases have become a major threat to global agricultural productivity. Although traditional disease detection methods are widely used, they generally suffer from low accuracy and complex operations, making them difficult to meet the needs of modern agriculture. In response to these problems, this article proposes a new deep learning model-LabInt. LabInt uses dual-branch independent convolution in Lab color space, which effectively reduces the impact of illumination changes on feature extraction. Through spatial reconstruction and multi-scale pooling technology, the model can deeply mine shallow disease features and use deep convolution instead of ordinary convolution to reduce computational costs and improve the model's fine-grained identification capabilities. In addition, in order to adapt to complex agricultural environments, LabInt introduces a multi-head self-attention (MHA) mechanism to dynamically enhance attention to disease characteristics and significantly reduce the interference of complex backgrounds. Experimental results show that LabInt exhibits excellent disease recognition performance in actual agricultural production environments, with key indicators such as precision and recall reaching high levels. Ablation studies show that each module makes an important contribution to model performance. Therefore, LabInt is not only innovative in theory, but also shows strong application potential in practice, and is expected to become an important tool to improve the efficiency of agricultural disease prevention and control.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {526–531},
numpages = {6}
}

@inbook{10.1145/3728725.3728810,
author = {Li, Wei and Zhang, Kangwei and Li, Penglin and Yan, Keyang},
title = {DAT-YOLO: Small Object Detection Model from the Perspective of Drones},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728810},
abstract = {A DAT-YOLO model incorporating deformable attention is proposed to address the challenges of dense target distribution, small pixel size, and uneven sample size in object detection from the perspective of unmanned aerial vehicles. This model is based on YOLOv8s and optimizes the Backbone and Neck structures, allowing the model network to be shallower in depth while being more focused on small targets; Using RepNCSPELAN4 structure instead of C2f structure to better adapt the model to different computing and hardware requirements; The deformable attention mechanism DAT is integrated into the spatial pyramid pooling layer, allowing the network to capture more effective features while expanding the receptive field; The use of inner-ShapeIoU loss function improves the algorithm's localization and classification performance for small targets in complex environments. The experimental results of DAT-YOLO on the VisDrone dataset show that with a 57.5\% reduction in parameter count, the mAP@50 and the mAP@50:95 has also increased by 6.7\% and 5.0\% respectively, making it suitable for drone target detection tasks.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {538–545},
numpages = {8}
}

@inbook{10.1145/3728725.3728750,
author = {Yang, Tonghui and Xu, Yamei},
title = {Super-resolution Reconstruction of Remote Sensing Images with Improved SwinIR Transformer},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728750},
abstract = {Aiming at the problem of large differences in feature information between different targets in remote sensing images and the complexity of the scene, an improved SwinIR Transformer remote sensing image super-resolution reconstruction algorithm is proposed, combining residual hybrid attention and spatial-gate feed-forward network. First, a new residual hybrid attention module (RHAB) is constructed, which is serially combined with multiple self-attention (MSA) to enhance feature extraction by mixing channel attention (CA), spatial attention (SA), and MSA, and to improve the model's ability in modeling locally and globally relevant information. Second, a spatial-gate feed-forward network (SGFN) is used instead of the multilayer perceptron (MLP) in the original network to effectively reduce redundant channel information, overcome the limitations of the MLP in dealing with local spatial features, and enhance the feature expression efficiency. The experiments use NWPU-RESISC45 and UCMerced_LandUse as the training set, RSOD-Dataset, RSSCN7 Data Set, and WHU-RS19 Data Set as the test set, and the model performance is evaluated by PSNR and SSIM metrics. The experimental results confirm that the model in this paper shows excellent performance in both objective evaluation and subjective visualization.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {157–162},
numpages = {6}
}

@inbook{10.1145/3728725.3728756,
author = {Wu, Keyu and Fu, Jingyi and Zhang, Wenfang},
title = {Aspect-based Capsule Neural Networks for Sentiment Analysis of Sensitive Information},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728756},
abstract = {To address the need for accurate sentiment masking of sensitive words in sensitive information, traditional sentiment analysis methods can only assess the overall polarity of a sentence, while existing Aspect-Based Sentiment Analysis (ABSA) mostly focuses on the field of commodity reviews, and insufficiently handles phenomena such as complex contexts and irony. To this end, this paper proposes the CapABSA model, which takes sensitive words as aspects, combines capsule neural networks to capture multidimensional semantic features, and migrates document-based sentiment analysis (DBSA) to ABSA through transfer learning to improve classification performance. Experiments show that the model performs effectively in sentence datasets containing sensitive words, providing a more fine-grained solution for sensitive information filtering.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {196–201},
numpages = {6}
}

@inbook{10.1145/3728725.3728825,
author = {Zhou, Ziye and Guo, Yanbin and Tang, Xin and Li, Kehan and You, Heng},
title = {Dynamic Decision Correction Framework Integrating Path Optimization and Incomplete Information Handling},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728825},
abstract = {Despite significant advancements in multi-agent systems, solving the optimal path problem and addressing incomplete information remain challenging in multi-agent reinforcement learning (MARL). Traditional MARL algorithms often struggle with these two critical issues. To address these challenges, this paper proposes a novel framework called DDCF(Dynamic Decision Correction Framework) that integrates deep reinforcement learning (DRL) with Long Short-Term Memory (LSTM) networks to optimize the decision-making process for agents navigating complex environments. The first key innovation of this framework is the application of LSTM networks, which enable agents to learn optimal paths over time while considering dynamic, temporal dependencies within the environment. The second key innovation is the introduction of a dynamic decision-making approach, which effectively handles the problem of incomplete information by enabling agents to make decisions based on both observable and inferred states. This decision method ensures that agents can intelligently handle uncertain environments by incorporating both certain and fuzzy information. Experimental results demonstrate that the proposed framework significantly outperforms traditional methods in terms of path optimization and decision-making performance, providing a robust solution for agents in environments characterized by incomplete information and complex decision tasks.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {632–642},
numpages = {11}
}

@inbook{10.1145/3728725.3728748,
author = {Bao, Qiaozhi and Chen, Yanlin and Ji, Xusheng},
title = {Research on evolution and early warning model of network public opinion based on online Latent Dirichlet distribution model and BP neural network},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728748},
abstract = {Online public opinion is increasingly becoming a significant factor affecting the stability of the internet and society, particularly as the frequency of online public opinion crises has risen in recent years. Enhancing the capability for early warning of online public opinion crises is urgent. The most effective approach is to identify potential crises in their early stages and implement corresponding management measures. This study establishes a preliminary indicator system for online public opinion early warning, based on the principles of indicator system construction and the characteristics and evolution patterns of online public opinion. Subsequently, data-driven methodologies were employed to collect and preprocess public opinion indicator data. Utilizing grey relational analysis and the K-Means clustering algorithm, we classified online public opinion events into three levels: slight, warning, and severe. Furthermore, we constructed an online topic evolution model using the online Hierarchical Dirichlet Process model to analyze the thematic changes of online public opinion events across different warning levels. Finally, we developed an online public opinion early warning model using a Backpropagation (BP) neural network. The test results of early warning samples show that the model achieves high accuracy. Thus, in practical early warning applications, the BP neural network can be effectively utilized for predicting online public opinion events.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {143–151},
numpages = {9}
}

@inbook{10.1145/3728725.3728828,
author = {Xu, Xintao},
title = {Exploration of Multi-modal Translation Resource Processing Modes under Computer Graphics: A Case Study of Visionbank},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728828},
abstract = {The rapid development of computer technology has brought computer graphics a qualitative leap in graphic images and visual communication. Computer graphics transforms digital information into image information while conducting digital processing and visualization of graphic images. Combining with the development process of computer graphics, this paper considers the application field of computer graphics, discusses the connotation deduction of multi-modal translation resource. Through case analysis of the Visionbank, this paper validates and advances computer graphics technology to optimize multi-modal translation resource, and realizes the comprehensive utilization of multi-modal translation resource.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {654–658},
numpages = {5}
}

@inbook{10.1145/3728725.3728834,
author = {Yuan, Yufeng and Han, Peng and Qiu, Jian and Liu, Dongmei and Peng, Li and Chen, Miao and Luo, Kaiqing},
title = {SLP-SLAM: A Multifeature Fusion Stereo SLAM Based on Structural Lines and Structural Planes},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728834},
abstract = {Accurate positioning and pose estimation of mobile robots in structured environments have persistently presented considerable challenges. This paper proposes SLP-SLAM, a multifeature fusion stereo simultaneous localization and mapping (SLAM) system that combines both structural lines and structural planes. We have enhanced the effectiveness of the method for extracting structural lines using the Manhattan World Assumption (MWA) and have suggested utilizing these lines to construct structural planes, which is different from the existing plane extraction methods. Furthermore, we develop the state estimator by minimizing the visual residuals of points, structural lines, and structural planes. As far as we know, this represents the inaugural stereo SLAM that concurrently employs points, structural lines, and structural planes for integrated optimization. An appraisal of our system on difficult public datasets has been conducted. The trajectory error associated with SLP-SLAM has been diminished by 15\% in comparison to contemporary state-of-the-art (SOTA) SLAM.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {685–690},
numpages = {6}
}

@inbook{10.1145/3728725.3728764,
author = {Zhao, Wei and Han, Shaolong and Wang, Xiang},
title = {Based on visual model and LSTM cyclic convolutional neural network Automatic recognition method for instrument readings},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728764},
abstract = {In order to achieve automatic acquisition of instrument readings, a recognition method based on visual model and Long Short Term Memory Network (LSTM) is proposed. This method introduces an attention mechanism model to find the location of the feature that has the greatest impact on the final prediction result in the feature map. Once the dial position is determined, this method utilizes Long Short Term Memory Network (LSTM) to recognize text within that position. The experimental results show that compared to traditional methods, this method avoids the character cutting and a large number of manual modeling operations required by traditional pointer reading recognition methods, significantly improving recognition accuracy.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {247–250},
numpages = {4}
}

