@inproceedings{10.1145/3718751.3718811,
  author = {Kou, Jingjing and Yuan, Guohua},
  title = {A Study of Scholar Influence Evaluation Algorithms Based on Open Peer Review in the Age of Artificial Intelligence},
  year = {2025},
  isbn = {9798400709753},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3718751.3718811},
  doi = {10.1145/3718751.3718811},
  abstract = {The continuous development of social media in scientific communication and the arrival of the era of artificial intelligence have provided more possibilities for evaluating the influence of scholars, making evaluation algorithms no longer limited to quantitative indicators. On the basis of existing scholars' influence evaluation algorithms at home and abroad, this article designs a scholar influence evaluation approach and algorithm based on open peer review. That is, the qualitative content of open peer review in scholar articles is processed by certain artificial intelligence technology to form multiple label sets that can represent the influence of scholars from different dimensions, thus exploring the multi angle evaluation of scholar influence from a quality perspective. Finally, this article conducts empirical research and comparative analysis on the effectiveness of the proposed algorithm using the open peer review platform F1000 as the data source platform. Empirical research has shown that the algorithm proposed in this article provides a new perspective and approach for evaluating the influence of scholars, which can preliminarily achieve multidimensional evaluation of scholars, endow scholars with personal academic profiles, and enhance the fullness and recognition of scholar influence. Comparative analysis shows that this algorithm does not have a significant correlation with bibliometric evaluation algorithms and can serve as a supplement to the existing evaluation system. The combination of traditional bibliometric analysis algorithms and open peer review based algorithms will be the future development trend of academic evaluation.},
  booktitle = {Proceedings of the 2024 4th International Conference on Big Data, Artificial Intelligence and Risk Management},
  pages = {377–385},
  numpages = {9},
  keywords = {artificial intelligence, evaluation, f1000, open peer review, scholar influence},
  location = {},
}

@inproceedings{10.1145/3744367.3744427,
  author = {Xu, Rong},
  title = {A Study of the Use of Artificial Intelligence in Differentiated Instruction},
  year = {2025},
  isbn = {9798400715068},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3744367.3744427},
  doi = {10.1145/3744367.3744427},
  abstract = {The study builds an AI support framework based on Tomlinson's theory, and achieves accurate learning diagnosis through five-dimensional data (cognitive, behavioural, emotional, environmental and social). Practice has proven that AI can dynamically optimise teaching. However, there are challenges in AI application: over-reliance on technology undermines teacher-student interaction, algorithmic bias exacerbates educational injustice, and data privacy risks need to be regulated. The study emphasises that AI in education should be based on the essence of “educating people”, and promote the role of teachers to shift from “knowledge transfer” to “growth guidance” under the ethical framework, and synergise technological empowerment and humanistic care. The future needs to explore localised adaptation paths. In the future, it is necessary to explore the local adaptation path, not only to make use of AI to break through the bottleneck of large-scale education but also to prevent the alienation of the technology, and build an intelligent education ecology of “teaching in accordance with one's aptitude”.},
  booktitle = {Proceedings of the 2025 International Conference on Artificial Intelligence and Educational Systems},
  pages = {381–386},
  numpages = {6},
  keywords = {Artificial Intelligence, Challenges, Differentiated Instruction, Pathways},
  location = {},
}

@inproceedings{10.1145/3745133.3745161,
  author = {Dai, Lian and Lai, Yulin and Gu, Yinhua and Guo, Caihui and Lei, Yan},
  title = {A Study on Optimization of Data Privacy Protection Algorithms Based on Distributed Artificial Intelligence Techniques},
  year = {2025},
  isbn = {9798400714375},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3745133.3745161},
  doi = {10.1145/3745133.3745161},
  abstract = {In the field of data privacy protection, data access is restricted by setting access privileges and authentication mechanisms. However, when data usage scenarios or access requirements change, the privilege settings need to be adjusted frequently, resulting in complex and inefficient management. Especially when dealing with large-scale distributed data, traditional methods are difficult to efficiently screen and eliminate malicious behavior clients, which further aggravates the risk of privacy leakage. Therefore, we study the optimization of data privacy protection algorithm based on distributed artificial intelligence technology. The key feature information is extracted to reflect the essential attributes of the data, in order to simplify the form of data representation. The federal learning algorithm in distributed artificial intelligence technology is used for data protection to efficiently screen and eliminate potential malicious clients. On this basis, we add appropriate amount of noise to the query process to protect the data from being leaked, and at the same time, dynamically adjust the data privacy protection algorithm and update the protection scheme in real time to cope with the changing nature of the data and its intrinsic relevance. The experimental results show that the optimization method of data privacy protection algorithm based on distributed artificial intelligence technology is significantly better than other comparative methods in reducing the possibility of privacy leakage, improving privacy protection efficiency, and reducing data loss rate. Moreover, the security and reliability of privacy data is as high as 90\%, fully verifying the superiority of the proposed method. The results indicate that this method provides an efficient, stable, and secure technical means for data privacy protection.},
  booktitle = {Proceedings of the 2025 International Conference on Digital Economy and Information Systems},
  pages = {162–168},
  numpages = {7},
  keywords = {Base Algorithm Optimization, Data Privacy Protection, Distributed Artificial Intelligence Techniques, Federated Learning, Privacy Preserving Algorithms},
  location = {},
}

@inproceedings{10.1145/3718751.3718783,
  author = {Ping, Zhe and Jiao, Dingyang},
  title = {A Study on the Application of Artificial Intelligence in DDoS Attack Defense: A Literature Review},
  year = {2025},
  isbn = {9798400709753},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3718751.3718783},
  doi = {10.1145/3718751.3718783},
  abstract = {As DDoS attack patterns become increasingly varied and challenging to detect with the advancement of the field, this paper aims to explore the possibilities of applying machine learning algorithms for the defense and detection of DDoS attacks. We selected papers published since 2000 concerning the aforementioned applications, investigated the algorithms described therein, and compared their data performance in the testing sections to explore the advantages and disadvantages. Our findings reveal that SVM exhibits excellent performance in detecting attack traffic, whereas decision trees and their enhanced versions are less prominent. Additionally, genetic algorithms are not suitable for attack traffic detection but excel in filtering irrelevant traffic features. These insights hold significant implications for both real-world applications in enhancing cybersecurity measures and theoretical contributions by laying groundwork for future research in the rapidly evolving domain of DDoS attack mitigation. In conclusion, this study offers a comprehensive overview of the landscape of machine learning in DDoS defense, providing valuable insights and directions for future investigations.},
  booktitle = {Proceedings of the 2024 4th International Conference on Big Data, Artificial Intelligence and Risk Management},
  pages = {200–205},
  numpages = {6},
  keywords = {Artificial Intelligence, DDoS Attack, Machine Learning},
  location = {},
}

@inproceedings{10.1145/3747227.3747262,
  author = {Ji, Fang and Zheng, Xiaoyu and Xue, Haozhong and Wang, Jun},
  title = {A study on the application of artificial intelligence in personalized Go-to-market strategy in retail industry},
  year = {2025},
  isbn = {9798400714382},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3747227.3747262},
  doi = {10.1145/3747227.3747262},
  abstract = {In order to improve the accuracy and efficiency of personalized offer strategies in the retail industry, a dynamic pricing and personalized recommendation system is constructed based on multi-source data fusion and intelligent decision-making models. Analyzing the homogenization problem of traditional offer strategies, deep learning and reinforcement learning algorithms are used to optimize the construction of user profiles, the prediction of purchasing behavior and the generation of offer strategies. The results show that the intelligence-driven personalized Go-to-market strategy effectively improves the user conversion rate and customer unit price, optimizes the inventory turnover efficiency, and enhances the ability to accurately deploy marketing resources. Further research can focus on data privacy protection, cross-platform adaptability and computational cost optimization to enhance the stability and value of the strategy.},
  booktitle = {Proceedings of the 2025 International Conference on Machine Learning and Neural Networks},
  pages = {217–222},
  numpages = {6},
  keywords = {dynamic pricing, personalized Go-to-market strategy, retail industry, user profiling},
