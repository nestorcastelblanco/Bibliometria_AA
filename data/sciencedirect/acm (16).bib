@inbook{10.1145/3728725.3728824,
author = {Yang, Chenming},
title = {Three-Dimensional Path Planning for Unmanned Aerial Vehicles Based on a Hybrid Strategy of Dung Beetle Optimization Algorithm and Grey Wolf Optimization Algorithm},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728824},
abstract = {This paper presents a novel UAV three-dimensional path planning method by integrating the Dung Beetle Optimization (DBO) algorithm and the Grey Wolf Optimization (GWO) algorithm. Aiming at the problems of traditional algorithms in UAV path planning, such as local optima trapping and low convergence accuracy, this hybrid algorithm combines the advantages of both algorithms. The Fuch and Logistic chaotic maps are used for population initialization to enhance the global search ability. During the optimization process, the DBO algorithm's strong local exploitation ability and the GWO algorithm's global search characteristics are coordinated. A B\'{e}zier curve is employed to smooth the planned path. Simulation results demonstrate that the proposed hybrid algorithm outperforms traditional algorithms in terms of path length, convergence speed, and path smoothness, validating its effectiveness in UAV three-dimensional path planning.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {626–631},
numpages = {6}
}

@inproceedings{10.1145/3728725.3728774,
author = {Ye, Jianbing and Lin, Chaowen and Li, Qi},
title = {Line Detection Based on Hough Transform and its Application},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728774},
doi = {10.1145/3728725.3728774},
abstract = {Line detection is an important task in computer vision, which is widely used in fields such as automated driving and remote sensing image processing. As a classical image processing method, Hough transform has strong robustness and anti-interference ability in line detection. In this paper, line detection based on Hough transform is implemented and accelerated to hardware implementation on High-Level Synthesis (HLS) platform. By performing edge detection on the input image, the Hough transform maps the image from Cartesian space to parameter space to detect line features in the image. Experimental results show that the method can accurately detect lines in the image while exhibiting strong noise rejection. The implementation not only improves the efficiency of line detection, but also provides an effective solution for hardware acceleration applications based on the Hough transform.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {307–311},
numpages = {5},
keywords = {Edge Detection, Hardware Acceleration, High-Level Synthesis, Hough Transform, Line Detection},
location = {
},
series = {GAIIS '25}
}

@inbook{10.1145/3728725.3728778,
author = {Sun, Minghai and Cheng, Yufan},
title = {Research and application of K-means clustering based on the color characteristics of woodblock prints of Zhuxianzhen},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728778},
abstract = {Objective This study aims to quantitatively analyze the color composition of woodblock prints of Zhuxianzhen, explore its color combination patterns, and provide a theoretical basis for the color design of cultural and creative products, so as to more accurately inherit and present the visual characteristics of this traditional Chinese folk art. Methods In this study, K-means clustering algorithm is used to extract color features from selected sample images of woodblock prints of Zhuxianzhen, and a color association network model based on the priority of the main color is constructed by calculating the proportion of color distribution and its combination frequency. Based on the results of the analysis of the color matching law of this model, the practice of innovative visual design and cultural and creative product development of Zhuxianzhen woodblock prints is carried out. Conclusion The results of the research present the color matching and color matching law of Zhuxianzhen woodblock prints in the form of icons, which can reflect the color characteristics of Zhuxianzhen woodblock prints in a more objective way, provide the designers with scientific and rich color matching solutions, improve the efficiency of designers' work, and at the same time, promote the re-design application of the traditional Chinese folk art.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {332–336},
numpages = {5}
}

@inbook{10.1145/3728725.3728816,
author = {Zhu, Qifei and Tang, Jiangang and Cao, Haoshun},
title = {An Improved LSTM Crime Prediction Model Based on Environmental Factors},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728816},
abstract = {Crime prediction serves as a fundamental basis for preventing crime, maintaining social stability, and ensuring public safety. Current crime prediction models commonly suffer from issues such as low accuracy, unstable predictions, or weak generalization capabilities. Therefore, we constructed an improved LSTM crime prediction model based on environmental factors, integrated the data of various environmental factors such as economy, culture and climate, improved the LSTM network structure and optimization method, and enhanced the prediction ability of crime in the region. Empirical evidence shows that the MAPE predicted by the model is 0.0491 and R2 is 0.9229. Compared with the traditional model, the accuracy, stability and generalization ability of the prediction are significantly improved, which can provide a basis for the formulation of crime prevention and control strategies, improve the level of social security management, and promote social stability.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {577–584},
numpages = {8}
}

@inbook{10.1145/3728725.3728781,
author = {Li, Chan and Guo, Wenjing and Qin, Jikai},
title = {Backward automatic screening feature selection algorithm based on multi-evaluation criterion fusion},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728781},
abstract = {In view of the tedious problem that the feature selection algorithm based on multi-evaluation criterion fusion needs to manually set the feature threshold repeatedly to obtain the optimal feature subset, a backward automatic selection feature selection algorithm based on multi-evaluation criteria fusion is proposed in this paper. In this algorithm, the feature evaluation value is calculated using multi-evaluation criteria fusion algorithm, then the discrimination performance of Support Vector Data Description (SVDD) is used as the screening criterion, and finally the optimal feature subset is obtained by backward automatic screening algorithm. The experimental results show that the feature selection algorithm has the advantages of automatic screening, noise robust, clutter robust, low feature dimension, high discrimination rate and low deterioration of discriminative performance for small sample problems.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {347–353},
numpages = {7}
}

@inbook{10.1145/3728725.3728814,
author = {Liu, Shani and Wang, Shan and Zhao, He},
title = {Realization and Experimental Research on Immersive Visual Communication System Based on Real-Time Rendering},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728814},
abstract = {Aiming at the problems of low image processing efficiency and insufficient rendering quality in virtual reality content creation, a real-time rendering system based on deep learning is designed and realized. An improved U-Net network structure is used for image processing, and a rendering speed of 112fps is realized in a standard test environment, with the end-to-end delay controlled within 12ms. The system achieves a task completion rate of 95.8\% and a satisfaction score of 4.6 in a test with 1000 users. The experimental results show that the system significantly improves the rendering efficiency and interactive experience, and provides reliable technical support for virtual reality content creation.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {565–569},
numpages = {5}
}

@inbook{10.1145/3728725.3728799,
author = {Wen, Chengjiang and Yue, Yang and Wang, Zhixiang},
title = {The Application of Membership Inference in Privacy Auditing of Large Language Models Based on Fine-Tuning Method},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728799},
abstract = {Thanks to the large datasets collected from the internet, large language models (LLMs) have made significant progress across various fields, with widespread applications in digital employees, intelligent customer service, and smart decision-making. However, during the training process, models may memorize sensitive private data, posing risks to data security and privacy protection. Member Inference Attacks (MIA) aim to determine whether a specific record was used in the training of the target model. This approach is crucial for detecting privacy risks in models and has seen substantial progress in both traditional and fine-tuned models. However, this remains an unresolved issue for large language models (LLMs). Recent studies indicate that under proper experimental setups, the AUC-ROC of existing methods approaches 0.5 (random guessing). Subsequently, some research has shifted MIA to dataset inference, achieving promising results in this area. Our study shows that when fine-tuning a target model with aggregated samples, it is possible to distinguish whether these samples were used in training the target model by observing the initial loss returned by the model. Based on this observation, we propose a fine-tuning-based member inference method for large language models. To validate the effectiveness of our method, we conducted extensive experiments on three mainstream models and seven datasets. The results indicate that member inference attacks with aggregated samples can achieve acceptable outcomes.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {473–479},
numpages = {7}
}

@inbook{10.1145/3728725.3728815,
author = {Zhu, Yiqi},
title = {Multi-label classification of medical images based on feature fusion},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728815},
abstract = {This article considers the role of multi-label medical image classification and the related revolution in clinical diagnosis. Here, the use of deep learning technologies brings up the problem of single-label classification methods that may not be able to deal with the multi-label one as they do not cover the complex, sometimes coexisting, multiple-pathological features in medical images. To address these challenges, we propose the CTTransNet speculative model, a combination of Convolutional Neural Networks (CNNs) and Vision Transformers (ViTs), which aims to better integrate the local features and the global context. Similarly, a Transformer decoder is used to model inter-label dependencies, and a less-weighted fusing loss mechanism is utilized to deal with the data imbalance issue. The AUC of 81.68\% on the ChestXray14 dataset is achieved by the proposed method, which shows its higher accuracy and superiority to the robustness compared to the traditional methods in multi-label medical image classification. This breakthrough thus brings about the complete information needed for a clinical diagnosis and makes it easier for personalized precision medicine to develop.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {570–576},
numpages = {7}
}

@inbook{10.1145/3728725.3728795,
author = {Zhou, Chenglin and Huang, Cheng and Luo, Xiangyu},
title = {Synthesis of Real-time Linear Dynamic Logic over Finite Traces},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728795},
abstract = {Reactive synthesis, or simply synthesis, is primarily used to automatically generate control-intensive programs. Applications of reactive synthesis span various domains, including robot motion planning, autonomous system control, and communication protocol development. This paper investigates the reactive synthesis for formulas expressed in Real-Time Linear Dynamic Logic over finite traces (RTLDLf). RTLDLf is more expressive than Linear Temporal Logic over finite traces (LTLf). While RTLDLf and Linear Dynamic Logic over finite traces (LDLf) share equivalent expressive power, RTLDLf offers greater simplicity in its logical constructs compared to LDLf. Existing synthesis methods for LDLf predominantly rely on automata-based approaches. In this paper, we propose an RTLDLf synthesis method based on temporal testers. We first translate an RTLDLf specification into a temporal tester, a non-deterministic transducer. The tester is then transformed into a graph game between the system and the environment. We finally propose a symbolic algorithm to solve the game to derive a winning strategy for the system player. The proposed method has two advantages: (1) high composability, enabling convenient and efficient synthesis of complex RTLDLf formulas, and (2) symbolic representation of the tester's transition relations and state space, which facilitates the use of efficient symbolic algorithms based on Ordered Binary Decision Diagrams.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {447–452},
numpages = {6}
}

@inbook{10.1145/3728725.3728811,
author = {Zhu, Minghua},
title = {Solving Class Imbalance in Medical Image Classification Based on Federated Learning},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728811},
abstract = {Federated Learning (FL) has excellent privacy-preserving characteristics and is widely used in the medical field. However, medical data often suffer from class imbalance, which can affect the performance of models. In this paper, we first introduce a novel privacy-preserving federated learning scheme that addresses the class imbalance problem through both feature learning and classifier learning. In the feature learning stage, two feature extraction networks are used in parallel and optimized. Finally, in the classifier learning stage, multiple loss functions are designed to ensure balanced learning across all classes. Experimental results on a publicly available skin dataset with simulated multi-source medical image data demonstrate outstanding performance in handling class imbalance. The model achieves balanced classification accuracy, overall accuracy, and F1 score of 87.02\%, 88.27\%, and 87.33\%, respectively. Compared to other models, the balanced accuracy, overall accuracy, and F1 score are improved by 8.71\%, 3.61\%, and 8.23\%, respectively.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {546–550},
numpages = {5}
}

@inbook{10.1145/3728725.3728736,
author = {Fang, Heng},
title = {ColKGC: Collaborative Enhancement using Large Language Model for Knowledge Graph Completion},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728736},
abstract = {Knowledge Graph Completion (KGC) aims to address incomplete facts in Knowledge Graphs (KGs). As a mainstream approach for KGC, text-based methods are limited in performance due to insufficient knowledge caused by constraints in fine-tuning data. To incorporate broader external knowledge, some researchers explored KGC approaches based on large language models (LLMs), but general-purpose LLMs lack direct awareness of knowledge graph domain knowledge, making it challenging for them to independently perform KGC tasks. To balance the dependence of text-based methods on knowledge graph domain knowledge and LLMs on external knowledge, we propose Collaborative Enhancement using Large Language Model for KGC (ColKGC). We define the prompt template to implement the interaction between the text-based model and the LLMs, and divide the knowledge graph completion process into two stages. In the concept enhancement stage, we supplement the fine-tuning data by interacting with the prompts of the LLMs to enrich the graph domain knowledge learned by the text-based model. In completion inference stage, we adopted the iterative interaction strategy, taking the text-based model output as the prompt and taking the interaction content of the previous stage as the context to enhance the inference, which improved the LLMs' understanding of the KGC task. Our experiments demonstrate that ColKGC achieves superior results across various standard knowledge graph benchmarks. Extensive experiments also bear out ColKGC's efectiveness in generated data and the iterative interaction framework in assisting LLMs reasoning.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {66–71},
numpages = {6}
}

@inproceedings{10.1145/3706598.3713762,
author = {Choi, Youjin and Moon, JaeYoung and Yoo, JinYoung and Hong, Jin-Hyuk},
title = {Understanding the Potentials and Limitations of Prompt-based Music Generative AI},
year = {2025},
isbn = {9798400713941},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3706598.3713762},
doi = {10.1145/3706598.3713762},
abstract = {Prompt-based music generative artificial intelligence (GenAI) offers an efficient way to engage in music creation through language. However, it faces limitations in conveying artistic intent with language alone, highlighting the need for more research on AI-creator interactions. This study evaluates three different interaction modes (prompt-based, preset-based, and motif-based) of commercialized music AI toots with 17 participants of varying musical expertise to examine how prompt-based GenAI can improve creative intention. Our findings revealed that user groups preferred prompt-based music GenAI for distinct purposes: experts used it to validate musical concepts, novices to generate reference samples, and nonprofessionals to transform abstract ideas into musical compositions. We identified its potential for enhancing compositional efficiency and creativity through intuitive interaction, while also noting limitations in handling temporal and musical nuances solely through prompts.  Based on these insights, we present design guidelines to ensure users can effectively engage in the creative process, considering their musical expertise.},
booktitle = {Proceedings of the 2025 CHI Conference on Human Factors in Computing Systems},
articleno = {766},
numpages = {15},
keywords = {Music generative AI, Prompt-based music generative AI, Composition experience, Creative process},
location = {
},
series = {CHI '25}
}

@inbook{10.1145/3728725.3728746,
author = {Yang, Rui and Chen, Hongqian and Gao, Yu and Ge, Yuting},
title = {EAM_DiffusionSeg: Research on Instance Segmentation Methods Based on Enhanced Aggregation Masks and Diffusion Models},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728746},
abstract = {In this paper, we propose an instance segmentation framework EAM_DiffusionSeg based on enhanced aggregation masks, to address the problems of incomplete instance detection and occlusion sensitivity in diffusion models for multi-target segmentation. To overcome the limitations of existing methods that rely on a single mask optimization path, this work innovatively constructs a dual-path mask generation mechanism: a dual-path mask generation mechanism is proposed: (1) a noise filter based on the diffusion model generates the structure-optimized mask M2(M_diffusion); (2) a mask prediction branch integrated into the local decoder outputsM1(M_local), which preserves finer details. and based on the spatial adaptive weighting, the augmented aggregation formula is designed to achieve the feature complementation. Experiments on the COCO dataset demonstrate that the proposed method significantly improves detection capability and boundary segmentation accuracy in occluded scenes, and provide a new solution for instance segmentation of complex scenes.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {132–136},
numpages = {5}
}

@inbook{10.1145/3728725.3728727,
author = {Li, Hao and Zhao, Jiahui and Gu, Daixuan},
title = {HoloDiT: Exploring 3D Diffusion Transformer Models Using 2D Images},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728727},
abstract = {Recently, Transformer-based diffusion models have produced impressive results in 3D shape generation. However, at this stage, they can only be trained using 3D point cloud data to generate simple 3D shapes based on point cloud representations. Building on this, we propose HoloDiT. It uses a set of real-world 2D images and corresponding camera poses as input, generating 3D auxiliary samples that are fed into a Transformer-based 3D diffusion model for training in 3D shape generation. By training the rendering function to generate color and density, we can restore the color details of object surfaces. This enables HoloDiT to generate realistic 3D shapes from 2D images. We trained on the real-world CO3Dv2 dataset, and the generated results demonstrate competitive quality and consistency compared to existing 3D generation models.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {7–11},
numpages = {5}
}

@inbook{10.1145/3728725.3728804,
author = {Wang, Zheng and Jia, Xuezeng and Liu, Liya},
title = {Analysis of occupant escape from indoor building fire based on optimized cellular automaton model},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728804},
abstract = {Indoor decoration materials are increasingly used in buildings. Most of them are combustible or even flammable, which makes them potential fire sources and increases the fire load of buildings. Based on the cellular automaton theory and the multi-lattice theoretical calculation model, this paper simulates the temperature and heat release rate in indoor fires. The results show that the model can better simulate the accuracy of personnel evacuation in indoor fires.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {505–508},
numpages = {4}
}

@proceedings{10.1145/3643690,
title = {IWSiB '24: Proceedings of the 7th ACM/IEEE International Workshop on Software-intensive Business},
year = {2024},
isbn = {9798400705717},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
abstract = {The workshop brings together research communities working on softwareintensive business and software engineering. It aims to bridge the gap between the research in these areas. This year's theme, "Software Business in the Era of Generative Artificial Intelligence," reflects our focus on exploring how generative artificial intelligence (GenAI) and the related large language models (LLMs) impact the established practices of software engineering and software business.},
location = {Lisbon, Portugal}
}

@inbook{10.1145/3728725.3728784,
author = {Li, Wang},
title = {Improvements to the Apriori Algorithm and Its Application in Educational Decision-Making Systems},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728784},
abstract = {The Apriori algorithm is among the traditional algorithms that are mainly employed to mine the educational data. Regrettably, the Apriori algorithm is not only the most classical algorithm, but also has some weaknesses, for instance, it has high computational complexity, generates too many candidate itemsets, which in turn leads it to be mainly used in small-scale educational data scenarios only. In this paper, the authors propose a modified version of the Apriori algorithm which aims to the rule generation process and the computational operation more efficient through data preprocessing phase information improvement and using the dynamic thresholding adjusting component as well as a not-so-well-known intelligent technique. The usefulness of the improved algorithm is tested by an experiment with actual course performance assessments, and the algorithm is also applied in the educational decision support system which comprises the course recommendation, teaching strategy optimization, and academic risk prediction the students. The results of the study suggest that there are no gains in script execution speed, generalization rate, or memory usage while there are with great advantage in the run time of the proposed algorithm. Consequently, the proposed algorithm is superior to the traditional one as it provides higher efficiency and thus, could be the best choice for educational data mining.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {367–373},
numpages = {7}
}

@article{10.1145/3718098,
author = {Shi, Wenda and Wong, Waikeung and Zou, Xingxing},
title = {Generative AI in Fashion: Overview},
year = {2025},
issue_date = {August 2025},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
volume = {16},
number = {4},
issn = {2157-6904},
url = {https://doi.org/10.1145/3718098},
doi = {10.1145/3718098},
abstract = {Generative Artificial Intelligence (GenAI) has recently gained immense popularity by offering various applications for generating high-quality and aesthetically pleasing content of image, 3D, and video data format. The innovative GenAI solutions have shifted paradigms across various design-related industries, particularly fashion. In this article, we explore the incorporation of GenAI into fashion-related tasks and applications. Our examination encompasses a thorough review of more than 470 research papers and an in-depth analysis of over 300 applications, focusing on their contributions to the field. These contributions are identified as 13 tasks within four categories: multi-modal fashion understanding, and fashion synthesis of image, 3D, and dynamic (video and animatable 3D) formats We delve into these methods, recognizing their potential to propel future endeavors toward achieving state-of-the-art performance. Furthermore, we present a comprehensive overview of 53 publicly available datasets suitable for training and benchmarking fashion-centric models, accompanied by the relevant evaluation metrics. Finally, we review real-world applications, unveiling existing challenges and future directions. With comprehensive investigation and in-depth analysis, this article is targeted to serve as a useful resource for understanding the current landscape of GenAI in fashion, paving the way for future innovations in this dynamic field. Papers discussed in this article, along with public code and datasets links are available at: .},
journal = {ACM Trans. Intell. Syst. Technol.},
month = jun,
articleno = {74},
numpages = {73}
}

@inproceedings{10.1145/3641554.3701873,
author = {Thorgeirsson, Sverrir and Ewen, Tracy and Su, Zhendong},
title = {What Can Computer Science Educators Learn From the Failures of Top-Down Pedagogy?},
year = {2025},
isbn = {9798400705311},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3641554.3701873},
doi = {10.1145/3641554.3701873},
abstract = {While educational researchers in various disciplines are grappling with how to develop policies and pedagogical approaches that address the use of generative artificial intelligence, the challenge is particularly complex in computer science education where the new technology is changing the core of the field. In this paper, we take a look at the pedagogy of other subjects with a longer history than computer science and a more extensive body of educational research to collect insights on how this challenge can be met. We begin by drawing from recent neurological research to find domains that share cognitive commonalities with computer programming and then build upon comparisons that others have made to literacy and mathematics education. We then consider how the "reading wars" and "math wars" have shaped these fields, which we see as conflicts between less effective top-down pedagogy and more effective bottom-up pedagogy, and reflect on what would be comparable approaches in teaching computing. We find that approaches that make heavy use of large language models without teaching fundamentals can be compared to the top-down pedagogy of reading and mathematics and are likely to be ineffective on their own. Therefore, we advise against the exclusive use of such approaches with novices. However, we also acknowledge that the social science surrounding computer science education is complex and that effectiveness only tells a part of the story, with other factors such as engagement, motivation and social dynamics also being important.},
booktitle = {Proceedings of the 56th ACM Technical Symposium on Computer Science Education V. 1},
pages = {1127–1133},
numpages = {7},
keywords = {bottom-up pedagogy, computer science education, generative artificial intelligence, large language models, literacy, math wars, phonics, position paper, reading, reading wars, top-down pedagogy, whole language},
location = {Pittsburgh, PA, USA},
series = {SIGCSETS 2025}
}

@inbook{10.1145/3728725.3728747,
author = {Li, Fengdi},
title = {MH-PEFT: Mitigating Hallucinations in Large Vision-Language Models through the PEFT Method},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728747},
abstract = {Large vision-language models (LVLMs) have achieved significant progress and been widely applied across various industries. However, a series of significant challenges have also been posed, such as the hallucination, where the generated attributes of a target are inconsistent with the information in the image. Recent works have primarily focused on improving generation performance by training the multimodal alignment module of the model with additional data. However, this approach incurs higher costs and introduces redundant information due to the reliance on large amounts of additional data. To address these challenges, we propose a novel multimodal mitigation framework, termed MH-PEFT. Specifically, we introduce an effective data augmentation strategy that leverages existing data to generate additional attribute-related information, thereby enhancing the model's generative capabilities. Furthermore, we develop a PEFT-based training approach specifically tailored to mitigate multimodal hallucination in LVLMs. Notably, our framework features a plug-and-play design, which enhances its robustness and facilitates its deployment in real-world scenarios. Extensive experiments demonstrate that our method achieves state-of-the-art performance across multiple benchmark datasets.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {137–142},
numpages = {6}
}

@inbook{10.1145/3728725.3728744,
author = {Huang, Hui-Wen and Chang, Dong-Meau and Sankey, Andrew},
title = {Exploring EEG-based Attention Levels in Human-AI Interactions: A Case Study},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728744},
abstract = {This study used a non-invasive electroencephalogram (EEG) instrument to explore learner attention during interactions with AI chatbots. Compared with traditional self-reported surveys, EEG-based data offer a more direct and objective method for measuring cognitive engagement. To assess differences in attention levels before and after interacting with AI chatbots, the authors invited three Chinese college students to participate in this case study. The results indicated their attention levels increased during chatbot interactions, suggesting that personalized and interactive AI tools can help students stay focused. This case study contributes to the literature on observing cognitive engagement and offers educators a fresh strategy for improving students’ attention and academic performance.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {120–124},
numpages = {5}
}

@inbook{10.1145/3728725.3728760,
author = {Dong, Bingchen and Wu, Gengshen},
title = {A Swin Transformer-Based Cold Diffusion Model for Accelerated MRI Reconstruction},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728760},
abstract = {Clinical diagnosis has become easier because of magnetic resonance imaging (MRI) since detailed images of soft tissues are possible. While having its own advantages MRI has multiple limitations. MRI machines are generally expensive, and extended scan time can (over)burden clinical healthcare systems at the expense of medical work efficiency. The rise of deep learning technologies has spurred the development of methods to accelerate MRI reconstruction. In this regard, we introduce a novel cold-diffusion-based approach to accelerate MRI reconstruction. This method leverages the Swin Transformer architecture for effective feature extraction and incorporates a high-frequency self-attention mechanism to improve the recovery of high-frequency components, thus enhancing image quality. Unlike traditional diffusion models that rely on Gaussian noise, our cold-diffusion model simulates data loss through specific degradation processes like Cartesian undersampling. Experimental results demonstrate that the Swin-CDMR approach outperforms existing techniques in reconstruction quality, achieving superior structural similarity index (SSIM) and peak signal-to-noise ratio (PSNR) scores while maintaining robustness across varying acceleration factors (AF).},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {226–230},
numpages = {5}
}

@inbook{10.1145/3728725.3728832,
author = {Yang, Wangfeng and Li, Zhongyang},
title = {Research on the Development and Application of Digital Twin Technology in the Field of Aircraft Maintenance and Support},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728832},
abstract = {With the development of science and technology, the modernization of equipment is accelerated, the application of digital twin technology in the field of aircraft maintenance and support has been moving from concept to reality and showing its unique and broad prospect. The combination of digital twin technology and intelligent maintenance technology can realize equipment failure prediction, rapid positioning and maintenance in order to increase the production efficiency and reduce the production cost. This paper introduces the research status of digital twin technology and its characteristics, discusses the obvious advantages in the field of aircraft maintenance and support and the possible development trend in the future. For aircraft, the application of digital twin in maintenance support is realized, the digital twin framework is constructed, and the simulation model of aircraft is established. The efficiency and reliability of maintenance support for aircraft are improved. This case has been applied in outfield aircraft support and can be further extended to the other equipment.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {680–684},
numpages = {5}
}

@inproceedings{10.1145/3631700.3664873,
author = {Loepp, Benedikt and Ziegler, J\"{u}rgen},
title = {Exploring the Potential of Generative AI for Augmenting Choice-Based Preference Elicitation in Recommender Systems},
year = {2024},
isbn = {9798400704666},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3631700.3664873},
doi = {10.1145/3631700.3664873},
abstract = {The recent boost in generative artificial intelligence has also reached the field of recommender systems. However, as is often the case, much of the work focuses on the algorithms, overlooking the crucial aspect of improving the systems from a user perspective. In this initial research, we explore the potential of large language models to achieve improvements in preference elicitation. The interactive choice-based method we are augmenting has previously demonstrated significant improvements in a number of aspects related to the user experience. Through an exploratory user study, we show that the item set comparisons presented by this method can be successfully accompanied by independently generated textual summaries, thereby improving the user experience even further.},
booktitle = {Adjunct Proceedings of the 32nd ACM Conference on User Modeling, Adaptation and Personalization},
pages = {114–119},
numpages = {6},
keywords = {Large language models, Preference elicitation, Recommender systems, User experience.},
location = {Cagliari, Italy},
series = {UMAP Adjunct '24}
}

@inbook{10.1145/3728725.3728820,
author = {Chen, Xunlin and Li, Xiaoju and Chen, Ziyong},
title = {Research on Weed Identification Based on Deep Learning},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728820},
abstract = {In the intricate environment where crops and weeds grow intermingled, an effective solution to this problem is to precisely spray herbicides on weeds while accurately avoiding crops. The technical reliance in this process is the focus of this paper, which involves classifying, identifying, and locating weeds and crops. Currently, deep learning is widely used for image recognition and classification. However, the complex characteristics of the intermingled growth of weeds and crops are the main factors affecting the extraction of weed features. Based on an extensive review of domestic and foreign literature, and combining the research progress and methods of deep learning in the field of image recognition, this paper designs a method that can identify weeds with complex characteristics, effectively improving the accuracy of weed image recognition. This provides a favorable reference for the rapid and precise targeted spraying of herbicides in agricultural intelligent equipment or other devices beneficial to human life.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {605–609},
numpages = {5}
}

@inbook{10.1145/3728725.3728797,
author = {Zuo, Mengxue},
title = {Infringement and Protection in Personal Information on Data Mining},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728797},
abstract = {Different from the personal information processing stage such as collection and analysis, the infringement of personal information by Data Mining is based on the mining process and mining results. On the basis of analyzing the operation mechanism and technical characteristics of Data Mining, there are four forms of infringement of personal information Data Mining. Specific manifestations are as follows: manifested as data labeling and unfavorable evaluation, information cocoons and passive autonomy, social sorting and data discrimination, information association and personal privacy infringement. The automatic decision-making and black box of Data Mining, the constructiveness and hiding of mining infringement behaviors, and the difficulty and challenge of infringement results pose the challenges and shortcomings of the current infringement law of personal information Data Mining in China. Finally puts forward the path to improve the legal protection of personal information, that is introducing the rules of due process, restricting the data power of Data Mining organizations, and establishing channels and procedures for effective interaction between Data Mining organizations and subjects of personal information.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {459–465},
numpages = {7}
}

@inproceedings{10.1145/3728725.3728775,
author = {Zheng, Jiamin and Peng, Shijia and Zhou, Yipeng and Song, Shaoyi and Zhao, Xuan},
title = {Secure Sharing and Protection of Agricultural Data Based on Federated Learning},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728775},
doi = {10.1145/3728725.3728775},
abstract = {With the acceleration of agricultural digitization process, the security sharing of agricultural data has become a key issue to be solved urgently. This paper focuses on this, introduces the related technologies such as blockchain, federated learning and interplanetary file system, and proposes the design of agricultural data sharing model based on federated learning. The work flow of this scheme is described in detail and evaluated by experiment. The security analysis shows that the scheme can effectively resist various security threats and protect the privacy of agricultural data. The accuracy analysis shows that the training accuracy of the model is almost unaffected while the safety is improved. Homomorphic encryption analysis verifies the effectiveness of the encryption technology in the scheme. This scheme provides a feasible and efficient solution for data security sharing of agricultural intelligent knowledge service platform, and is expected to play an important role in related fields.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {312–319},
numpages = {8},
location = {
},
series = {GAIIS '25}
}

@inbook{10.1145/3728725.3728737,
author = {Lei, Zhongxia and Zhang, Caixia and Gui, Junzhen},
title = {Applications and Comprehensive Evaluation of Commodity Reconstruction based on NeRF},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728737},
abstract = {NeRF, as an implicit 3D scene generation algorithm, has attracted widespread attention in the field of 3D reconstruction. However, its application in the field of small commodity 3D modeling has not been fully evaluated. To offer specific technical guidance for the application of Nerf in the e - commerce field, we perform detailed experiments and comparisons from multiple aspects such as the complexity of the background, lighting conditions, the number of images, and the surface texture of the experiment.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {72–79},
numpages = {8}
}

@inbook{10.1145/3728725.3728790,
author = {Zhao, Jiahui and Li, Hao and Tang, Suqin},
title = {A Hybrid Framework for Predicting Citation Harm from Retracted Papers},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728790},
abstract = {The increasing prevalence of retractions poses significant challenges to academic integrity and scientific progress. While various tools track retracted papers, the academic community lacks proactive mechanisms to predict the potential harm caused by citing retracted research. Building upon recent work quantifying the harm, we construct a dataset to forecast the harm papers will suffer from citing retracted research. We further propose a hybrid framework that combines language model with gradient boosted trees. Our framework achieves the lowest mean absolute error (MAE) across all prediction time horizons, achieving a 2.4\% reduction in MAE compared to baseline methods. This work provides foundational resources and methodological insights for future studies on retraction impacts, aiming to help researchers make more informed citation decisions.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {411–416},
numpages = {6}
}

@inbook{10.1145/3728725.3728835,
author = {Lv, Hao},
title = {Aircraft Detection Based on Improved DETR},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728835},
abstract = {With the wide application of deep learning in computer vision, Transformer-based target detection methods, especially DETR, have become a research hotspot. However, DETR still faces low detection accuracy and efficiency problems when dealing with small objects and complex backgrounds. In order to enhance the performance of DETR in aircraft detection, this paper proposes an aircraft detection method based on an improved DETR architecture, which mainly consists of two innovative improvements: first, an ECA attention mechanism is introduced after CNN feature extraction to enhance the model's adaptive learning ability for important features; second, Swin Transformer is used to replace the encoder in the original DETR -decoder structure in the original DETR, using its local windowing self-attention mechanism to improve the computational efficiency and accuracy of the model. The experimental results show that the method proposed in this paper improves the detection accuracy and speed compared with the traditional DETR in the aircraft detection task, especially in the complex background with better robustness and generalization ability. The research in this paper provides an effective improvement idea for the Transformer-based target detection method and provides new technical support for high-precision target detection tasks in practical applications.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {691–697},
numpages = {7}
}

@inbook{10.1145/3728725.3728759,
author = {Xiong, Yuzhu and Du, Hongwei and Zhang, Daikun},
title = {A Guided-learning Network for Brain Tumor Segmentation with Missing Modalities},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728759},
abstract = {Accurate segmentation of brain tumors is essential for clinical assessment and treatment planning, as it leverages multiple MRI modalities to provide complementary diagnostic information. However, missing modalities are frequently en- countered in clinical practice due to factors such as image degradation, artifacts, protocol inconsistencies, or patient con- traindications to contrast agents. While certain unified models are designed to accommodate various missing modality scenarios, their performance declines markedly when multiple modalities are absent, particularly in cases where only a single modality is available. To tackle this challenge, we introduce a Guided- learning Network that leverages dynamic FFT filters. This network trains multiple specialized models designed for different missing modality scenarios, achieving outstanding performance. The architecture comprises two distinct learning paths: one dedicated to capturing multimodal information and another focused on generating modality-specific representations for the missing modality. A guided-learning strategy connects these two paths. Initially, the multimodal path is trained and its parameters are locked, enabling the missing modality path to learn under its guidance. The training process incorporates several key com- ponents: adversarial learning to align high-level features across paths, frequency-domain adaptive filtering using dynamic FFT filters to extract global features, and mutual information transfer to ensure critical information is retained even when modality data is missing. Experiments conducted on the BraTS2018 dataset demonstrate that our model outperforms comparable state-of- the-art methods in all missing modality cases.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {219–225},
numpages = {7}
}

@inbook{10.1145/3728725.3728754,
author = {Sun, Qinhui and Gao, Bin},
title = {Emotion-enhanced Cross-modal Contrastive Learning for Fake News Detection},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728754},
abstract = {This paper proposes a fake news detection model based on emotion-enhanced cross-modal contrastive learning (ECC-FND), which aims to fully utilize the information from images, text, and user comments in social media news to improve the accuracy and efficiency of fake news detection. ECC-FND combines cross-modal contrastive learning and sentiment analysis methods to effectively explore the semantic associations between multimodal information and dynamically weights features from different modalities using an attention mechanism. Experimental results demonstrate that ECC-FND significantly outperforms existing state-of-the-art methods on the Weibo dataset, which validates its effectiveness and generalization ability in fake news detection tasks. This study provides new insights for multimodal fake news detection and offers valuable references for future research.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {184–189},
numpages = {6}
}

@inproceedings{10.1145/3745900.3746116,
author = {Suzawa, Haruki and Watanabe, Ko and Dengel, Andreas and Ishimaru, Shoya},
title = {Augmenting Online Meetings with Context-Aware Real-time Music Generation},
year = {2025},
isbn = {9798400715662},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3745900.3746116},
doi = {10.1145/3745900.3746116},
abstract = {As online communication continues to expand, participants often face cognitive fatigue and reduced engagement. Cognitive augmentation, which leverages technology to enhance human abilities, offers promising solutions to these challenges. In this study, we investigate the potential of generative artificial intelligence (GenAI) for real-time music generation to enrich online meetings. We introduce Discussion Jockey 2, a system that dynamically produces background music in response to live conversation transcripts. Through a user study involving 14 participants in an online interview setting, we examine the system’s impact on relaxation, concentration, and overall user experience. The findings reveal that AI-generated background music significantly enhances user relaxation (average score: 5.75/9) and concentration (average score: 5.86/9). This research underscores the promise of context-aware music generation in improving the quality of online communication and points to future directions for optimizing its implementation across various virtual environments.},
booktitle = {Proceedings of the Augmented Humans International Conference 2025},
pages = {487–490},
numpages = {4},
keywords = {Generative AI, Music Generation, Online Meeting, Communication},
location = {
},
series = {AHs '25}
}

@inbook{10.1145/3728725.3728794,
author = {Chen, Jian and Li, Guangao and Wang, Jun and Sun, Siluo and Sun, Shoudao and Zhao, Yuntao},
title = {CBAM-EANet: Convolutional Block Attention Module Enhanced Efficient Network for Detection on Distributed Denial of Service Attacks},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728794},
abstract = {With the emergence of numerous DDoS attack methods, traditional detection techniques can no longer accurately and swiftly detect DDoS attacks. Furthermore, deep learning-based traffic image classification models often become overly complex in their pursuit of higher accuracy. In the paper, we propose a deep learning model-based method for the visual detection of DDoS malicious traffic. In response to the issue of model complexity, this article has made improvements based on the EfficientNet model, especially in resource constrained environments. This article introduces CBAM into the original model to improve its ability to process global information. By replacing the loss function in the final Softmax layer of the EfficientNet model, the ability to extract image features for binary classification problems has been enhanced, resulting in improved model performance. The experimental results show that this method can effectively identify various DDoS attacks. Compared with the EfficientNet model, this method has improved accuracy, training time, and single image prediction time.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {440–446},
numpages = {7}
}

@inbook{10.1145/3728725.3728805,
author = {Chen, Hongfu and Jin, Minghui and Gao, Feng and Zhai, Yujia},
title = {Research and Implementation of Access Control Strategy Based on Apriori and Collaborative Filtering Techniques},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728805},
abstract = {This paper presents an automated method for generating access control policies by combining the Apriori algorithm and collaborative filtering technology. This approach solves the complex access control challenges arising from data sharing and large number of users in power grid regulation systems. First, the Aprior algorithm is used to mine strong association rules from the user authority relationship to reveal the potential relationship between resources. Then, the user rights relationship is enhanced, resource similarity is calculated, and collaborative filtering is used to generate recommended values for unauthorized resources. Finally, the access control policy is classified and authorized according to the recommended value, and the automatic generation of the policy is realized. The experimental results show that this method can not only generate access control policies automatically, but also assign new permissions effectively and ensure the privacy of resource data.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {509–514},
numpages = {6}
}

@inbook{10.1145/3728725.3728752,
author = {Xiong, Botao},
title = {Audio-driven Talking-face Synthesis based on 3D Gaussian},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728752},
abstract = {Audio-driven talking-face synthesis is of significant importance in various application scenarios, such as remote meetings, AR/VR, and digital humans. Currently, the work in this field can be broadly divided into two categories: implicit representation-based methods and explicit representation-based methods. Implicit representation-based methods often use neural network models to represent human face. For example, NeRF (Neural Radiance Field) uses MLP (Multi-Layer Perceptron) to represent human face, and by inputting the camera pose, NeRF renders a face image from that viewpoint. When training with audio features, it can render face images with different facial movements. However, due to the lack of editability and slow rendering speed of implicit representations, the industry has been exploring new representation methods. Explicit representation-based methods commonly use 3D Gaussian representations of the face, which offers high editability and nearly real-time rendering speed, making these methods surpass implicit-based approaches in many metrics. However, it is difficult to incorporate other signals during the rendering optimization process. To address this issue, this paper combines 3D Gaussian representations with the 3D head model FLAME (Faces Learned with an Articulated Model and Expressions). The FLAME model represents the face's shape and facial movements with a set of parameters. In this paper, audio signals are first transformed into parameters representing facial movements, which are used to initialize the 3D model generated by the parameters, followed by rendering optimization. This paper proposes a 3D Gaussian-based audio-driven lip-sync video generation system. It consists of two main modules: the audio-to-facial movement module and the 3D Gaussian rendering module. The audio-to-facial movement module uses the extracted audio features to generate parameters that control the FLAME model, converting the audio input into natural and reasonable lip shapes and facial movements. The 3D Gaussian rendering module initializes the 3D Gaussian with the FLAME model, optimizes the rendered image from coarse to fine, and finally stitches the generated images together to create a continuous video. Experiments show that this system can generate lip-synchronized, accurate, high-fidelity videos.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {171–177},
numpages = {7}
}

@inbook{10.1145/3728725.3728783,
author = {Wang, Qiaoxin and Zheng, Shouguo and Qiu, Mengqing and Hu, Dan},
title = {Detection of pesticide residues by sensor arrays fused from SERS spectra of various substrates combined with deep learning},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728783},
abstract = {In this study, a rapid, accurate and intelligent method for detecting pesticide residues was developed by sensor arrays fused from surface-enhanced Raman spectroscopy (SERS) spectra of various substrates combined with deep learning. First, spectral data of pesticide residues were collected using conventional substrates with various morphologies. Then, the sensor array is acquired using different fusion methods on the spectral data, specifically concatenation and parallel methods. Finally, the intelligent and precise detection of chlormequat chloride and paraquat is implemented in a regression model constructed through the sensor array combined with machine learning and deep learning. In this experiment, the lowest detection limits for chlormequat chloride and paraquat detected are 0.03 mg/L and 0.003 mg/L, respectively. The regression model which was constructed using the sensor arrays obtained from the concatenation fusion method combined with the MobileNet obtained the best prediction results, with the coefficients of determination of prediction of 0.9865 and 0.9846 for chlormequat chloride and paraquat, and root-mean-square errors of prediction of 0.2254 and 0.0174, respectively. Overall, the method we proposed achieves rapid, intelligent and accurate detection of chlormequat chloride and paraquat, with the advantages of low cost and simple operation, which has great application potential in trace detection of pesticide residues.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {359–366},
numpages = {8}
}

@inbook{10.1145/3728725.3728768,
author = {Yu, Ze and Miao, Quanqiang},
title = {Cybersecurity Survivability Testing Technology Based on ATT&amp;CK and D3FEND},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728768},
abstract = {In the context of the continuous evolution of current network threats, organizations urgently need advanced cybersecurity defense strategies to deal with complex and changeable network attacks. Survivability testing is usually implemented by simulating real hacker attacks, but this kind of testing and evaluation method depends on the professional level of testers to a large extent. There are problems such as incomplete testing and low testing efficiency. This paper comprehensively utilizes the ATT&amp;CK (Adversarial Tactics, Technologies, and Common Knowledge) and D3FEND (Defensive Tactics, Technologies, and Common Knowledge) frameworks of MITRE, aiming to propose a new cybersecurity survivability testing technology. Through in-depth analysis of the attack tactics, techniques and procedures (TTPs) in the ATT&amp;CK framework, combined with the defense strategies and measures in the D3FEND framework, this paper designs a cybersecurity survivability evaluation program. This scheme not only evaluates the protection, detection, response and recovery capabilities of the system against network attacks, but also provides a scientific and perfect defense measures for the system. The case study further verifies the effectiveness of the proposed scheme, and shows how to conduct survivability testing against attacks on the Linux operating system. This study provides a new perspective and practical guidance for the development of cybersecurity defense technology.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {272–277},
numpages = {6}
}

@inbook{10.1145/3728725.3728807,
author = {Wan, Guofeng},
title = {Research and Application of Generalized Predictive Control Algorithm in ATO System},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728807},
abstract = {Aiming at the safety guarantee of the train in the process of operation, whether it can arrive on time and comfort in the process of riding. In this paper, generalized predictive control algorithm is applied to automatic train operation system. First of all, the structure and function of the ATO system of the train are studied and analyzed, and then the force analysis of the train operation is carried out and the transformation of each working condition is understood and the situation is applicable. Secondly, the GPC algorithm is studied in detail, and its prediction model, rolling optimization and online identification are recognized respectively, and the train speed controller is designed. Finally, the ATO system is simulated and designed based on GPC algorithm, and the train model is analyzed. Moreover, it can be verified by simulation that under the GPC algorithm, the train can track the set target curve well during operation, ensuring the safety, comfort, energy saving and accuracy of the train operation.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {522–525},
numpages = {4}
}

@inbook{10.1145/3728725.3728793,
author = {Chen, Jian and Wang, Lei and Jiang, Jun and Liu, Ruitong and Ji, Heng and Zhao, Yuntao},
title = {A Knowledge Reasoning Modeling for security vulnerability databases based on Gaussian-SVM hybrid clustering},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728793},
abstract = {At present, network security becomes more and more important, but the security vulnerability data in various databases are relatively dispersed and it is difficult to establish the connection between them, network security knowledge graph can associate the security vulnerability data of different databases together, and it can be updated in real time with the latest security vulnerabilities. This paper establishes network security knowledge graph based on CAPEC, CWE and other security libraries. Aiming at the problem that TransR model cannot utilize entity semantic information, TransR-Roberta model is proposed, and splicing layer, attention layer and convolutional layer are added on the basis of TransR model. And for the problem that the quality of negative samples generated by Bernoulli negative sampling is too low, a clustering-based negative sampling optimization method is proposed. Firstly, the clustering task is accomplished with the help of Gaussian hybrid clustering algorithm on the entities in the security vulnerability knowledge graph to label the entities with clusters. In the next step, the clustered vulnerability entities are reclassified using support vector machine. Finally, a classifier combined with Facolloss function is added to the loss function of the TransR model, which is used to improve the ability of the knowledge inference model to distinguish between positive and negative samples. The experimental results show that the improved model improves the hit@N, MR and MMR metrics for the knowledge reasoning task.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {435–439},
numpages = {5}
}

@inproceedings{10.1145/3728725.3728819,
author = {Li, Mingyang and Li, Pengpeng and Wang, Rui and Mou, Weihua},
title = {Residual Neural Network with Attention Mechanism for Direction-of-Arrival Estimation in Low SNR},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728819},
doi = {10.1145/3728725.3728819},
abstract = {In this paper, we propose an improved residual neural network (ResNet) based on an attention mechanism for estimating the direction of arrival (DOA) of linear arrays. The real, imaginary, and phase components of the covariance matrix are used as inputs to an auto-encoder (AE), whose output is then introduced into a cascaded ResNet for DOA estimation. To enhance estimation accuracy, an attention mechanism is integrated into the ResNet, effectively assigning greater weight to the most relevant features. Experimental results demonstrate the proposed method's superior DOA estimation performance compared to existing methods, especially in low SNR.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {598–604},
numpages = {7},
