 pages = {297–302},
 publisher = {Association for Computing Machinery},
 series = {GAIIS '24},
 title = {95598 Customer Service Complaint Spillover Risk Prediction Method Based on Data Association Clustering},
 url = {https://doi.org/10.1145/3665348.3665399},
 year = {2024}
}

@inproceedings{10.1145/3665348.3665400,
 abstract = {Unmanned aerial vehicles (UAV) are susceptible to unsecure communication problems in denied environments because of strong interferences from physical fields and electromagnetic fields. A dynamic key management scheme based on Chinese remainder theorem (CRT) and the Hash function is proposed here to solve this problem. Specifically, as changes in the topological structure and unstable communication are likely to cause security problems in communication of drones in the battlefield, two dynamic key update protocols, i.e., topology-aware key update and mutual-healing key update, are put forth here to ensure secure communication through dynamically-updated keys. Analyses reveal that the proposed scheme has forward and backward security and resistance to collusion attacks and replay attacks. Meanwhile, the proposed method has been compared with existing methods through tests on the popular MIRACL dataset by Visual Studio 2019, and the test results show that our method optimizes the computing performance and the communication overhead, and achieves higher updating efficiency.},
 address = {New York, NY, USA},
 author = {Yuan, Lin and Feng, Zhishang},
 booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
 doi = {10.1145/3665348.3665400},
