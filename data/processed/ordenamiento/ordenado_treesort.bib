@inproceedings{10.1145/3729706.3729781,
  author = {Yang, Hsi-Hsun and Chou, Wei-Ting},
  title = {AI-Driven Learning Approaches in the Era of Artificial Intelligence: Innovations in Design Education},
  year = {2025},
  isbn = {9798400712715},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3729706.3729781},
  doi = {10.1145/3729706.3729781},
  abstract = {Artificial Intelligence (AI) is integrated with various domains for design education — expanding creativity, problem-solving, design methodologies, research, etc. due to its ability to simulate human cognition and behavior. Using systematic review and content analysis, this study conducts a systematic review to understand the AI-powered curriculum innovation trends in design education. From the analyzed papers, five emerging themes were identified: (1) Art and Design Education, (2) Visual Communication and Graphic Design, (3) Integration and Innovation of AI in Design Education, (4) Design Thinking and AI for Education, and (5) Generative AI for Design and Fashion. The results suggest that AI has evolved from a foundational technical tool to a transformative force changing how we learn, update curricula, and assess knowledge. Although AI is a game changer in terms of efficiency and finding different creative paths, the ethics and influence on the design process will have to be examined thoroughly. Future research directions may include human-AI collaboration in pedagogy, the ethics of AI-generated design content, and learner adaptation to AI-enabled design education. This research offers the most recent review of AI in practice in design schools, helping educators and policymakers alike shape the role AI plays within the design education context.},
  booktitle = {Proceedings of the 2025 4th International Conference on Cyber Security, Artificial Intelligence and the Digital Economy},
  pages = {471–477},
  numpages = {7},
  keywords = {Artificial intelligence, Content analysis, Curriculum innovation, Design education, Systematic review},
  location = {},
}

@inproceedings{10.1145/3734921.3734941,
