 location = {},
 numpages = {12},
 pages = {563–574},
 publisher = {Association for Computing Machinery},
 series = {SBQS '24},
 title = {Teaching Refactoring to Improve Code Quality with ChatGPT: An Experience Report in Undergraduate Lessons},
 url = {https://doi.org/10.1145/3701625.3701681},
 year = {2024}
}

