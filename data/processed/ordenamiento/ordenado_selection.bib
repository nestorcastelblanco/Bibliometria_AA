@inbook{10.1145/3760023.3760133,
 abstract = {With the rapid development of digital and intelligent technology, enterprises have encountered many difficulties in managing human resources. The problems of scattered data, untimely manual decision-making, and mismatch between employees and positions are becoming more and more common. Management methods that used to rely on personal experience or fixed rules can't keep up with the pace of business changes now. On the other hand, artificial intelligence, especially deep learning algorithm, brings new and promising solutions, which can handle human resources-related work more efficiently and accurately. This paper discusses the main challenges faced by human resource allocation in artificial intelligence environment, introduces how AI technology can help, and puts forward some improvement strategies using XGBoost algorithm. This model is not only supported by survey data, but also tested in actual scenes. The results show that this method works well and can really optimize the allocation of human resources. We hope that through this research, we can provide some practical suggestions for enterprises to adjust their human resource management in the AI era, and help them maintain their competitive advantage and operate more smoothly.},
 address = {New York, NY, USA},
