@inproceedings{10.1145/3759275.3759292,
  author = {Hao, Xiaoyan and Cui, Yingjie},
  title = {Smart Museums, Smarter Experiences: Exploring the Impact of GenAI Usability on Digital Heritage Enjoyment},
  year = {2025},
  isbn = {9798400719554},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3759275.3759292},
  doi = {10.1145/3759275.3759292},
  abstract = {With the increasing integration of generative artificial intelligence (GenAI) into cultural heritage dissemination, users' digital interactions in museums have evolved from passive information reception to active co-creation. To investigate the mechanisms through which GenAI usability influences user enjoyment, this study applies the Hedonic Motivation System Adoption Model (HMSAM) and develops a theoretical framework incorporating six mediating constructs: perceived ease of use, perceived usefulness, curiosity, joy, sense of control, and immersion. Based on 582 valid responses collected from the “Cloud Tour of Dunhuang” digital heritage initiative, structural equation modeling was conducted using SmartPLS 4.0. The findings reveal that GenAI usability does not directly affect users' enjoyment; instead, its influence is transmitted indirectly through several psychological mediators. Specifically, GenAI usability significantly enhances perceived ease of use, usefulness, curiosity, joy, and control, each of which contributes to users’ enjoyment, with joy demonstrating the strongest mediating effect. Although GenAI usability also increases users’ sense of immersion, immersion itself does not significantly predict enjoyment. These results extend the applicability of HMSAM in the cultural technology domain, deepen the theoretical understanding of user behavior in smart museums, and offer practical guidance for optimizing GenAI-based interactive system design in cultural heritage contexts.},
  booktitle = {Proceedings of the 2025 2nd International Conference on Digital Systems and Design Innovation},
  pages = {107–117},
  numpages = {11},
  keywords = {Cultural Heritage Technology, Generative Artificial Intelligence, Hedonic Motivation, Smart Museums, Usability, User Enjoyment},
  location = {},
}

@inproceedings{10.1145/3722237.3722344,
  author = {Fang, Ling and Dai, Wenji and Xu, Xiaoyuan},
  title = {Social media literacy in the age of Artificial Intelligence},
  year = {2025},
  isbn = {9798400712692},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3722237.3722344},
  doi = {10.1145/3722237.3722344},
  abstract = {In the age of Artificial Intelligence, college students have adapted to interact with the world in a new media ecosystem. Since the era of web 2.0, social networking site (SNS) combined attributes of interpersonal communication technology and mass media, and its arrival made “many to many” interaction mode easily available. Young people especially rely on social media to socialize, entertain, seek information, and present themselves. Scholars and educators have paid more and more attention to the youth digital literacy as well as their new media literacy. How college students’ social media involvement influences their psychological well-being has been researchers’ concerns. To better understand current situation of college students’ social media literacy, this empirical research conducted a survey on college students’ social media use behavior and stress coping preferences. A total of 431 undergraduate students participated in an online survey in 2023. Three mediators between student participants’ individualized social media involvement and their adaptive stress coping are tested by AMOS. A few social network attributes are tested as moderators through multiple linear regression by SPSS.  Findings suggest that college students’ social media involvement assist their psychological well-being in some way. Sample students’ frequency of participating in social media groups is positively related of adaptive stress coping. Their perceived social support from SNS is a key mediator between individualized SNS involvement and adaptive stress coping. However, sample students’ online role modeling behavior is negatively associated with their adaptive stress coping. Implications of these findings are further discussed.},
  booktitle = {Proceedings of the 2024 3rd International Conference on Artificial Intelligence and Education},
  pages = {614–618},
  numpages = {5},
  keywords = {College students, Social media involvement, Social media literacy},
  location = {},
}

