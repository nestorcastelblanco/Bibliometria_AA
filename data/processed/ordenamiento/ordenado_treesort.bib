@inproceedings{10.1145/3643834.3660703,
  author = {Wang, Ziyan and Shen, Luyao and Kuang, Emily and Zhang, Shumeng and Fan, Mingming},
  title = {Exploring the Impact of Artificial Intelligence-Generated Content (AIGC) Tools on Social Dynamics in UX Collaboration},
  year = {2024},
  isbn = {9798400705830},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3643834.3660703},
  doi = {10.1145/3643834.3660703},
  abstract = {Artificial Intelligence-Generated Content (AIGC) tools have gradually been integrated into the daily workflow of UX practitioners. While existing research has explored the integration of AIGC tools in daily workflow, little is known about their impact on social dynamics within UX collaboration. We conducted four focus groups and eight semi-structured interviews with 26 UX practitioners to investigate how AIGC tools influence social dynamics in UX collaboration. Our findings indicated that AIGC tools not only mitigated conflicts but also introduced potential new conflicts. AIGC tools expanded the roles of UX practitioners and fostered a team culture characterized by exploring and discussing. Participants have higher expectations for AI-assisted design in user understanding and prototype evaluation, and team-motivated AI tools learning. Based on these findings, we discussed the benefits and concerns of conflict resolution through AIGC and the importance of teams in AI learning. Finally, we proposed several suggestions for future AI design research.},
  booktitle = {Proceedings of the 2024 ACM Designing Interactive Systems Conference},
  pages = {1594–1606},
  numpages = {13},
  keywords = {AI-generated content, collaboration, social dynamics, user experience design, user interface design},
  location = {Copenhagen, Denmark},
}

@inproceedings{10.1145/3695080.3695117,
  author = {Chen, Xianyi},
  title = {Exploring the Intersection of Artificial Intelligence and Fine Arts: A Data-Driven Approach},
  year = {2024},
  isbn = {9798400710223},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3695080.3695117},
  doi = {10.1145/3695080.3695117},
  abstract = {This paper delves into the fascinating intersection of artificial intelligence (AI) and fine arts, a dynamic synergy that is reshaping both fields. With the evolution of fine arts from ancient cave paintings to modern abstract expressions and the emergence of AI as a pivotal technology, this study explores their intertwined development and impact on each other. The focus is on how AI has revolutionized artistic creation, art preservation and restoration, and art style and pattern recognition. It examines the role of AI in extracting art design features, including texture and color analysis, using deep learning models like VGG. The paper highlights the historical progression of fine arts and AI, their convergence, and AI's application in art, providing new insights into understanding and classifying art. It also probes the ethical, philosophical, and practical aspects of AI-generated art, contributing significantly to the discourse on the nature of art, creativity, and technology-driven artistic expression. This exploration not only provides an academic perspective but also holds cultural significance, marking a new era in the fusion of human creativity and technological innovation.},
  booktitle = {Proceedings of the 2024 International Conference on Cloud Computing and Big Data},
  pages = {212–216},
  numpages = {5},
  location = {Dali, China},
}

