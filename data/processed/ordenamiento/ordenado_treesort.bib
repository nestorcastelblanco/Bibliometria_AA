@inproceedings{10.1145/3727648.3727787,
  author = {Song, Jinfu},
  title = {A Chinese Cyber Threat Intelligence Named Entity Recognition Method},
  year = {2025},
  isbn = {9798400712647},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3727648.3727787},
  doi = {10.1145/3727648.3727787},
  abstract = {Chinese cybersecurity named entity recognition often suffers from model performance problems due to data noise and insufficient feature extraction. To address this problem, this paper proposes the BERT-ABIM-CRF model, which uses BERT to extract dynamic word embeddings optimized by adversarial training, combines BiLSTM and IDCNN for joint feature extraction, and fuses global and local features using a multi-head self-attention mechanism. Finally, CRF is used for decoding to ensure the consistency of the results. Experiments show that the model has an F1 score of 0.8305 on the CDTier dataset and 0.9122 on the cybersecurity dataset, and exhibits strong robustness in dealing with noise and long-range dependencies. This greatly improves the recognition accuracy and performance, validating its potential in the field of cyber threat intelligence.},
  booktitle = {Proceedings of the 4th International Conference on Computer, Artificial Intelligence and Control Engineering},
  pages = {848–853},
  numpages = {6},
  keywords = {Cyber Threat Intelligence, Feature Fusion, Multi-Feature Fusion, Named Entity Recognition, Natural Language Processing},
  location = {},
}

@inproceedings{10.1145/3747012.3747058,
  author = {Su, Jiayi},
  title = {A CiteSpace-Based Bibliometric Analysis of Artificial Intelligence in Urban Sustainable Design Research},
  year = {2025},
  isbn = {9798400715167},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3747012.3747058},
  doi = {10.1145/3747012.3747058},
  abstract = {As the process of sustainable urban development advances globally, green development has become a priority in urban renewal and development. The scientific analysis and management enabled by artificial intelligence can facilitate the rapid achievement of urban green development goals. As a result, a substantial amount of research is currently underway in this area. This paper provides a summary and analysis of the literature on urban green renewal driven by artificial intelligence from 2006 to 2024, aiming to identify current hot topics and future development trends. Using bibliometric methods and the CiteSpace software, a visual analysis was conducted on data from 5,421 selected articles, including information on countries, institutions, authors, keywords, and references. The results indicate that current research in this field primarily focuses on areas such as "machine learning," "green space," "urbanization," "dynamics," "air pollution," and "cloud computing." Additionally, this study presents potential future development trends in the field. The findings of this research will provide comprehensive insights into the current state of research and future directions for scholars in the field. Furthermore, the study can guide practitioners in exploring business opportunities and implementing best practices in the integration of artificial intelligence and urban green design innovation and technology applications. The research results can also offer valuable references for governments in formulating effective policies to guide the development of urban green renewal.},
  booktitle = {Proceedings of the 2025 International Conference on Smart City and Sustainable Development},
  pages = {244–254},
  numpages = {11},
  keywords = {Citespace, Urban design, deep learning, green space, machine learning},
  location = {},
}

