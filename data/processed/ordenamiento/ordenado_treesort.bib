@inproceedings{10.1145/3625007.3627484,
  author = {Duncan, Clay and Mcculloh, Ian},
  title = {Unmasking Bias in Chat GPT Responses},
  year = {2024},
  isbn = {9798400704093},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3625007.3627484},
  doi = {10.1145/3625007.3627484},
  abstract = {Generative artificial intelligence (AI) has gained a great deal of recent attention with the release of Chat GPT 4. It has been praised for its ability to generate human-like responses but has perhaps faced even more criticism over potential concerns for biased responses, misinformation, and generation of harmful or inappropriate content. Chat GPT utilizes large sources of data to curate responses to all kinds of questions. The generative AI models are designed to be objective and avoid any sort of bias in their output. However, in the age of misinformation, social media, user-generated content and the 24-hour news cycle, biased information has never been more plentiful. This paper investigates the possibility of biased responses produced by Chat GPT 4 utilizing public data from biased media sources through Support Vector Machines. We find Chat GPT tends to have bias in its responses.},
  booktitle = {Proceedings of the 2023 IEEE/ACM International Conference on Advances in Social Networks Analysis and Mining},
  pages = {687–691},
  numpages = {5},
  keywords = {chat GPT, bias, artificial intelligence, generative AI, AI ethics},
  location = {Kusadasi, Turkiye},
}

@inproceedings{10.1145/3638884.3638910,
  author = {Ren, Zhilin and Qin, Xiangang and Wang, Bixuan},
  title = {Unraveling the Impact of Explainability of Artificial Intelligence-Generated Content(AIGC) on Design Style Transfer Effects},
  year = {2024},
  isbn = {9798400708909},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3638884.3638910},
  doi = {10.1145/3638884.3638910},
  abstract = {The advent of Artificial Intelligence-generated Content(AIGC) has revolutionized the traditional creative activities in the design field, the workflow of which includes idea generation, sketching, solution determination, low-fidelity prototype interaction, detail tuning, high-fidelity prototyping, front-end slicing, and deployment. By contrast, AIGC streamlines the process of design into idea generation, design model prompting, image dataset collection, AIGC tool use, model fine-tuning, and tool optimization. The designer's focus, the subject, and the object of attention have been changed accordingly. Traditional design activities without AIGC focus on realizing individual and team creativity; the ones with AIGC introduce AI models, algorithms, and AI-HMIs as collaborators in the creative process. The quality of the created content is no longer determined by the designer alone due to the involvement of Artificial Intelligence(AI) of in the prediction and decision-making of the created content. There is also a growing need for designers to have a self-explanatory and responsible intelligent system. This study investigates the role of explainability in four AIGC tools and its impact on creative design. It aims to assess how explainability influences user satisfaction and computer vision metrics in the context of style transfer for iOS mobile interface design. The results demonstrate the adaptations and limitations of these AIGC tools. As observed in CycleGAN, a high level of explainability involves various factors, including superior model performance, manageable datasets, extensive fine-tuning of model parameters, user-centered human-computer interaction, explainability results presentation, and input methods. This high degree of explainability leads to the highest levels of user satisfaction and computer vision evaluation. However, it has drawbacks, as it has limited control over design details.AIGC tools with a medium level of explainability, such as Stable Diffusion and Midjourney, rely on text-to-image cues for user interaction. They scored in the medium range for user satisfaction and computer vision evaluation. These tools rely on system scalability and functional control for explainability. In contrast, the low level of explainability of "Sense of Everything" stems from single dataset control and simple user interaction. As a result, it scored low on user satisfaction and computer vision assessments, indicating a need for more controllability and poor-quality content generation. The study also analyzes the impact of computer vision assessment metrics, where SSIM aligns more with users' subjective evaluations than VGG19. The study highlights areas for improving explainability to meet user expectations for higher performance and ease of use of AIGC tools.},
  booktitle = {Proceedings of the 2023 9th International Conference on Communication and Information Processing},
  pages = {171–185},
  numpages = {15},
  keywords = {Artificial Intelligence Generated Content, Computer Vision Assessment, Explainable Artificial Intelligence, Human-Computer Interaction, Style Transfer, User Satisfaction},
  location = {Lingshui, China},
}

@inproceedings{10.1145/3665348.3665401,
  author = {Huang, Yaqing and Dong, Zhe and He, Junpeng},
  title = {Unsupervised Industrial Internet Time Series Anomaly Detection Based on Reinforcement Learning},
  year = {2024},
  isbn = {9798400709562},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3665348.3665401},
  doi = {10.1145/3665348.3665401},
  abstract = {With the popularization of big data and Internet of things technology, the amount of time series data is exploding, which makes anomaly detection in time series particularly important. Anomaly detection aims to find those data points or behaviors that do not conform to regular patterns. This study proposes an innovative unsupervised time series anomaly detection algorithm that is able to dynamically learn and adapt to anomalies in real-time data streams by combining reinforcement learning, spectral residual (SR), and variational encoder (VAE) techniques. We use a two-layer long short-term memory (LSTM) model and a self-attention network to build a reinforcement learning agent that is able to integrate temporal information of time series data to optimize policies. In addition, we propose a novel unsupervised reinforcement learning model by combining SR and VAE as the reward function. Experimental results show that our proposed algorithm outperforms the other four methods on two public datasets and one real-world dataset.},
  booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
  pages = {313–318},
  numpages = {6},
  location = {Kuala Lumpur, Malaysia},
}

@inproceedings{10.1145/3613905.3651083,
  author = {Shibani, Antonette and Knight, Simon and Kitto, Kirsty and Karunanayake, Ajanie and Buckingham Shum, Simon},
  title = {Untangling Critical Interaction with AI in Students’ Written Assessment},
  year = {2024},
  isbn = {9798400703317},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3613905.3651083},
  doi = {10.1145/3613905.3651083},
  abstract = {Artificial Intelligence (AI) has become a ubiquitous part of society, but a key challenge exists in ensuring that humans are equipped with the required critical thinking and AI literacy skills to interact with machines effectively by understanding their capabilities and limitations. These skills are particularly important for learners to develop in the age of generative AI where AI tools can demonstrate complex knowledge and ability previously thought to be uniquely human. To activate effective human-AI partnerships in writing, this paper provides a first step toward conceptualizing the notion of critical learner interaction with AI. Using both theoretical models and empirical data, our preliminary findings suggest a general lack of Deep interaction with AI during the writing process. We believe that the outcomes can lead to better task and tool design in the future for learners to develop deep, critical thinking when interacting with AI.},
  booktitle = {Extended Abstracts of the CHI Conference on Human Factors in Computing Systems},
  articleno = {357},
  numpages = {6},
  keywords = {Artificial Intelligence, Assessment, CIAW, ChatGPT, Computing, Critical Interaction, Data Science, Education, GenAI, Writing},
  location = {Honolulu, HI, USA},
}

