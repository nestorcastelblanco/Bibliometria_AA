abstract = {Wisdom education in the era of digital intelligence emphasizes the use of intelligent technology to empower education. It can integrate data wisdom, educational wisdom, and cultural wisdom to achieve the precision, creation, and optimization of education, and cultivate learners' wisdom literacy. Due to the potential problems in algorithm, calculation data, and computing power and other aspects of problems in intelligent technology, as well as due to human-related factors such as emotional barriers and management's losing control, wisdom education in the era of digital intelligence faces many risks. This paper applies the methods of philosophical speculation, ethical analysis, emotional analysis, and teaching pathology analysis to study the four major risks in them. (alienation of educational nature, human-machine ethical anomie, lack of artificial emotion, and pathological teaching), and intending to find the resolutions of the corresponding risks in wisdom education. The supposed resolutions are empowering the human-like nature of educational artificial intelligence and combining it with moral education to obtain a complete educaitonal nature of wisdom education; strengthening the governance of human-machine ethics from the source of risk (participants) through the utilization of intelligent educational ethical norms; combining human emotions with artificial emotions to establish a soul that is inclined to goodness of intelligent education; inhibiting pathogenic factors and enhancing disease resistance factors treat teaching diseases and prevent effectively through various therapies.},
booktitle = {Proceedings of the 2024 7th International Conference on Educational Technology Management},
pages = {114–120},
numpages = {7},
keywords = {Alienation of nature, Ethical anomie, Lack of soul, Pathological teaching, Risk resolution, The era of digital intelligence, wisdom education},
