  title = {23rd International Workshop on Data Mining in Bioinformatics (BIOKDD 2024)},
  year = {2024},
  isbn = {9798400704901},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3637528.3671485},
