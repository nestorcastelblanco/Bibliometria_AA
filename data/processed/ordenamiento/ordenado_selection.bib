 pages = {289–294},
 publisher = {Association for Computing Machinery},
 series = {ICEKIM '25},
 title = {Pathways and Empirical Research on Generative Artificial Intelligence Empowering Curriculum Reform in Higher Education: A Case Study of the 'Customer Relationship Management' Course},
