@inproceedings{10.1145/3744367.3744422,
  author = {Wu, Guijie and Lin, Qingqing},
  title = {Artificial intelligence-driven innovation in language teaching and learning in vocational colleges: paths, practices and challenges},
  year = {2025},
  isbn = {9798400715068},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3744367.3744422},
  doi = {10.1145/3744367.3744422},
  abstract = {The “Technology and Humanities Integration” teaching model aims to seamlessly integrate advanced technologies with humanities education principles, thereby enriching the experience of Chinese Language classrooms. Intelligent assessment systems powered by powerful data processing and analysis capabilities can accurately assess teaching efficiency and student learning progress. The collaborative framework between teachers and technology emphasizes the optimal use of teachers’ professional skills, supported by advanced technologies, thereby improving teaching efficiency and quality. Regarding the application of technology, semantic analysis is performed using the BERT model, the random forest algorithm is used to optimize teaching scenarios, and the structural equation model is applied to evaluate moral hazard. Chinese teaching practice using virtual reality technology shows that students' cultural perception ability has improved by 42\%, a finding that has been verified by rigorous statistical tests (P &lt; 0.001) and is therefore very reliable. In addition, the writing aids used significantly improved students’ Chinese writing skills, with students using these tools increasing their average Chinese score by 15\%. These empirical data firmly affirm the value of cutting-edge technology in language education in vocational schools.},
  booktitle = {Proceedings of the 2025 International Conference on Artificial Intelligence and Educational Systems},
  pages = {345–352},
  numpages = {8},
  keywords = {Artificial Intelligence, Ethical Governance, Multimodal Learning, Natural Language Processing, Vocational Education},
  location = {},
}

@inproceedings{10.1145/3749566.3749627,
  author = {Tian, Ye and Yang, Shuili and Li, Nan},
  title = {Artificial Intelligence-Driven Supply Chain Risk Mitigation},
  year = {2025},
  isbn = {9798400713927},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3749566.3749627},
  doi = {10.1145/3749566.3749627},
  abstract = {This study is grounded in the resource-based view theory and examines 1,367 Chinese A-share listed companies from 2008 to 2023. Use the Python language to crawl words related to "supply chain risk" in corporate annual reports, which are used to calculate the supply chain risk of enterprises. The fixed effects model and the intermediary effect model are used to empirically test the impact of Artificial Intelligence (AI) in mitigating the supply chain risk. Through empirical research, we find that: AI plays an instrumental role in mitigating corporate supply chain risk. AI mitigates the supply chain risk by improving resource allocation efficiency of enterprises. The heterogeneity analysis shows that for enterprises in highly competitive industries, the mitigating effect of AI on the supply chain risk is more evident; in non-technology-oriented enterprises, the mitigating effect of artificial intelligence on supply chain risk is more significant. This study contributes to the research on intelligent supply chain management and supply chain risk management.},
  booktitle = {Proceedings of the 2025 5th International Conference on Internet of Things and Machine Learning},
  pages = {316–323},
  numpages = {8},
  keywords = {Artificial Intelligence, Intelligent Supply Chain, Resource Allocation Efficiency, Supply Chain Risk},
  location = {},
}

@article{10.1145/3759253,
  author = {Bhatia, Munish},
  title = {Artificial Intelligence-inspired Anxiety Detection in Smart Office: Cyber Twin Perspective},
  year = {2025},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  issn = {2157-6904},
  url = {https://doi.org/10.1145/3759253},
  doi = {10.1145/3759253},
  abstract = {Cyber twin technology, a successful branch of simulation modeling in business, is now being applied in the healthcare sector. An intelligent architecture inspired by cyber twins is proposed to explore the unique visual, behavioral, and physiological experiences of individuals with anxiety disorders while working in smart office environments. Temporal data mining is utilized for framing data granules, and quantum probability techniques are employed for anomaly detection within the framework. Additionally, a novel multilayer Convolutional Neural Network is introduced to predict a quantifiable Health Vulnerability Index. A smart alert system is included, capable of notifying caregivers of any identified health concerns, enabling timely assistance. To evaluate the effectiveness of the proposed strategy, it was tested on real-world data comprising 82,235 cases. The results demonstrate that the method excels in several key performance metrics: time efficiency (24.6s), classification efficiency (Precision (92.77\%), Specificity (92.43\%), and Sensitivity (92.82\%)), decision-making efficiency ( (r^{2}=79\%) ), error rate (AAE 0.31\%), and stability (75\%), surpassing current state-of-the-art methodologies.},
  note = {Just Accepted},
