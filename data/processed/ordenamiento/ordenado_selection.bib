 url = {https://doi.org/10.1145/3757749.3757841},
 year = {2025}
}

@inbook{10.1145/3757749.3757856,
 abstract = {Artificial intelligence is an important technology driving the transformation of higher education in the future, and its application in the field of education has become a hot topic in academia. In this study, the CiteSpace visualization tool was used to conduct quantitative analysis on 1,467 relevant literatures selected from the CNKI database from 2015 to 2025. It is evaluated from dimensions such as publication time characteristics, author collaboration networks, and keyword clustering graphs. Systematically sort out the current research status, hot topics and development trends in this field. Research has found that in recent years, studies have focused on core areas such as higher education, integration of industry and education, and digital transformation of education. Research shows that the research direction has shifted from the application of technological tools to educational ethics and the essence of talent cultivation. Based on these findings, the study puts forward development suggestions from three dimensions. The three dimensions are respectively building a cross-disciplinary collaboration mechanism, strengthening technology-driven innovation, and enhancing the digital literacy of teachers and students. The aim is to promote the deep integration of artificial intelligence and higher education and contribute to the establishment of a new intelligent education ecosystem.},
