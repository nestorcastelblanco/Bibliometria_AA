@inbook{10.1145/3730436.3730448,
  author = {Yu, Zhijie and Zhang, Hanwen and Zheng, Guanghai},
  title = {SemiBiGAN: Semi-Cycled Bi-Level Generative Adversarial Network for Face Super-Resolution},
  year = {2025},
  isbn = {9798400713637},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3730436.3730448},
  abstract = {This paper presents a novel face super-resolution technique, SemiBiGAN, designed to overcome the limitations of traditional methods in restoring fine details and textures. Additionally, it addresses the challenges of detail recovery and artifact reduction in existing GAN-based approaches under complex scenarios. SemiBiGAN enhances feature representation and global consistency through a semi-cyclic, three-branch architecture and a Dynamic Feature Generation (DFG) block, which together improve the restoration quality of high-resolution (HR) images. The network comprises a synthetic degradation branch, a shared restoration branch, and a real-world degradation branch. The integration of the Bi-Level Routing Attention (BRA) module enables dynamic capture of region- and pixel-level features, significantly enhancing the performance of face super-resolution. Experimental results on multiple benchmark datasets demonstrate that SemiBiGAN outperforms state-of-the-art methods, both in terms of face structure/detail restoration and quantitative metrics for real-world face super-resolution.},
  booktitle = {Proceedings of the 2025 International Conference on Artificial Intelligence and Computational Intelligence},
  pages = {71–77},
}

