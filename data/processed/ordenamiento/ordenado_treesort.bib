@inproceedings{10.1145/3612783.3612798,
  author = {Manresa-Yee, Cristina and Ramis, Silvia and Gaya-Morey, F. Xavier and Buades, Jose M.},
  title = {Impact of Explanations for Trustworthy and Transparent Artificial Intelligence},
  year = {2024},
  isbn = {9798400707902},
