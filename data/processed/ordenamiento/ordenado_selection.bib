 location = {},
 numpages = {5},
 pages = {7–11},
 publisher = {Association for Computing Machinery},
 series = {ICCSIE '24},
 title = {Research on Deep Forgery Defense Technology Based on Artificial Intelligence Security},
 url = {https://doi.org/10.1145/3689236.3689240},
 year = {2024}
}

@inproceedings{10.1145/3689299.3689314,
 abstract = {Biological information processing plays a pivotal role in modern life, with applications ranging from health monitoring to personalized medicine. This study endeavors to delve into a method employing artificial neural networks for the analysis of biological signals, specifically focusing on assessing individual attention levels. By harnessing physiological data such as heart rate variability and skin conductance, coupled with the utilization of artificial neural networks for data analysis and modeling, a more comprehensive understanding of individual attention states can be attained. In the experimental phase, a diverse pool of individuals was tested, and the experimental results were validated using recorded videos. The findings underscore the efficacy of the artificial neural network-based method in assessing individual attention levels, achieving an accuracy rate of 76.9\%. This research not only introduces novel methodologies but also opens avenues for leveraging artificial intelligence technology in biological signal analysis and attention assessment. Future research endeavors could delve deeper into exploring additional features of biological signals and enhancing model performance to bolster assessment accuracy and practical applicability.},
