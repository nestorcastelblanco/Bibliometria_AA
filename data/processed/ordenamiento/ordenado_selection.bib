@inproceedings{10.1145/3708657.3708770,
 abstract = {With the rapid development of Artificial Intelligence-Generated Content (AIGC) technology, its extensive application in areas such as image generation has attracted widespread attention. However, the proliferation of generated images has also raised issues such as information distortion, which poses challenges to the protection of original content and the assurance of information authenticity. As a result, many detection methods have been proposed in recent years to identify generated images. By analysing existing AIGC detection methods, we categorise them into four groups: frequency domain features, traditional models, image reconstruction and multi-modal large models. Through comparative analysis, this paper highlights the advantages and limitations of current detection methods and outlines potential research directions for the future. Our aim is to provide valuable insights to researchers in related fields, thereby promoting the further development of AIGC detection technology and enhancing our ability to address the challenges posed by AIGC.},
