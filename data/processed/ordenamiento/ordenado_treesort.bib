@inproceedings{10.1145/3608367.3608375,
  author = {Li, Shengyu and Zhou, Yaping and Zhang, Yunjie and Zhang, Zirou},
  title = {An Overview of Artificial Intelligence Applications in Food Cold Chain Transportation},
  year = {2023},
  isbn = {9781450398510},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3608367.3608375},
  doi = {10.1145/3608367.3608375},
  abstract = {In recent years, the rising income level of the population and the young consumer force brought about a continuous improvement and upgrading of the diet structure and rapid growth of the food cold chain logistics market. However, frequent outbreaks of food quality and safety issues have raised consumer concerns. As the key link of cold chain logistics, cold chain transportation has a direct and important impact on the freshness of food and even food safety. Facing the current situation of imperfect food cold chain transportation systems, it is necessary to make full use of artificial intelligence algorithms and convenient framework platforms in order to reduce food loss rates, strengthen transportation efficiency and improve customer satisfaction. In this paper, we study and summarize the application of artificial intelligence in cold chain transportation, and optimize the path planning problem of cold chain logistics delivery vehicles by using the bee colony algorithm. The environmental monitoring system and cold chain precise regulation cloud platform are used in the transportation process to monitor the external environment and guarantee the quality of food transportation. In the scheduling of cold chain transportation vehicles, the optimized ant colony algorithm is used to resolve the vehicle scheduling problem. The problem of specific vehicle search directionality from the scheduling source point to the target point of the logistics center is included in the optimization consideration, which will greatly improve the vehicle scheduling time and distance efficiency. The combination of artificial intelligence technology in cold chain transportation can reduce the average 10\% of human and material resources and effectively improve the quality of transported food, thus reducing transportation costs and food loss and ensuring food safety and sustainable development.},
  booktitle = {Proceedings of the 2023 5th International Conference on Robotics Systems and Automation Engineering},
  pages = {34–40},
  numpages = {7},
  keywords = {Algorithm Optimization, Artificial Intelligence, Cold Chain, Food Transportation},
  location = {Online, United Kingdom},
}

