@inbook{10.1145/3728725.3728779,
author = {Liu, Chengyu and Liu, Jing},
title = {Research on Load Monitoring and Regulation of Human Movement Training Assisted by Intelligent Algorithms},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
