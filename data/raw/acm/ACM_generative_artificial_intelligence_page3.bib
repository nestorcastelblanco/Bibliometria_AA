@inproceedings{10.1145/3691422.3691471,
author = {Ma, Xiaoqian},
title = {The potential legal risks of artificial intelligence},
year = {2025},
isbn = {9798400717260},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3691422.3691471},
doi = {10.1145/3691422.3691471},
abstract = {ChatGPT, as a typical application of generative artificial intelligence, means that human society is moving towards the "era of high knowledge revolution". From the perspective of functionalism, generative artificial intelligence will certainly have an important impact on the reshaping of legal society at the level of "instrument" and "Tao". However, while generative AI is deeply embedded in Chinese society, it also impacts personal information security on a large scale, challenges national security, causes intellectual property rights disputes, and academic ethics irregularities. Therefore, it is necessary for the national regulatory authorities to exercise careful governance, formulate accurate and fair market access guidelines and mechanism accountability systems, improve the public's awareness of risk prevention, and gradually improve the generative AI governance system.},
booktitle = {Proceedings of the 2024 15th International Conference on E-Business, Management and Economics},
pages = {366–370},
numpages = {5},
keywords = {ChatGPT, Generative artificial intelligence, Legal regulation, Risk management},
location = {
},
series = {ICEME '24}
}

@inproceedings{10.1145/3766918.3766963,
author = {Wang, Yunning},
title = {Refined Prompt Strategies in Generative Image Models: From Semantic Construction to Output Control},
year = {2025},
isbn = {9798400716027},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3766918.3766963},
doi = {10.1145/3766918.3766963},
abstract = {In generative image models, the quality of textual prompts directly influences the accuracy and coherence of the generated outputs. This paper focuses on how refined textual prompt design can enhance the precision of image generation. It systematically analyzes the linguistic strategies used in subject description, environment construction, style constraints, lighting control, and compositional elements. By comparing the outcomes of vague prompts with those of detailed prompts, the study demonstrates that clear attribute specifications (such as age, ethnicity, and emotional state), spatiotemporal scene details (e.g., weather conditions, background dynamics), and explicit boundary settings (e.g., negation constraints, spatial symmetry) are critical to reducing image distortion and enhancing semantic consistency. Furthermore, the paper proposes a multi-element prompt template to serve as a practical linguistic control framework for image generation tasks. The study emphasizes that prompts are not merely input commands but essential logical units for output regulation, and their refinement is vital for improving both the quality and controllability of AI-generated images.},
booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence for Business},
pages = {266–271},
numpages = {6},
keywords = {Fine-grained Control, Generative Image Models, Prompt Engineering, Semantic Consistency, Textual Prompts},
location = {
},
series = {GAIB '25}
}

@inproceedings{10.1145/3766918.3766980,
author = {Wang, Runzhe and Lu, Xinyuan and Wang, Xueqing},
title = {Decision agency: How can generative AI reconstruct consumer behavior in trendy toys?},
year = {2025},
isbn = {9798400716027},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3766918.3766980},
doi = {10.1145/3766918.3766980},
abstract = {Amid intensifying algorithmic mediation, collectible toy brands confront a loyalty crisis as generative AI redirects consumer attention from iconic IPs to context-matched novelties. The field lacks a unifying framework explaining how AI reallocates decision agency. We address this gap by developing and testing the Decision Agency Model. Using a between-subjects experiment, we exposed 100 verified Pop Mart consumers to either AI-curated or self-selected product sets across 20 usage scenarios and 100 SKUs. AI recommendations reduced IP selection by 46 percentage points, elevated scenario-congruent items to top rank, and cut composite satisfaction by 0.5 points despite faster choices. The findings reveal that scenario tags supplant brand salience as the dominant cue and uncover an efficiency–affect trade-off inherent in algorithmic curation. We contribute a parsimonious model of AI-driven loyalty erosion and provide actionable guidance for hybrid recommender architectures.},
booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence for Business},
pages = {375–382},
numpages = {8},
keywords = {Brand loyalty, Consumer behavior, Decision agency, Generative AI, Scenario tags, Trendy toys},
location = {
},
series = {GAIB '25}
}

@inproceedings{10.1145/3766918.3766981,
author = {Yin, Yexing and Chen, Tingting and Deng, Qiuping},
title = {Dynamic public opinion risk assessment based on generative AI and multimodal sentiment analysis},
year = {2025},
isbn = {9798400716027},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3766918.3766981},
doi = {10.1145/3766918.3766981},
abstract = {This paper proposes a dynamic public opinion risk assessment algorithm based on generative AI and multimodal sentiment analysis, aiming to solve the problem of insufficient utilization of multimodal information and difficulty in dynamically capturing risk evolution in traditional public opinion risk assessment methods. The algorithm first collects and preprocesses multimodal public opinion data such as text, images, videos, and audio, and then extracts the features of each modality. Through the designed multimodal feature dynamic fusion mechanism, the weights are adjusted in real time according to the impact of different modalities on sentiment and risk, and then combined with the Transformer-based generative model to predict the evolution trend of public opinion risk, and finally realize dynamic public opinion risk assessment. The experimental simulation uses CMU-MOSI, MOSEI datasets and multimodal public opinion data in specific fields. The results show that in terms of sentiment analysis, the algorithm in this paper has an accuracy of 89.2\%, precision of 88.5\%, recall of 89.0\%, and F1 value of 88.7\% on the CMU-MOSI dataset, and an accuracy of 87.8\%, precision of 87.0\%, recall of 88.2\%, and F1 value of 87.6\% on the MOSEI dataset, which is an average increase of 3.5\%-5.2\% over the accuracy of the comparison algorithm; in public opinion risk assessment, the risk prediction accuracy is 85.6\%, MAE is 0.12, and RMSE is 0.15, which is 4.8\%-6.3\% higher than the comparison method, and MAE and RMSE are reduced by 0.03-0.05 and 0.04-0.06 respectively. The algorithm can effectively improve the accuracy and timeliness of dynamic public opinion risk assessment.},
booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence for Business},
pages = {383–388},
numpages = {6},
keywords = {Generative AI, dynamic public opinion risk assessment, feature fusion, multimodal sentiment analysis, risk evolution prediction},
location = {
},
series = {GAIB '25}
}

