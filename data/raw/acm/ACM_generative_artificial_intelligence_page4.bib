abstract = {With the increase in the volume of data within auditing environment, the risk assessment of audit increasingly depends on data-based anomaly detection and internal control issues detection. In this paper, we propose a novel deep generative model with variational autoencoder Hierarchical Attention-Driven VAE (HAD-VAE) for audit risk scoring and review visualization. The model extends the typical VAE framework, and forms a two-layer hidden structure of latent variables for current study to capture both global behavioral patterns of the audit object and small local differences in transactions’ features. Through the design of attention mechanism and weighting to the modeling of temporal audit features, the model improves the raw skills of perceiving high risk behaviors, and leverages prior risk information from the auditing field to regularize the latent space of embedding, which enables the model to learn more discriminative risk representations. In the decoder side, the model uses two channels to reconstruct path: one channel for numerical fields and another for the semantic descriptions of transactions, which enable fine-grained modeling of the complex data, and supports the following visualization analysis. The loss function of the overall training maximizes the evidence lower bound (ELBO) and adds a contrastive loss to further separate high-risk samples from low-risk samples in the latent space. HAD-VAE achieves better performance than state-of-the-art baselines in terms of both risk scoring accuracy and anomaly localization.},
booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence for Business},
pages = {73–77},
numpages = {5},
keywords = {Attention Mechanism, Audit Risk Assessment, Latent Variable Modeling, Variational Autoencoder},
location = {
},
series = {GAIB '25}
}

@inproceedings{10.1145/3748825.3748959,
author = {Zhong, Lin and Gao, Chaomin},
title = {Study on Authors'  Attitudes Toward Generative AI Applications in Scientific Journal Proofreading},
year = {2025},
isbn = {9798400714337},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3748825.3748959},
doi = {10.1145/3748825.3748959},
abstract = {This study investigates authors' acceptance of generative artificial intelligence (AI) tools for proofreading scientific journals and explores the underlying mechanisms. It specifically analyzes the moderating role of trust in the relationship between technology-task fit (TTF) and acceptance, and the mediating role of intention to use between perceived usefulness and acceptance. Data were collected via a questionnaire survey from 352 valid respondents, covering core variables: TTF, trust level, perceived usefulness, intention to use, and acceptance. Statistical methods including reliability tests, correlation analysis, and regression analysis were employed to test the hypotheses. The results indicate that trust significantly enhances the positive effect of TTF on acceptance. Furthermore, intention to use partially mediates the relationship between perceived usefulness and acceptance, explaining 43.05\% of the total effect. High reliability was confirmed for all variables, and correlation results supported the theoretical framework. The findings conclude that trust level and intention to use act as crucial moderator and mediator, respectively, in the acceptance process of generative AI tools, with TTF and perceived usefulness being key determinants of acceptance. This research offers significant insights for promoting the application of generative AI within academic publishing.},
booktitle = {Proceedings of the 2025 2nd International Conference on Digital Society and Artificial Intelligence},
pages = {870–876},
numpages = {7},
keywords = {Acceptance, Generative Artificial Intelligence, Intention to Use, Perceived Usefulness, Scientific Journals, Technology-Task Fit, Trust Level},
location = {
},
series = {DSAI '25}
}

@inproceedings{10.1145/3734921.3734932,
author = {Ren, Zhiyang and Jin, Guangri},
title = {Artificial neural network-based model for predicting compressive strength of cemented gravels},
year = {2025},
isbn = {9798400713460},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3734921.3734932},
doi = {10.1145/3734921.3734932},
abstract = {In order to study the compressive strength of cemented gravel, a compressive strength prediction model was established; with 142 groups of in-house uniaxial compressive strength test data of cemented gravel with different mixing ratios as the object of study, the 11 influencing factors in the samples were downscaled by using the principal component analysis method, and the downscaled principal components were used as the input layer of the BPNN, so as to establish the compressive strength prediction of cemented gravel based on the PCA-BPNN model. The results show that when the number of principal components is 3, the cumulative contribution rate of the principal components reaches 86.483\%, and the contribution rates of the three principal components are 65.084\%, 11.702\%, 9.097\%, and the eigenvalues of the three principal components are all greater than 1. Then the three principal components are used as the input layer to build the compressive strength model of cemented gravel based on PCA-BPNN, and the RMSEs of the training set and test set are 0.253 and 0.28, respectively. The RMSE of the training set and the test set are 0.253 and 0.286, and the R2 is 0.947 and 0.942, which are not much different from the BPNN model with the number of nodes in the input layer of 6 and 11 before dimensionality reduction, and they are obviously better than the BPNN model in the convergence speed and training time. It can be seen that the PCA-BPNN model has good prediction effect and prediction efficiency, especially drastically reducing the training time, which provides a new idea for the prediction research of the mechanical properties of cemented gravel.},
booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence and Digital Media},
pages = {63–70},
numpages = {8},
keywords = {Artificial neural network, Cemented Gravels, Compressive Strength, PCA},
location = {
},
series = {GADM '25}
}

@inproceedings{10.1145/3715336.3735808,
author = {Yuan, Xiaojing and Yuan, Xiang and He, Yuxiao and Wang, Zixi and Ren, Jia and Bryan-Kinns, Nick},
title = {Cultural Erosion or Innovation? Artisans’ Attitudes Toward AI-Generated Patterns in Chinese Traditional Subcultures},
year = {2025},
isbn = {9798400714856},
