 url = {https://doi.org/10.1145/3726010.3726018},
 year = {2025}
}

@inproceedings{10.1145/3726010.3726027,
 abstract = {With the rapid development of artificial intelligence (AI) technology, the field of art creation has also ushered in unprecedented changes. As an important form of traditional Chinese art, ink painting has attracted more and more attention to its unique expression techniques and cultural connotation. This paper discusses the application of artificial intelligence in the creation of ink painting visual art, and analyzes how AI combines with ink painting art to promote the innovation and development of traditional art forms. Through case study and experimental analysis, this paper demonstrates the potential of AI in ink art and proposes the direction of future development.},
