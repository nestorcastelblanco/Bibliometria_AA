@inbook{10.1145/3730436.3730449,
 abstract = {Digital accessibility is essential for the equitable dissemination of content; however, certain EPUB files fail to adhere to fundamental accessibility standards, particularly in the provision of alternate text (alt text) for images. Alt text is indispensable for visually impaired individuals to understand visual content through assistive technologies. Nonetheless, the manual generation of high-quality alt text at scale continues to pose a considerable difficulty because of the requisite skill and time involved.This paper introduces AltGen, a framework utilizing generative AI to automate the development of alt text for EPUB files. AltGen uses pre-trained transformer models to provide descriptive and contextually pertinent alt text. It employs a structured process encompassing data extraction, visual content analysis with computer vision models (e.g., CLIP, ViT), and context-sensitive text generation utilizing fine-tuned large language models. The produced alt-text is assessed by computational measures, including cosine similarity and BLEU scores, and qualitative input from visually challenged individuals. Experimental findings demonstrate that AltGen lowers accessibility errors by 97.5\%, with participants noting substantial enhancements in content understanding and usability.},
 address = {New York, NY, USA},
 author = {Shen, Yixian and Zhang, Hang and Shen, Yanxin and Wang, Lun and Shi, Chuanqi and Du, Shaoshuai and Tao, Yiyi},
 booktitle = {Proceedings of the 2025 International Conference on Artificial Intelligence and Computational Intelligence},
 isbn = {9798400713637},
 numpages = {6},
 pages = {78–83},
 publisher = {Association for Computing Machinery},
 title = {AltGen: AI-Driven Alt Text Generation for Enhancing EPUB Accessibility},
 url = {https://doi.org/10.1145/3730436.3730449},
 year = {2025}
}

@inbook{10.1145/3730436.3730526,
 abstract = {In the context of the information era of rapid development of artificial intelligence, the frequency of data updating and the role of data is always in dynamic change, and the research of design behavior is facing new opportunities and challenges. This paper focuses on the study of factors affecting design behavior in the view of artificial intelligence, aiming to analyze the weight of each influencing factor accurately. Through the hierarchical analysis method (AHP), its advantage of quantitative subjective judgment is brought into play, and experts' subjective perception of the importance of design behavior factors is deeply explored. At the same time, combined with the entropy weight method, the weights are determined based on the objective information entropy of the data, which effectively makes up for the lack of subjective assignment. Using the AHP-entropy weight method combined with these two methods, a comprehensive and systematic weight evaluation of design behavior factors is carried out to improve the efficiency of multi-attribute decision-making.},
 address = {New York, NY, USA},
 author = {Liang, Jian},
