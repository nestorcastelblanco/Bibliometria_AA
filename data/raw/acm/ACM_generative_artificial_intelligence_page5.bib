@inproceedings{10.1145/3655497.3655516,
author = {Le Dinh, Thang and Le, Tran Duc and Uwizeyemungu, Sylvestre and Nguyen, Chan Nam},
title = {Literature Review as a Service: A Human-Centered Artificial Intelligence Approach},
year = {2024},
isbn = {9798400709302},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3655497.3655516},
doi = {10.1145/3655497.3655516},
abstract = {Nowadays, the use of generative artificial intelligence (AI), such as chatGPT and other AI-powered tools, has a significant impact on conducting research projects. Despite the advantages of this trend, it is believed that AI-powered tools should be utilized to enhance the literature review process rather than completely automating it. For this reason, this paper examines the emerging concept of "Literature Review as a Service" (LRaaS) based on the human-centered artificial intelligence (HCAI) perspective. Drawing on the theoretical background and service science, the paper explores a new approach to exploit the potential of AI-powered tools while putting people at the center of the research process. Through an initial prototype, the paper illustrates how AI-powered tools assist the literature review process. The paper ends with conclusions and recommendations for further research.},
booktitle = {Proceedings of the 2024 International Conference on Innovation in Artificial Intelligence},
pages = {52–58},
numpages = {7},
keywords = {Artificial intelligence, human-centered AI, literature review, service science},
location = {Tokyo, Japan},
series = {ICIAI '24}
}

@inproceedings{10.1145/3605098.3636201,
author = {Conde, Javier and Mart\'{\i}nez, Gonzalo and Reviriego, Pedro and Salvach\'{u}a, Joaqu\'{\i}n and Hern\'{a}ndez, Jos\'{e} Alberto},
title = {Designing Metadata for the Use of Artificial Intelligence in Academia},
year = {2024},
isbn = {9798400702433},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3605098.3636201},
doi = {10.1145/3605098.3636201},
abstract = {Academic writing is one of the most important tasks in Academia. The pressure to "publish or perish" drives researchers to use all the tools available to try to improve their papers and their impact. Generative artificial intelligence (AI) that can create content such as text, tables, and images can be used for many tasks in academic writing such as summarizing, translating, paraphrasing, data analysis, and presentation. Therefore, AI is expected to be widely used in academic writing in the near future and have a significant impact. Understanding this impact is far from trivial and needs to be carefully studied. The first step in doing so is to be able to identify papers written with the help of AI tools. This can be done by adding metadata on the use of AI in academic results. In this paper, we embark on an initial endeavor to devise such metadata and delineate the potential advantages that the inclusion of AI-related metadata in academic publications may bring forth.},
booktitle = {Proceedings of the 39th ACM/SIGAPP Symposium on Applied Computing},
pages = {1662–1664},
numpages = {3},
keywords = {metadata, artificial intelligence, academic writing, linked data, semantics},
location = {Avila, Spain},
series = {SAC '24}
}

@inproceedings{10.1145/3709022.3736543,
author = {Park, Sunyoung and Kim, Hyunji and Choi, Seul-Ki and Kim, Taeeun and Park, Eunil},
title = {Privacy-Driven Faces: A Survey on Generative Facial De-identification},
year = {2025},
isbn = {9798400714191},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3709022.3736543},
doi = {10.1145/3709022.3736543},
abstract = {The rise of deepfake technologies has introduced powerful tools for synthetic face generation, which is often associated with manipulation and privacy violations. However, recently, these generative techniques are repurposed for privacy-preserving applications. This paper presents a structured review of facial anonymization methods that leverage generative models to conceal identity while preserving semantic features, such as expressions and poses. We analyze seven representative studies, comparing their model architectures, alignment with deepfake transformation types, evaluation metrics, and application scenarios. Our review identifies a range of promising technical strategies with particular attention to how recent approaches address demographic fairness, face and body anonymization, and real-world deployment challenges. We underscore the importance of ethically sourced data and policy-aware designs as prerequisites for responsible use. By positioning facial de-identification not only as a technical challenge but also as a sociotechnical necessity, this review outlines a research agenda centered on the privacy, utility, and equitable adoption of generative artificial intelligence systems.},
booktitle = {Proceedings of the 4th Workshop on Security Implications of Deepfakes and Cheapfakes},
pages = {27–32},
numpages = {6},
keywords = {DeepFake, DeepPrivacy, Face-identification, Privacy, Survey},
location = {Hanoi, Vietnam},
series = {WDC '25}
}

@inproceedings{10.1145/3724389.3731270,
author = {Silberg, Owen and Glavan, Mary and Myers, Risa B.},
