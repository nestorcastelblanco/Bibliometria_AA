 isbn = {9798400712791},
 keywords = {Artificial Intelligence Silicone pressure-sensitive adhesive, Silicone resin, Thiol-ene click chemistry, UV-curing},
 location = {},
 numpages = {8},
 pages = {1111–1118},
 publisher = {Association for Computing Machinery},
 series = {DEAI '25},
 title = {Development of organic pressure-sensitive adhesive under the background of artificial intelligence},
 url = {https://doi.org/10.1145/3745238.3745412},
 year = {2025}
