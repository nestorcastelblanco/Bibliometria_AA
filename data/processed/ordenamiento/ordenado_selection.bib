@inproceedings{10.1145/3757110.3757144,
 abstract = {The adoption of artificial intelligence (AI) in the anti-infringement detection of IP theft has taken on increased significance with the increase in digital content creation and distribution. This paper discusses how AI is successful in detecting IP violations for images, sound and text, not only for detection, but also for legal and ethical issues. These are the fundamental issues around originality, transformative use, and fair use for AI that will need to be taken into account to meet IP law. It also tests model performance metrics — accuracy, precision, recall, F1-score — as a function of legality and proof-based reliability. True positives, bias, and data privacy are discussed from legal perspectives as well as impacts to individuals and companies, as well as regulatory requirements such as GDPR and CCPA. The future of the sector lies in morally better model training, data selection and privacy protection. Findings indicate that AI provides promising IP protection, but risk management is essential to ensure legal, moral application in IP enforcement.},
 address = {New York, NY, USA},
 author = {Si, Wen},
 booktitle = {Proceedings of the 2025 2nd International Conference on Modeling, Natural Language Processing and Machine Learning},
 doi = {10.1145/3757110.3757144},
 isbn = {9798400714344},
