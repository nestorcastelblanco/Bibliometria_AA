 location = {},
 numpages = {5},
 pages = {59–63},
 publisher = {Association for Computing Machinery},
 series = {IECA '25},
 title = {A Support Vector Machine Approach to Evaluating the Impact of Generative AI on College Students' Innovation Ability},
