@inproceedings{10.1145/3700297.3700374,
  author = {Liu, Can and Xu, Siyi and Chen, Yuxuan and Wang, Zixiang and Chao, Lu and Lin, Yang and Yan, Hao},
  title = {Research on Students’ Utilization of Artificial Intelligence Based on Random Forest Model and PCA-K-means Algorithm},
