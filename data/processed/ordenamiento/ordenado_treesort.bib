@inbook{10.1145/3728725.3728769,
  author = {Wang, Hongjuan and Wang, Nan and An, Xinjun},
  title = {CAM-YOLO v8: A model for water meter word wheel region segmentation},
  year = {2025},
  isbn = {9798400713453},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3728725.3728769},
  abstract = {The use of mobile phone APP meter reading reduces the probability of human intervention and misidentification, but due to the complex location and geographical conditions of the buried water meter, as well as the base code and gear in the water meter dial, there are problems of low accuracy and rough segmentation results when the water meter word wheel region is divided, so a YOLO v8 segmentation method based on coordinate attention mechanism is proposed. The backbone network incorporates the AM (Attention-MobileNet_block) module to apply the Coordinate Attention (CA) mechanism, which facilitates the extraction of multi-scale features, reduces model parameters and computational complexity, and enhances computing efficiency. In the feature fusion stage, the C2f_CA mechanism, based on the Bottleneck_CA module, effectively mitigates background interference while concentrating on the water meter's target region. Experimental results indicate that, compared to other segmentation models, our model achieves an F1-score of 98.61\% and a mean Pixel Accuracy (mPA) of 99.11\% for word wheel region segmentation, establishing a solid foundation for the precise identification of water meter code words in the future.},
  booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
  pages = {278–283},
}

@inproceedings{10.1145/3702386.3702394,
  author = {Qi, Yunlong and Fu, Fangxiang and Tian, Jianchi and Sun, Yan},
  title = {Can AI Be Environmentally Responsible? A Comparative Study on the Pro-Environmental Portrait of ChatGPT and Chinese Respondents},
  year = {2025},
  isbn = {9798400710131},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3702386.3702394},
  doi = {10.1145/3702386.3702394},
  abstract = {ChatGPT, a representative of the new generation of generative artificial intelligence (AI), has garnered widespread attention due to its extraordinary natural language processing capabilities. This study examined ChatGPT's environmental value orientations and ecological worldview within the Value-Belief-Norm (VBN) theory framework and compared these findings with the perceptions and self-assessments of 300 Chinese respondents. The results indicate that ChatGPT exhibits a significant pro-environmental orientation, although differences exist between its orientations and individual predictions of its environmental stance. The study provides theoretical support for the application of next-generation AI in environmental education and advocacy, contributes new evidence to the discourse on human-machine value alignment, and offers insights into human-AI collaboration in environmental protection.},
  booktitle = {Proceedings of the 2024 International Conference on Artificial Intelligence and Teacher Education},
  pages = {43–49},
  numpages = {7},
  keywords = {ChatGPT, alignment, pro-environmental orientation, “Value-Belief-Norm” theory},
  location = {},
}

