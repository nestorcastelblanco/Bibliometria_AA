@inproceedings{10.1145/3745238.3745412,
author = {Shen, Qili and Liu, Guocong and Hou, Bolin and Qiang, Na and Yang, Xiaolong and Xu, Runfeng and Liu, Zhu and Fang, Yanxiong and Huang, Xin and Lu, Zhe},
title = {Development of organic pressure-sensitive adhesive under the background of artificial intelligence},
year = {2025},
isbn = {9798400712791},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3745238.3745412},
doi = {10.1145/3745238.3745412},
abstract = {Silicone pressure-sensitive adhesives (SiPSAs) have demonstrated broad application potential in electronics and other fields due to their exceptional properties, such as temperature resistance, weather resistance, and chemical corrosion resistance. However, traditional SiPSAs are predominantly thermally cured solvent-based products, which not only exhibit slow curing rates but also release harmful solvents like toluene during application, causing environmental pollution. To address this issue, this study Under the background of artificial intelligence. synthesized thiol-functionalized MTQ silicone resins (MTQ-SH) with various M/(T+Q) ratios using γ-mercaptopropyltrimethoxysilane, tetraethyl orthosilicate, and hexamethyldisiloxane as raw materials. These resins were then photo-crosslinked with vinyl-terminated silicone oil to prepare UV-curable silicone PSAs (UV-SiPSAs). The results revealed that the optimal precursor ratio for UV-SiPSA preparation was M/T/Q = 1.00/0.21/1.58. When the photoinitiator TPO-L was added at 1 wt.\% and the molar ratio of thiol groups to vinyl end groups was 1.2:1, the resulting UV-SiPSA exhibited an initial adhesion strength capable of holding a 22# steel ball and a holding time of 230 minutes at 25℃. This UV-curable PSA shows promising potential for replacing traditional thermally cured silicone PSAs in the protective film industry.},
booktitle = {Proceedings of the 2nd Guangdong-Hong Kong-Macao Greater Bay Area International Conference on Digital Economy and Artificial Intelligence},
pages = {1111–1118},
numpages = {8},
keywords = {Artificial Intelligence Silicone pressure-sensitive adhesive, Silicone resin, Thiol-ene click chemistry, UV-curing},
location = {
},
series = {DEAI '25}
}

@inbook{10.1145/3756423.3756543,
author = {Shi, Junjie and Gao, Yong and Guo, Hantao},
title = {Research Progress on the Reliability of Aircraft Emergency Hatch Ejection Drive Mechanisms Integrated with Artificial Intelligence},
year = {2025},
isbn = {9798400714351},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3756423.3756543},
abstract = {Artificial intelligence (AI) and advanced sensor technologies have developed into maintenance, reliability, and optimized aircraft emergency hatch ejection drive mechanisms. The review study narrates research on AI-driven approaches: machine learning, deep learning, federated learning, and generative algorithms (GAs) for fault diagnosis. Internet of Things (IoT) enabled real-time monitoring and intelligence maintenance techniques. Recurrent neural networks particularly long short-term memory (LSTM) models and ML techniques analyse the sensor data for prediction of stress, wear, and failure modes of components, achieving high accuracy and enabling proactive interventions. The results reveal that IoT networks coupled with AI approaches continuously monitor critical parameters including vibration and temperature. This study reveals that optimization techniques, genetic algorithms, and particle swarm optimization play significant roles in enhancing the structural design of components like gears and connecting rods that achieve weight reductions without compromising safety. This study identifies challenges of model interpretability, data scarcity, and regulatory compliance. These issues are addressed by proposing hybrid AI models and federated learning. Emerging trends include self-healing materials, quantum ML approaches, and automated maintenance systems, which could transform safety in the aviation industry. By bridging research gaps between engineering practices and data-driven insights, this study shows that AI can play a vital role in advancing the efficiency and reliability of drive mechanisms in next-generation aircraft, compliance with aviation protocols, and paving the way for adaptive and autonomous systems.},
booktitle = {Proceedings of the 2025 International Conference on Artificial Intelligence and Smart Manufacturing},
pages = {729–734},
numpages = {6}
}

