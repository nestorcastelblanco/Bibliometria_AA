@inproceedings{10.1145/3665348.3665399,
  author = {Zhang, Mingjie and Zhu, Longzhu and Ding, Maomao and Liu, Lu and Gong, Lihua and Gong, Jian},
  title = {95598 Customer Service Complaint Spillover Risk Prediction Method Based on Data Association Clustering},
  year = {2024},
  isbn = {9798400709562},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3665348.3665399},
  doi = {10.1145/3665348.3665399},
  abstract = {Conventional 95598 customer service spillover risk prediction method mainly uses SMOTE (Synthetic Minority Oversampling) minority oversampling method to deal with the prediction samples, which is susceptible to the influence of weighted fusion, resulting in poor prediction results, therefore, a new 95598 customer service spillover risk prediction method needs to be designed based on the correlation clustering of data, i.e., collecting and processing the complaint spillover risk prediction data, designing the customer service spillover risk prediction algorithm using data correlation clustering, and thus realizing the customer service spillover risk prediction. Therefore, it is necessary to design a new 95598 customer service complaint spillover risk prediction method based on data association clustering, i.e., we collected and processed the complaint spillover risk prediction data, and designed a customer service complaint spillover risk prediction algorithm by using data association clustering, so as to realize the customer service complaint spillover risk prediction. The experimental results show that the designed customer service complaint spillover risk prediction method based on data association clustering has good prediction effect, and the predicted root mean square error (RMSE), predicted mean absolute percentage error (MAPE), and predicted mean absolute error (MAE) are low, reliable, and have certain application value, which makes a certain contribution to the improvement of customer service quality.},
  booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
  pages = {297–302},
  numpages = {6},
  location = {Kuala Lumpur, Malaysia},
}

@inproceedings{10.1145/3675812.3675869,
