@inproceedings{10.1145/3722237.3722376,
  author = {Wang, Lipeng and Leong, Wai Yie},
  title = {Comparative Study on Artificial Intelligence Application and International Talent Training Strategies from an Interdisciplinary Perspective},
  year = {2025},
  isbn = {9798400712692},
