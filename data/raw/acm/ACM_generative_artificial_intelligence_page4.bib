doi = {10.1145/3766918.3766937},
abstract = {The limited availability of labeled defect data poses a critical challenge in developing robust industrial defect detection systems. This study presents a Transformer-guided diffusion framework capable of synthesizing high-quality defect images for data augmentation. A Transformer encoder is embedded within a U-Net-based denoising network, facilitating the modeling of long-range dependencies and preserving fine-grained local textures. An Adaptive Defect Attention Block (ADAB) is incorporated to enhance the representation of subtle defect features through combined spatial and channel-wise attention. A multi-channel hybrid loss function supervises both image content and defect mask consistency, promoting structural coherence and stable training. Experimental evaluations on the Cotton Fabric and MVTec datasets reveal that the proposed method consistently surpasses diffusion-based baselines—including DDPM, Sin-Diffusion, and Defect-Gen—across various metrics such as LPIPS, DISTS, PSNR, FID, and SSIM. These results highlight the effectiveness of the architectural improvements in producing realistic and diverse defect images. The proposed framework offers a practical and scalable solution for industrial scenarios constrained by limited defect annotations.},
booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence for Business},
pages = {107–113},
numpages = {7},
keywords = {Attention mechanism, Data augmentation, Defect image synthesis, Diffusion model, Industrial inspection, Transformer},
location = {
