@inproceedings{10.1145/3665348.3665385,
author = {Yang, Lang},
title = {A meta-learning based method for detecting malicious traffic with few-shot},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665385},
doi = {10.1145/3665348.3665385},
abstract = {For the increasingly complex network environment, new types of attacks are emerging, and the limited attack samples may not be sufficient for traditional detection methods to achieve good detection results. In this regard, this paper proposes a few-shot detection method based on meta-learning, which mainly consists of three parts: data preprocessing, feature extraction and comparison, and classification and detection. The original data samples are transformed into grey scale graphs, and after combining the samples two by two, they are respectively subjected to feature extraction and comparison, which results in similarity scores, which are finally used to carry out classification. It is shown through experiments that the accuracy of detection can reach up to 98.45\% in the case of few-shot.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {211–216},
numpages = {6},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inbook{10.1145/3728725.3728731,
author = {Ma, Xuening and Chen, Xin and Yuan, Chunhong},
title = {Crossroads of AI and Tourism: Enhancing Destination Management and Traveler Engagement},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728731},
abstract = {The swift progression of artificial intelligence (AI) technology is transforming the tourist industry by redefining decision-making processes, augmenting operational accuracy, and elevating user experience. This study examines the transformative influence of AI through a comprehensive analysis of its application in destination management and traveler engagement. This study combines traditional survey methods with advanced heatmap analysis to reveal the complex impact of AI on traveler behavior, preferences, and decision-making efficiency. The results indicate that AI-powered tailored suggestions, and predictive analytics markedly improve customer satisfaction and decision-making, while optimizing operational procedures for tourism enterprises. The study emphasizes the necessity for more algorithmic transparency and trust-building measures to alleviate visitors' apprehensions regarding fairness and relevance. This research integrates theoretical frameworks with practical applications, offering significant insights for enhancing AI systems to foster innovation and growth in the tourism sector.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {31–36},
numpages = {6}
}

@inproceedings{10.1145/3665348.3665354,
author = {Niu, Lianqiang and Bao, Han},
title = {Fast Tactile Paving Segmentation Model Based on Reparameterized Structure},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665354},
doi = {10.1145/3665348.3665354},
abstract = {In order to solve the problem of low segmentation accuracy due to the interference of noise information such as light and shadow in the current lightweight blind segmentation model, we propose a fast tactile paving segmentation model based on reparameterize. The multi-branch training structure learns spatial features such as tactile paving, light and shadow while compensating for the loss of information in the feature map due to downsampling operations as well as enhancing the learning of its features in the channel dimension. The single-branch structure obtained after reparameterization of the multibranch structure ensures faster inference of the model as well as lower memory access costs. The introduction of coordinate attention allows the model to learn the distribution law of the features of tactile paving, light and shadow, etc. in the spatial dimension and automatically generate the attention map with weight information, effectively reducing the weight of the spatial structural features of noise such as light and shadow. Experiments show that the model's ability to resist interference from environmental noise such as light and shadow has been enhanced and the segmentation accuracy has been further improved under the premise of ensuring the inference speed.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {24–28},
numpages = {5},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inproceedings{10.1145/3734921.3734927,
