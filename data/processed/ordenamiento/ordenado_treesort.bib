@inproceedings{10.1145/3715020.3715061,
  author = {Suansap, Anucha and Kesvarakul, Ramil and Sukmanee, Jenjira},
  title = {Development of Microalgae Detection via Artificial intelligence},
  year = {2025},
  isbn = {9798400709623},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3715020.3715061},
  doi = {10.1145/3715020.3715061},
  abstract = {Microalgae, often found in untreated water sources used in water production, pose a significant challenge due to their microscopic size, making it difficult to eliminate them using traditional filtration systems. Identifying and quantifying microalgae contaminants in raw water is time-consuming and labor-intensive. This research aims to develop a system for counting microalgae through automatic image detection using the fast and accurate YOLO (You Only Look Once) algorithm. The system involves training datasets, generating data weights for processing within programs developed in Microsoft Visual Studio, and evaluating the program's accuracy in detecting algae compared to expert analyses. The model achieved an impressive 93.54\% accuracy in detecting all 11 types of algae, ensuring reliable results. Additionally, the system reduced the counting time by 94.44\%, with an average image processing time of approximately 0.6 seconds, a significant improvement compared to the manual visual inspection time of 10.8 seconds per grid. The system's high accuracy demonstrates its effectiveness, making it a reliable tool for water quality management.},
  booktitle = {Proceedings of the 2024 8th International Conference on Computational Biology and Bioinformatics},
  pages = {157–162},
  numpages = {6},
  keywords = {Artificial Intelligence, Image Processing, Microalgae Counting, Microalgae Detection, YOLO},
  location = {},
}

@inproceedings{10.1145/3745238.3745412,
  author = {Shen, Qili and Liu, Guocong and Hou, Bolin and Qiang, Na and Yang, Xiaolong and Xu, Runfeng and Liu, Zhu and Fang, Yanxiong and Huang, Xin and Lu, Zhe},
  title = {Development of organic pressure-sensitive adhesive under the background of artificial intelligence},
  year = {2025},
  isbn = {9798400712791},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3745238.3745412},
  doi = {10.1145/3745238.3745412},
  abstract = {Silicone pressure-sensitive adhesives (SiPSAs) have demonstrated broad application potential in electronics and other fields due to their exceptional properties, such as temperature resistance, weather resistance, and chemical corrosion resistance. However, traditional SiPSAs are predominantly thermally cured solvent-based products, which not only exhibit slow curing rates but also release harmful solvents like toluene during application, causing environmental pollution. To address this issue, this study Under the background of artificial intelligence. synthesized thiol-functionalized MTQ silicone resins (MTQ-SH) with various M/(T+Q) ratios using γ-mercaptopropyltrimethoxysilane, tetraethyl orthosilicate, and hexamethyldisiloxane as raw materials. These resins were then photo-crosslinked with vinyl-terminated silicone oil to prepare UV-curable silicone PSAs (UV-SiPSAs). The results revealed that the optimal precursor ratio for UV-SiPSA preparation was M/T/Q = 1.00/0.21/1.58. When the photoinitiator TPO-L was added at 1 wt.\% and the molar ratio of thiol groups to vinyl end groups was 1.2:1, the resulting UV-SiPSA exhibited an initial adhesion strength capable of holding a 22# steel ball and a holding time of 230 minutes at 25℃. This UV-curable PSA shows promising potential for replacing traditional thermally cured silicone PSAs in the protective film industry.},
  booktitle = {Proceedings of the 2nd Guangdong-Hong Kong-Macao Greater Bay Area International Conference on Digital Economy and Artificial Intelligence},
  pages = {1111–1118},
  numpages = {8},
  keywords = {Artificial Intelligence Silicone pressure-sensitive adhesive, Silicone resin, Thiol-ene click chemistry, UV-curing},
  location = {},
}

@inproceedings{10.1145/3723366.3723384,
  author = {Sung, Jinghui and Jiang, Zikang and Chang, Yun-Chen and Huang, Yuanpeng and Chen, Ting-Yu},
  title = {Diagnostic Testing Analysis of Digital Stethoscopes Based on Artificial Intelligence and Noise Reduction Technology},
  year = {2025},
  isbn = {9798400718298},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3723366.3723384},
  doi = {10.1145/3723366.3723384},
  abstract = {This study evaluates the consistency between an AI system and traditional doctor diagnoses in detecting heart sound abnormalities, and explores the impact of noise reduction technology on AI performance in noisy environments. Methods: Stage 1: AI and doctor diagnoses were compared in detecting single and multiple heart sound abnormalities using a deep neural network (DNN) model. Performance was measured by Cohen's Kappa, sensitivity, specificity, and F1 score. Stage 2: Noise reduction technology was applied to assess its effect on AI performance in high-noise environments, using frequency-domain filtering, CNNs, and metrics like the Noise Impact Index (NII) and Noise Reduction Rate (NRR). Results: In Stage 1, the AI showed high consistency with doctors for single abnormalities (Cohen's Kappa = 0.81), but underperformed for multiple abnormalities. It achieved 96\% sensitivity, 93\% specificity, and an F1 score of 0.89, though performance was affected by noise. In Stage 2, noise reduction significantly improved AI accuracy, particularly in detecting multiple abnormalities, where AI outperformed doctors. Conclusion: The AI system demonstrated strong agreement with doctors, especially in detecting single abnormalities. Noise reduction technology enhanced its diagnostic accuracy and stability, particularly in complex environments. Further optimization is needed for more comprehensive performance in diverse cases.},
  booktitle = {Proceedings of the 2024 4th International Symposium on Big Data and Artificial Intelligence},
  pages = {113–120},
  numpages = {8},
  keywords = {Artificial Intelligence, Big Data, Computer, Diagnostic Consistency, Heart Sound Abnormality Detection, Methods and Models, Noise Reduction},
  location = {},
}

@inproceedings{10.1145/3709026.3709101,
  author = {He, Haofan and Duan, Wei and Wang, Haihang},
  title = {Diffusion Model-Based Adaptive Generation of Adversarial Perturbations against Deepfake},
  year = {2025},
  isbn = {9798400718182},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3709026.3709101},
  doi = {10.1145/3709026.3709101},
  abstract = {Deepfakes have posed significant risks to personal privacy and reputation. At present, active defense measures often introduce imperceptible adversarial perturbations into the protected images, aiming to disrupt the normal forgery of deepfake models. Among them, generative perturbations are favored because of their higher efficiency. However, the existing generative perturbations are mainly based on the Lp-norm method, which still has the problem of high perception and difficulty in achieving personalized strength. To address this shortcoming, we propose an adaptive deepfake adversarial perturbation generation method based on a diffusion model. Initially, our approach constructs an adversarial denoising framework that utilizes the reverse process of diffusion model to learn unrestricted perturbations. Then an adversarial guided mechanism is introduced during the sampling stage, enabling the adaptive control of the adversarial perturbation through sampling step and guidance strength. Extensive experiments show that our method achieves effective defense effects on various typical deepfake models and has good adaptive generation capabilities.},
  booktitle = {Proceedings of the 2024 8th International Conference on Computer Science and Artificial Intelligence},
  pages = {147–152},
  numpages = {6},
  keywords = {Deepfake, adversarial attack, diffusion model, image reconstruction},
  location = {},
}

@inproceedings{10.1145/3756423.3756444,
  author = {Zhang, Yalong and Xiong, Wu and Xiong, Shuigen and Ao, Lihua and Zhan, Fuyuan and Yang, Yuejun},
  title = {Digital Intelligence Monitor and Intelligent Diagnosis Technology for Health Condition of Turbine EH Oil System},
  year = {2025},
  isbn = {9798400714351},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3756423.3756444},
  doi = {10.1145/3756423.3756444},
  abstract = {With the deepening of industrial automation and intelligence, the health condition of steam turbines, as the key equipment of the power system, is particularly important. As a key component of the steam turbine, the health condition of the EH oil system plays a decisive role in the safety and stability of the entire power system. The working principle and common problems of EH oil system is reviewed, the root causes of common failures of EH oil system are analyzed, and the development and application of condition monitoring and fault diagnosis technology in power system are discussed, aiming at exploring the use of artificial intelligence and other technologies to monitor the condition and fault diagnosis of EH oil system. The design and application of digital intelligent monitoring and intelligent diagnosis technology in EH oil system is proposed. Through the analysis, it is concluded that: In the future, a large number of fault samples will be provided through digital and intelligent monitoring, intelligent diagnosis technology will be integrated, and intelligent technologies such as artificial intelligence, machine learning, and genetic algorithm will be used to carry out fault warning, fault diagnosis and health condition evaluation of the EH oil system, so as to improve the operational stability and safety of the EH oil system.},
  booktitle = {Proceedings of the 2025 International Conference on Artificial Intelligence and Smart Manufacturing},
  pages = {133–137},
  numpages = {5},
  keywords = {EH oil system, fault diagnosis, health condition monitoring, steam turbine},
  location = {},
}

