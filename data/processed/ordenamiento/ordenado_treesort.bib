@inproceedings{10.1145/3652628.3652765,
  author = {Zhang, Xue and Qi, Yue},
  title = {Application of Cold Chain Intelligent Sorting Prediction System Based on Artificial Intelligence Shapley Values},
  year = {2024},
  isbn = {9798400708831},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3652628.3652765},
  doi = {10.1145/3652628.3652765},
  abstract = {In the process of using cold chain detection data for information mining and analysis, different data features will have different contributions to the prediction results. Based on the different contributions of these data features, this paper proposes a data feature contribution allocation method based on Shapley value, which is applied to the feature value analysis of the cold chain intelligent sorting and prediction system. This method is based on the Shapley value method, which is one of the important contents of cooperative games. It treats each feature as an alliance party, and regards prediction gains as benefits. It explores the contribution of data features, providing theoretical support for identifying important features that affect the results of prediction algorithms and explaining the prediction results of specific instances. By analyzing the contribution of input data features of Japanese artificial intelligence (AI) "fish face" recognition intelligent sorting projects, it is beneficial to optimize intelligent prediction sorting models and provide data sources for subsequent transportation of goods and scheduling resources.},
