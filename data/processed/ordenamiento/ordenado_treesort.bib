@inproceedings{10.1145/3613905.3650732,
  author = {Yan, Lixiang and Echeverria, Vanessa and Fernandez-Nieto, Gloria Milena and Jin, Yueqiao and Swiecki, Zachari and Zhao, Linxuan and Ga\v{s}evi\'{c},
  title = {Human-AI Collaboration in Thematic Analysis using ChatGPT: A User Study and Design Recommendations},
  year = {2024},
