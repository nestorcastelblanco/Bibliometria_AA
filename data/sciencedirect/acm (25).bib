@inproceedings{10.1145/3745238.3745525,
author = {An, Hui and Guo, Zhenpeng and Shi, Yunxia},
title = {Can generative AI features increase users' continued use intention?: The mediating role of user perceived value and user satisfaction},
year = {2025},
isbn = {9798400712791},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3745238.3745525},
doi = {10.1145/3745238.3745525},
abstract = {The emergence of generative AI has become a hot topic in academia and industry. Generative AI is also gradually becoming an interaction mode between enterprises and customers, improving customer experience and influencing their continued use intention. However, there is still uncertainty as to whether users are willing to use generative AI-driven products and services for a long time. Previous studies have mostly been based on traditional industries. How to enhance users’ continued use intention from the perspective of AI has become a widely concerned issue in academia and industry.Based on the Technology Acceptance Model (TAM) and Expectation Confirmation Theory (ECT), this study explores the impact of generative AI features on users’ continued use intention by improving the two paths of user perceived value and user satisfaction. 306 valid questionnaires were used, and SPSS 27.0 and SEM were used for data analysis and hypothesis testing. The results show that (1) generative AI features have a significant positive impact on users’ continued use intention; (2) user perceived value and user satisfaction play a mediating role in the relationship between generative AI features and users’ continued use intention. This finding improves users’ continued use intention of generative AI features, which is of great significance for understanding the marketing layout of generative AI for enterprises.},
booktitle = {Proceedings of the 2nd Guangdong-Hong Kong-Macao Greater Bay Area International Conference on Digital Economy and Artificial Intelligence},
pages = {1838–1845},
numpages = {8},
keywords = {Generative AI features, User Continued use intention, User Perceived value, User satisfaction},
location = {
},
series = {DEAI '25}
}

@article{10.1145/3719209,
author = {Storey, Veda Catherine},
title = {Knowledge Management in a World of Generative AI: Impact and Implications},
year = {2025},
issue_date = {September 2025},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
volume = {16},
number = {3},
issn = {2158-656X},
url = {https://doi.org/10.1145/3719209},
doi = {10.1145/3719209},
abstract = {The management of organizational and worker knowledge is a critical part of any organization. Knowledge management recognizes the explicit knowledge that workers contribute to an organization, as well as the organizational knowledge and skills acquired over time. The potential impact of generative AI (GenAI) raises many issues and opportunities related to effective knowledge management as organizations explore the impact and implications of its adoption. This research explores issues related to an increasing world of GenAI and proposes a Framework for Knowledge Management in the GenAI Era. Knowledge management workers, who are novices, might be more inclined to use generative AI for performing their tasks. Expert knowledge workers might focus on quality evaluation and quality assessment. Adoption of GenAI may help with the progression from novice to expert, while freeing up time and resources for other work. Challenges include the potential loss of critical organizational knowledge, definition of job-related requirements, expectations, and tasks, as well as over-reliance and loss of tacit or unarticulated knowledge. These challenges must be balanced against available access to large amounts of general and domain-specific knowledge from GenAI to capture the value realized from its adoption.},
journal = {ACM Trans. Manage. Inf. Syst.},
month = sep,
articleno = {26},
numpages = {14},
keywords = {Knowledge management, generative AI (GenAI), novice knowledge workers, expert knowledge workers, Framework for Knowledge Management in the GenAI Era, risk assessment, organizational knowledge, Knowledge shifting}
}

@inproceedings{10.1145/3762329.3762342,
author = {Wang, Yi and Zhang, Zhenyu},
title = {Feasibility Research on Application of Artificial Intelligence Technology in Enterprises Based on Deep Learning},
year = {2025},
isbn = {9798400718625},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3762329.3762342},
doi = {10.1145/3762329.3762342},
abstract = {The industrial application of artificial intelligence technology is profoundly changing the competitive pattern of enterprises. However, small and medium-sized enterprises generally face bottlenecks such as high investment threshold, lack of talents and difficult scene adaptation in the process of technology landing. This study focuses on the feasibility discrimination problem of enterprise application of artificial intelligence technology, and proposes a deep learning framework based on LSTM-Transformer hybrid model. By integrating the three types of time series characteristics of enterprise basic information, business dynamics and technology application data, a multi-dimensional evaluation system is constructed. The model adopts layered architecture: LSTM layer captures short-term business fluctuations, Transformer layer mines long-term correlation between technical input and business effectiveness, and finally realizes two-classification of "feasible" and "temporarily infeasible" through Softmax. Based on the six-year data of 2006 enterprises, the mixed model accuracy reached 89.06\% (±0.31\%) and F1 score was 87.88\% (±0.36\%), which was significantly better than the single model and CNN-LSTM baseline. Confusion matrix shows that 93.7\% of high recall guarantees the identification of potential enterprises, and 74.4\% of specificity reveals negative class discrimination optimization space. This study provides a data-driven decision-making tool for SMEs to avoid technology investment risks.},
booktitle = {Proceedings of the 2nd International Conference on Artificial Intelligence of Things and Computing},
pages = {65–70},
numpages = {6},
keywords = {Artificial Intelligence Technology, Deep Learning, Feasibility Research, LSTM-Transformer},
location = {
},
series = {AITC '25}
}

@inproceedings{10.1145/3719384.3719439,
author = {Chang, Chi In and Choi, Wan Chong and Choi, Iek Chong},
title = {Challenges and Limitations of Using Artificial Intelligence Generated Content (AIGC) with ChatGPT in Programming Curriculum: A Systematic Literature Review},
year = {2025},
isbn = {9798400717925},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3719384.3719439},
doi = {10.1145/3719384.3719439},
abstract = {This systematic literature review examined the challenges and limitations of integrating Artificial Intelligence Generated Content (AIGC) tools into programming curricula. Following Kitchenham's framework, a comprehensive search was conducted across ACM Digital Library and Web of Science databases to select 22 relevant peer-reviewed papers. Key issues identified included increased plagiarism risk due to AI-generated unique code, accuracy and reliability concerns of AI outputs, privacy and data security risks, curriculum changes to incorporate prompt engineering and critical thinking, and challenges in traditional assessment methods. Additionally, the review highlighted the potential over-reliance on AI tools, which could hinder the development of fundamental programming skills. Implementation challenges and inadequate training for educators further complicate the integration process. Addressing these challenges is crucial for leveraging the benefits of AIGC tools while maintaining academic integrity and enhancing learning outcomes in programming education. Future research should focus on developing strategies to mitigate these issues and ensure the responsible use of AI in education.},
booktitle = {Proceedings of the 2024 7th Artificial Intelligence and Cloud Computing Conference},
pages = {372–378},
numpages = {7},
keywords = {Artificial intelligence generated content, Challenges of AIGC, ChatGPT, Large language models, Programming curriculum, Systematic literature review},
location = {
},
series = {AICCC '24}
}

@inproceedings{10.1145/3745676.3745719,
author = {Shi, Boli and Nie, Zhaoying and Chen, Jing and Wang, Zhen},
title = {Research on the Network Characteristics and Cooperation Models of Artificial Intelligence Group Standards in China—Based on the Analysis of the Artificial Intelligence Industry},
year = {2025},
isbn = {9798400715150},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3745676.3745719},
doi = {10.1145/3745676.3745719},
abstract = {This paper takes the group standards of China's artificial intelligence-related industries as the research object and analyzes the characteristics of participating drafting units and group standard organizations in the group standard cooperation network based on the structural characteristics such as centrality and network density obtained from social network analysis. The study finds that although the development time of group standards in China's artificial intelligence-related industries is short, the development is rapid, and the cooperation network has basically taken shape. The central positions of some group standard organizations and participating drafting units have been initially formed. However, it can also be seen that some group standard organizations, despite having a close relationship with participating drafting units, have not yet established connections with the overall network, indicating that the overall cooperation network is still loose, and only some group standard organizations play relatively key roles in constructing the network for the formulation of group standards in the artificial intelligence-related industries. In the process of promoting the maturity of the cooperation network of group standards in artificial intelligence-related industries, there are still many challenges and room for optimization. On the one hand, it is urgent to take effective measures to strengthen the connections among all entities within the network, reduce the looseness of the network, and improve the overall collaborative efficiency, such as establishing an information sharing platform. On the other hand, group standard organizations and participating drafting units in central positions should give full play to their leading and exemplary roles, and drive the common development of other entities within the network through means such as technical assistance and experience sharing. In addition, the government and industry associations should also play an active role by introducing targeted policies and regulations to guide the formulation and implementation of group standards for artificial intelligence, urging all parties involved to strengthen cooperation and build a more complete standard system. Looking ahead, with the continuous deepening of collaborative governance by all parties, the cooperation network of group standards in the artificial intelligence industry will continue to be optimized and upgraded. It will not only promote the high-quality development of China's artificial intelligence industry but also demonstrate China's wisdom in the field of global artificial intelligence standard setting and enhance China's international discourse power and influence in this field.},
booktitle = {Proceedings of the 2025 2nd International Conference on Innovation Management and Information System},
pages = {296–302},
numpages = {7},
keywords = {Artificial Intelligence, Cooperation Model, Group Standards, Network Characteristics, Social Network Analysis},
location = {
},
series = {ICIIS '25}
}

@inproceedings{10.1145/3708394.3708439,
