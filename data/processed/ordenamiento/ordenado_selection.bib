 numpages = {5},
 pages = {206–210},
 publisher = {Association for Computing Machinery},
 series = {GAIIS '24},
 title = {Exploring Robustness under New Adversarial Threats: A Comprehensive Analysis of Deep Neural Network Defenses},
 url = {https://doi.org/10.1145/3665348.3665384},
 year = {2024}
}

@inproceedings{10.1145/3665348.3665385,
 abstract = {For the increasingly complex network environment, new types of attacks are emerging, and the limited attack samples may not be sufficient for traditional detection methods to achieve good detection results. In this regard, this paper proposes a few-shot detection method based on meta-learning, which mainly consists of three parts: data preprocessing, feature extraction and comparison, and classification and detection. The original data samples are transformed into grey scale graphs, and after combining the samples two by two, they are respectively subjected to feature extraction and comparison, which results in similarity scores, which are finally used to carry out classification. It is shown through experiments that the accuracy of detection can reach up to 98.45\% in the case of few-shot.},
 address = {New York, NY, USA},
 author = {Yang, Lang},
 booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
 doi = {10.1145/3665348.3665385},
