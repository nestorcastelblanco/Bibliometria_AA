@inproceedings{10.1145/3711403.3711407,
 abstract = {Leading the innovative development of higher education by digitization of higher education and exploring a road of education digitization development with Chinese characteristics is a necessary way for our country to realize the construction of a powerful country in education. This study builds online learning environment with the help of generative artificial intelligence tools and platforms, relying on the public course “Modern Educational Technology” for normal university students and integrating ideological and political education into the curriculum, guided by human-machine co-creation projects, independent learning activities, teacher-student interaction activities, and student-student interaction activities are designed to improve the information-based instructional design and information-based resource development capabilities of normal university students and promote deep learning. The results of the study show that online interactive activities for deep learning can improve students' deep learning ability, digital literacy and enhance the effect of online interaction. At the same time, this study also proposes relevant practical measures in order to provide a reference for online teaching in the era of generative artificial intelligence.},
