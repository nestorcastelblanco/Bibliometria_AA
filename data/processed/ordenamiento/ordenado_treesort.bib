@inproceedings{10.1145/3647444.3647836,
  author = {Sharma, Mansi and Gupta, Amit Kumar},
  title = {A Comprehensive Review of Artificial Intelligence Applications in the Fight against COVID19},
  year = {2024},
  isbn = {9798400709418},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3647444.3647836},
  doi = {10.1145/3647444.3647836},
  abstract = {In recent decades, artificial intelligence (AI) has arisen as a crucial and influential factor in the fields of healthcare and medicine, with its origins tracing back to the 1970s. Unlike its initial rule-based predecessors, contemporary AI utilizes machine learning algorithms to detect data patterns. Given the extensive data available on COVID-19 patients, this technology offers valuable insights into understanding viral transmission patterns, devising novel treatment modalities, and even determining the populations with higher susceptibility with a basis in their hereditary and physiological characteristics. This paper aims to review and delve into the broad applications of machine learning and deep learning across various domains, inclusive of medical imaging, protein structure analysis, health monitoring, virus severity forecasting, and drug development, with a particular emphasis on their relevance within the sphere of COVID-19. Furthermore, it explores future research avenues that could shed light on combatting future pandemics similar to the current one.},
