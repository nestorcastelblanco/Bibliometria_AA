@inproceedings{10.1145/3652628.3652761,
  author = {Wang, Siwen and Lin, Zeqiang},
  title = {Meta-learning in Artificial Intelligence Lyric Composition},
  year = {2024},
  isbn = {9798400708831},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3652628.3652761},
  doi = {10.1145/3652628.3652761},
  abstract = {This paper explores the application of meta-learning in AI music composition. In the age of digital music creation, AI composition has emerged as a prominent field of interest, though it often necessitates extensive data and time for model training. The introduction of meta-learning techniques provides the potential to expedite AI composition. This research aims to analyze how meta-learning methods can expedite AI's acquisition of diverse music styles and emotions, consequently enhancing the generation of high-quality English song lyrics. We utilized an English song lyrics dataset, applied meta-learning algorithms, and presented experimental findings. The results of this study indicate that meta-learning holds substantial potential in AI composition, significantly augmenting the efficiency and diversity of music generation. This bears critical implications for the future advancement of music creators and AI music generation tools.},
  booktitle = {Proceedings of the 4th International Conference on Artificial Intelligence and Computer Engineering},
  pages = {800–804},
  numpages = {5},
  location = {Dalian, China},
}

