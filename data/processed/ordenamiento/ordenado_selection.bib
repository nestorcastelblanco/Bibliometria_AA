@inproceedings{10.1145/3709022.3736542,
 abstract = {Fingerprint biometrics are extensively used for identification and security, from border control to consumer electronics. The permanence of fingerprints means security breaches can have lasting impacts. Traditionally, organizations store only fingerprint templates to mitigate the risks associated with stolen fingerprint images. However, advances in generative artificial intelligence (GenAI), particularly deepfake technologies, now enable adversaries to generate fingerprints from stolen templates, increasing the threat of data breaches.In this paper, we demonstrate and validate a previously theorized threat by evaluating an end-to-end attack in the physical world. Our approach involves: (1) generating fingerprint images from unseen templates, (2) fabricating silicone replicas of these deepfake fingerprints using a 3D resin printer, and (3) successfully deceiving fingerprint scanners with the replicas. The entire lab setup cost only $440 USD and only 7 cents to replicate each fingerprint thereafter, highlighting the attack's practicality.To support reproducibility and encourage the development of defenses, we publicly release our deepfake fingerprint pipeline.},
 address = {New York, NY, USA},
 author = {Hacmon, Yaniv and Gorelik, Keren and Mirsky, Yisroel},
 booktitle = {Proceedings of the 4th Workshop on Security Implications of Deepfakes and Cheapfakes},
 doi = {10.1145/3709022.3736542},
 isbn = {9798400714191},
 keywords = {Biometrics, Deepfake, Fingerprint Spoofing, Generative AI},
 location = {Hanoi, Vietnam},
 numpages = {8},
 pages = {1–8},
 publisher = {Association for Computing Machinery},
 series = {WDC '25},
 title = {The Threat of Deepfake Fingerprints},
 url = {https://doi.org/10.1145/3709022.3736542},
 year = {2025}
}

@inproceedings{10.1145/3709022.3736543,
 abstract = {The rise of deepfake technologies has introduced powerful tools for synthetic face generation, which is often associated with manipulation and privacy violations. However, recently, these generative techniques are repurposed for privacy-preserving applications. This paper presents a structured review of facial anonymization methods that leverage generative models to conceal identity while preserving semantic features, such as expressions and poses. We analyze seven representative studies, comparing their model architectures, alignment with deepfake transformation types, evaluation metrics, and application scenarios. Our review identifies a range of promising technical strategies with particular attention to how recent approaches address demographic fairness, face and body anonymization, and real-world deployment challenges. We underscore the importance of ethically sourced data and policy-aware designs as prerequisites for responsible use. By positioning facial de-identification not only as a technical challenge but also as a sociotechnical necessity, this review outlines a research agenda centered on the privacy, utility, and equitable adoption of generative artificial intelligence systems.},
 address = {New York, NY, USA},
 author = {Park, Sunyoung and Kim, Hyunji and Choi, Seul-Ki and Kim, Taeeun and Park, Eunil},
 booktitle = {Proceedings of the 4th Workshop on Security Implications of Deepfakes and Cheapfakes},
 doi = {10.1145/3709022.3736543},
 isbn = {9798400714191},
 keywords = {DeepFake, DeepPrivacy, Face-identification, Privacy, Survey},
 location = {Hanoi, Vietnam},
 numpages = {6},
 pages = {27–32},
 publisher = {Association for Computing Machinery},
 series = {WDC '25},
 title = {Privacy-Driven Faces: A Survey on Generative Facial De-identification},
 url = {https://doi.org/10.1145/3709022.3736543},
 year = {2025}
}

@inproceedings{10.1145/3709026.3709097,
 abstract = {The proliferation of Artificial Intelligence (AI), academic landscapes in research and learning are shifting from traditional to innovative strategies, greatly impacting students’ learning and teachers’ pedagogies in High Education (HE). There is an increasing number of universities worldwide engaging in AI exploration, aiming to adopt it as a lifelong learning objective. This study investigates the progression of AI literacy in engineering domain for both students and researchers in Higher Education. Some innovative teaching activities, such as AI working workshops and seminars, are implemented to enhance the researchers’ and students’ AI literacy. The 'conversational framework' serves as the theoretical foundation for the teaching methodologies and engineering pedagogy applied in this research. A novel BERT-based system is introduced and utilized within a real teaching context. The experimental results and collected teaching data indicate that the BERT-based system can enhance students’ and researchers’ learning to a certain extent.},
