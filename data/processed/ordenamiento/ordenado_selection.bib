 issn = {2157-6904},
 issue_date = {December 2024},
 journal = {ACM Trans. Intell. Syst. Technol.},
 keywords = {Abstractive summarization, Opinion focused-document, Intended message, Explainable artificial intelligence, Random masking, Machine learning, Artificial intelligence},
 month = {December},
 number = {6},
 numpages = {20},
 publisher = {Association for Computing Machinery},
 title = {User Opinion-Focused Abstractive Summarization Using Explainable Artificial Intelligence},
 url = {https://doi.org/10.1145/3696456},
 volume = {15},
 year = {2024}
}

