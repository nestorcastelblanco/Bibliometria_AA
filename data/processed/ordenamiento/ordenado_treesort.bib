@inproceedings{10.1145/3697355.3697418,
  author = {Xu, Jian and Sun, Liming},
  title = {Research and Analysis of Artificial Intelligence Algorithms for Solving Graph Coloring Problems},
  year = {2024},
  isbn = {9798400717529},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3697355.3697418},
  doi = {10.1145/3697355.3697418},
  abstract = {The graph coloring problem is a classic NP-hard problem in the field of computer science, with significant applications in various practical domains such as timetabling, frequency allocation, and register allocation. As the scale of data increases and the complexity of problems grows, traditional algorithms often struggle to find optimal solutions within a reasonable timeframe. This study primarily explores the effectiveness and performance of two artificial intelligence algorithms—Backtracking and the Welch-Powell algorithm—in addressing the graph coloring problem. Through experimental analysis and comparison, we aim to evaluate the adaptability and efficiency of these two methods across different types and scales of graph structures. The experimental results indicate that the Backtracking algorithm exhibits faster solution times and produces higher quality solutions compared to the Welch-Powell algorithm in solving the graph coloring problem.},
  booktitle = {Proceedings of the 2024 8th International Conference on Big Data and Internet of Things},
  pages = {382–386},
  numpages = {5},
  keywords = {Artificial Intelligence, Graph Coloring Problem, Graph Theory, Welch-Powell Algorithm},
  location = {},
}

@inproceedings{10.1145/3665348.3665411,
  author = {Li, Hong},
  title = {Research and application of network analysis algorithms based on underground pipeline networks},
  year = {2024},
  isbn = {9798400709562},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3665348.3665411},
  doi = {10.1145/3665348.3665411},
  abstract = {With the deepening application of underground pipeline GIS system, various graphical data are becoming increasingly rich and accurate. The demand for network analysis functions such as path analysis, resource allocation, and connectivity analysis is rapidly increasing both domestically and internationally. Various network analysis algorithms based on GIS urgently need industrial level applications. We have established a three-dimensional network model of water supply network based on GIS technology, and created a spatial database to store graphic data and study the effectiveness of network analysis algorithms through underground pipeline network business data. We have designed optimal algorithmic solutions for various complex underground pipeline problems, such as burst pipe problems, shortest path problems, and maximum flow problems in the water supply network. This solution can be applied to various pipeline networks and it has strong application value and economic value.},
  booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
  pages = {370–374},
  numpages = {5},
  location = {Kuala Lumpur, Malaysia},
}

@inproceedings{10.1145/3665348.3665410,
  author = {Li, Hong},
  title = {Research and application of neural network algorithm based on Water supply and drainage pipeline network},
  year = {2024},
  isbn = {9798400709562},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3665348.3665410},
  doi = {10.1145/3665348.3665410},
  abstract = {The explosion/leakage problem of the water supply and drainage pipeline network is the main hidden danger problem in the current domestic water supply and drainage system. GIS technology for equipment management and algorithm research in the water supply and drainage pipeline network to solve this problem. This article provides an evaluation method for the leakage problem of underground pipeline networks based on BP neural network algorithm and genetic algorithm, which is based on historical accident type data, time data, and regional feature data of accident points. The data is then processed and transformed into neural network model data, which is then trained and verified by the neural network model. The results have shown that this method can not only make predictions before water pipe leakage or even explosion accidents occur, but also provide accurate and effective guidance for daily pipeline inspections, and improve work efficiency. This solution can be applied to various pipeline networks and has strong application and economic value in avoiding water resource loss.},
  booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
