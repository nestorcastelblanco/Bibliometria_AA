 doi = {10.1145/3699538.3699567},
 isbn = {9798400710384},
 keywords = {Generative Artificial Intelligence, Human-AI Interaction, Self-regulation, CS1, User study, Novice programmers},
 location = {},
 numpages = {13},
 publisher = {Association for Computing Machinery},
 series = {Koli Calling '24},
 title = {Student-AI Interaction: A Case Study of CS1 students},
 url = {https://doi.org/10.1145/3699538.3699567},
 year = {2024}
}

@inproceedings{10.1145/3700035.3700043,
 abstract = {As the uncertainty and complexity of financial markets increase, traditional financial forecasting methods are increasingly revealing their limitations. This paper explores the design and implementation of a financial forecasting system based on artificial intelligence (AI) and blockchain technology, aiming to improve the accuracy and transparency of financial forecasts. By optimizing AI models and integrating blockchain technology, an innovative forecasting system is constructed, making the processing and storage of financial data more efficient and secure. Experimental results show that this system significantly outperforms traditional methods in terms of forecasting accuracy and data security, and it demonstrates great potential in practical applications. The paper also analyzes the typical application scenarios of the system in the stock market and foreign exchange trading and discusses the challenges and future development directions in this field.},
