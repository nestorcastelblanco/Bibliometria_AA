pages = {4176–4179},
numpages = {4},
keywords = {generative ai, human-computer interaction, interaction paradigms, retrieval-augmented generation, {information retrieval},
location = {Padua, Italy},
series = {SIGIR '25}
}

@article{10.1145/3685680,
