@inproceedings{10.1145/3712623.3712664,
author = {Liu, Liping and Zhang, Jie and Yang, Jiayi and Cai, Yucao},
title = {Research on the Application of Artificial Intelligence in Scenic Mobility Scooter Context-Aware Design Methodology},
year = {2025},
