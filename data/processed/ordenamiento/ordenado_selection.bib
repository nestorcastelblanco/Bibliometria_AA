@inproceedings{10.1145/3665348.3665417,
 abstract = {In addressing the limited receptive field of single-layer convolution in EEG emotion recognition, the need to stack multiple layers of convolution for expanding the receptive field poses challenges of increased parameter size and training cost. Simultaneously, there is an oversight of channel information in EEG emotion recognition. To tackle these issues, this paper introduces a multi-feature EEG emotion recognition model based on 1D-Inception (1D-Inception-Channel-Spatial-LSTM Network, DCSL). DCSL integrates channel information, spatial features, and temporal dependencies, resulting in more effective EEG emotion recognition. The Inception structure is enhanced to fit EEG signals, enabling multi-scale convolution. The Channel Attention Module is introduced to learn the weights of different channels, enhancing emotion recognition performance. Additionally, a Spatial Attention mechanism is incorporated to further extract spatial features from EEG signals. To better capture the temporal dependencies in EEG signals, an LSTM module is introduced at the end of the model. Experimental results confirm the effectiveness of the DCSL network in EEG emotion recognition tasks.},
 address = {New York, NY, USA},
 author = {Qi, Ji and Li, Xiujun and Zhang, Hang and Meng, Tianyu},
 booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
