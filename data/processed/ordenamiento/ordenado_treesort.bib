@inbook{10.1145/3745238.3745370,
  author = {Wu, Renjun and Chen, Yuexiang and Hou, Bolin and Wei, Guojun and Wang, Jing and Lv, Zhe and Hu, Yinfeng and Wei, Yi and Zhang, Fangyang},
  title = {Design of an automation modification of a TV backplane traversing module in the context of artificial intelligence},
  year = {2025},
  isbn = {9798400712791},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3745238.3745370},
  abstract = {This study deals with the automation of TV backplane transversal modules and proposes an innovative automation solution by integrating digital economy and artificial intelligence (AI) technologies. By analysing the limitations of existing manually operated systems, this study designs an automation system that incorporates AI algorithms to achieve an intelligent and data-driven production process. The study covers automation system design, key component selection, intelligent control system development and performance optimisation. Through theoretical analysis and experimental validation, the modified automation system significantly improves productivity and product quality, while reducing labour costs and operational risks. In addition, this study explores the role of automation transformation in promoting the digital economy, providing new insights into the intelligent transformation of the home appliance manufacturing industry. The research results provide a theoretical basis and practical guidance for the automation upgrading of TV backplane transversal modules, which is of great significance for improving the intelligent level of the home appliance manufacturing industry and promoting the development of the digital economy.},
