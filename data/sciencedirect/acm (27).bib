abstract = {The research explores the applications of artificial intelligence (AI) applications in the education. The role of AI as mobile learning technology, blended learning technology and personalized learning technology has been studied in this paper. These learning techniques with the integration of AI technology enhance prediction of usefulness of teaching methods and increase the interaction between students and their teachers. This paper identifies some challenges and limitation of using AI in education. Little access to internet, and resource allocation for diverse requirements of a learning management system are identified as key problems of AI based educational systems. This study emphasizes the improvement of teachers' digital literacy, including the cultivation of AI integration ability, data analysis ability and innovative thinking, so as to meet the challenges brought by rapid technology iteration, such as the difficulty of technology integration, the need for updating teaching concepts and the differentiated management of students.},
booktitle = {Proceedings of the 2024 2nd International Conference on Information Education and Artificial Intelligence},
pages = {814–820},
numpages = {7}
