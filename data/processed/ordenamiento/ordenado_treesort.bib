@inproceedings{10.1145/3733155.3737910,
  author = {Leporini, Barbara and Buzzi, Marina and Della Penna, Giuseppe},
  title = {A Preliminary Evaluation of Generative AI Tools for Blind Users: Usability and Screen Reader Interaction},
  year = {2025},
  isbn = {9798400714023},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3733155.3737910},
  doi = {10.1145/3733155.3737910},
  abstract = {The increasing use of Generative Artificial Intelligence (GAI) tools such as ChatGPT, Copilot, Perplexity and Gemini opens up new possible scenarios for supporting work and everyday activities. For people who are blind, the usability of such tools through screen readers is crucial to ensure their use of such AI-based technologies. In this study, we explore the accessibility and usability of the interfaces of four popular AI-based tools via screen readers through a combination of semi-automated evaluations and inspections conducted by both sighted and blind accessibility experts and screen readers with more than 20 years of experience. Navigation, labeling of control elements, feedback mechanisms, and prompt handling were considered in the study. The results point to usability difficulties in all tools, particularly in navigation structure, clarity of feedback and interactive elements. Although this work empirically explores the accessibility of AI-based tools it brings out the first critical issues that deserve further investigation. However, they are based on a small group of experts and thus should be considered preliminary and useful for future studies.},
  booktitle = {Proceedings of the 18th ACM International Conference on PErvasive Technologies Related to Assistive Environments},
  pages = {562–568},
  numpages = {7},
  keywords = {Accessibility, Blind users, ChatGPT, Copilot, Gemini, Generative AI, Perplexity, screen reader interaction},
  location = {},
}

@inproceedings{10.1145/3718391.3727884,
  author = {Jun, Guo and Ting, Guo Yan and Song, Zheng Yan},
  title = {A question-and-answer system based on artificial intelligence},
  year = {2025},
  isbn = {9798400717376},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3718391.3727884},
  doi = {10.1145/3718391.3727884},
  abstract = {This study is the research outcome of the project "Exploration of Student-Centered Teaching Reform and AI Technology Application in Dynamic Website Design Courses" led by the author at ZhuJiang College, South China Agricultural University. This study investigates the development and application of an intelligent question-and-answer (Q&amp;A) system tailored for computer science course instruction, using the BERT model as its core framework. Traditional teaching methods face limitations in providing timely, personalized responses to students' inquiries, especially in large classroom settings. To address this, we propose a Q&amp;A system based on BERT's bidirectional training capabilities, enabling deeper contextual understanding and precise response generation. The research introduces specific advancements in system structure, model selection, dataset creation, and training processes to address the unique needs of computer science education. The methodology encompasses dataset construction, using a collection of 2,000 carefully curated question-answer pairs spanning essential topics like operating systems, programming languages, and databases. We refined the BERT model with this dataset through fine-tuning, employing dynamic masking and the Adam optimizer with a decay learning rate to achieve optimal generalization. Furthermore, we developed a user feedback mechanism, where responses are evaluated for accuracy, enabling continuous model refinement and adaptation to student feedback. This study designed an MVC-based system integrating Java, TorchServe, and MySQL. Experiments show that it outperforms traditional methods in performance and efficiency, achieving an F1 score of 91.1\% with a response time of just 0.8 seconds. Error analysis indicates room for improvement in handling complex terminology and contextual understanding. This work's innovations contribute a robust, scalable tool for enhancing computer science education, offering educators a powerful solution to improve teaching interactivity, efficiency, and personalized learning support. The insights gained from this study support broader applications of AI-driven Q&amp;A systems in academic settings, paving the way for future developments in automated educational assistance.},
  booktitle = {Proceedings of the 2024 the 12th International Conference on Information Technology (ICIT)},
  pages = {299–305},
  numpages = {7},
  keywords = {Question-Answering System,Computer Science Courses,Mdoel-View-Controller},
  location = {},
}

@inproceedings{10.1145/3716554.3716589,
