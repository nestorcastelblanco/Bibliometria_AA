 publisher = {Association for Computing Machinery},
 series = {GAIIS '24},
 title = {Applying DMC of Segmentation Strategy of Reward in Military Chess Game},
 url = {https://doi.org/10.1145/3665348.3665379},
 year = {2024}
}

@inproceedings{10.1145/3665348.3665380,
 abstract = {In general, grammar polishing systems recombine words and their order of combination based on the comparison results of word combinations and large vocabulary grammar data to achieve error polishing. However, this process is limited by the storage capacity of the large vocabulary grammar, which hampers the effectiveness and efficiency of error polishing. To overcome this constraint, deep learning algorithms are employed. These algorithms involve reconstructing the underlying hardware framework and optimizing the error polishing calculation parameters of the system software using deep learning algorithms. This allows for global replacement of deep learning algorithms, enhances the system's self-learning ability for error polishing, and frees it from the limitations of large vocabulary grammar information. The test results demonstrate that the continuous error polishing ability of this approach is superior. Among the error polishing systems tested, the designed system exhibits the best overall stress test performance, showcasing strong stability and control. The overall stability demonstrated by the stress test aligns with the standards and requirements of practical applications. As a result, the design system possesses good overall error polishing ability, system stability, and practical applicability.},
 address = {New York, NY, USA},
 author = {Yan, Liuqing and Yu, Shihu},
 booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
 doi = {10.1145/3665348.3665380},
 isbn = {9798400709562},
 location = {Kuala Lumpur, Malaysia},
 numpages = {5},
 pages = {179–183},
 publisher = {Association for Computing Machinery},
 series = {GAIIS '24},
 title = {Study on English expression polishing system based on deep learning},
 url = {https://doi.org/10.1145/3665348.3665380},
 year = {2024}
}

@inproceedings{10.1145/3665348.3665381,
 abstract = {This paper focuses on the multimodal transportation path optimization problem for consumer electronics products under the condition of transport time uncertainty. Approaching the problem from the perspective of multimodal transport operators within a defined network, the study integrates the stochastic travel time, stochastic transfer time, customer demands for timely delivery and the complexity of multiple orders. Combining with the high-value time-sensitive characteristics of consumer electronic products and taking the minimum of total cost as the optimization objective, we establish a multimodal transport path optimization model. In order to solve this model, a genetic algorithm with elite strategy based on Monte Carlo sampling is designed. Finally, we verify the validity of the model and algorithm through a specific case. And the case results are analyzed. The case results indicate that for the long-distance transportation of goods such as consumer electronics, which have a very high time-value cost, it is more appropriate to choose a multimodal transportation scheme based on road and railroad transportation, while waterway transportation does not have an advantage. In addition, further consideration of the uncertainty of the transportation time results in a transportation solution that ensures a higher probability of on-time delivery, while the total cost increases slightly but the transportation time decreases. The transportation scheme is more stable. The results of this paper provide a reference basis for the actual multimodal transportation path optimization problem of consumer electronics.},
 address = {New York, NY, USA},
 author = {Yuan, Mengzhen and Lu, Lili},
 booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
 doi = {10.1145/3665348.3665381},
 isbn = {9798400709562},
 location = {Kuala Lumpur, Malaysia},
 numpages = {10},
 pages = {184–193},
 publisher = {Association for Computing Machinery},
 series = {GAIIS '24},
 title = {Multimodal transportation path optimization of consumer electronics products considering transportation time uncertainty},
 url = {https://doi.org/10.1145/3665348.3665381},
 year = {2024}
}

@inproceedings{10.1145/3665348.3665382,
 abstract = {To ensure the safe transportation of hazardous goods while minimizing transportation costs, a multi-objective optimization method is adopted to determine the optimal driving path of hazardous goods transportation vehicles. By simplifying the quantification process of risk factors for transportation vehicles passing through densely populated areas, with the goal of minimizing the number of vehicles, the total transportation distance, and the shortest driving distance through densely populated areas, an optimization model for the transportation path problem of hazardous goods vehicles is established. A multi-objective evolutionary algorithm based on probability model is designed for the model. Taking a hazardous material transportation network with 5 transportation vehicles, 24 demand nodes, and 6 densely populated areas as an example, validate the model and algorithm. The example results show that the Pareto solution of the vehicle routing problem can be obtained using this algorithm, providing decision support for the scheduling of hazardous goods transportation vehicles.},
 address = {New York, NY, USA},
 author = {Li, Wenbo},
 booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
 doi = {10.1145/3665348.3665382},
 isbn = {9798400709562},
 location = {Kuala Lumpur, Malaysia},
 numpages = {7},
 pages = {194–200},
 publisher = {Association for Computing Machinery},
 series = {GAIIS '24},
 title = {Multi-objective optimization of the route problem for hazardous goods transportation},
 url = {https://doi.org/10.1145/3665348.3665382},
 year = {2024}
}

@inproceedings{10.1145/3665348.3665383,
 abstract = {AI-pair programming is a novel approach to software development that involves a human developer and an AI assistant working together on the same code. AI-pair programming aims to improve the productivity, quality, and creativity of software development by leveraging the complementary strengths of human and AI agents. However, there is a lack of empirical evidence on its impact on code quality and developer satisfaction. In this paper, we conduct a case study of TiMi studio, a leading mobile game development studio group that has been using AI-pair programming tools since 2023, and has produced several popular and high-quality games. We collect and analyze data from code repositories, code reviews, developer surveys, and developer interviews, to measure and compare the code quality and developer satisfaction of AI-pair and non-AI-pair projects. The results show that AI-pair programming has a positive impact on code quality and developer satisfaction, as it offers various benefits, such as time-saving, error-avoiding, skill-learning, possibility-exploring, feedback-receiving, and quality-improving. The results also reveal some challenges, such as reliability-questioning, explainability-questioning, trust-lacking, communication-lacking, autonomy-losing, and motivation-losing, which need to be addressed and overcome by improving and enhancing the AI-pair programming tools and practices. The paper provides empirical and practical evidence on the impact of AI-pair programming on code quality and developer satisfaction, and provides useful and actionable insights and guidance for developers, managers, researchers, and educators, who are interested in or involved in AI-pair programming, and who want to improve and optimize their software development process and outcomes.},
 address = {New York, NY, USA},
 author = {Chen, Tianyi},
 booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
 doi = {10.1145/3665348.3665383},
 isbn = {9798400709562},
 location = {Kuala Lumpur, Malaysia},
 numpages = {5},
 pages = {201–205},
 publisher = {Association for Computing Machinery},
 series = {GAIIS '24},
 title = {The Impact of AI-Pair Programmers on Code Quality and Developer Satisfaction: Evidence from TiMi studio},
 url = {https://doi.org/10.1145/3665348.3665383},
 year = {2024}
}

@inproceedings{10.1145/3665348.3665384,
 abstract = {Although various methods have been proposed to improve the robustness of deep neural network models, evaluating these models in a fair and reasonable manner remains a challenge. Existing evaluation methods often consider only limited types of attacks, ignoring the generalization performance against new types of attacks, and fail to cover the most advanced defense models. To address these issues, we propose a unified framework to evaluate model robustness under different types of attacks. This framework integrates non-Lp and Lp attacks, and comprehensively evaluates the robustness of various advanced robust models on the CIFAR-10 and ImageNet subsets. Experimental results show that even the most advanced defense models exhibit vulnerabilities under certain new types of attacks, highlighting the importance of developing more comprehensive robustness benchmarks and providing guidance for the design of future robust deep learning models.},
 address = {New York, NY, USA},
 author = {Liu, Wenzhao and Yang, Kuiwu and Chen, Yue and Dai, Huanyao},
 booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
