@inbook{10.1145/3696630.3728723,
  author = {Freeman, Laura and Robert, John and Wojton, Heather},
  title = {The Impact of Generative AI on Test \&amp; Evaluation: Challenges and Opportunities},
  year = {2025},
  isbn = {9798400712760},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3696630.3728723},
  abstract = {Generative Artificial Intelligence (GenAI) is transforming software development processes, including test and evaluation (T&amp;E). From automating test case design to enabling continuous testing in DevOps pipelines, AI-driven tools enhance the efficiency, accuracy, and speed of software testing. At the same time, the integration of AI components into software-reliant systems introduces new challenges for verification and validation (V&amp;V). Traditional T&amp;E methodologies must evolve to address issues such as AI bias, hallucinated outputs, and the complexity of validating non-deterministic behaviors. This position paper examines how existing T&amp;E methods must evolve to account for AI's stochastic nature, and conversely how GenAI is transforming T&amp;E practices across the software development lifecycle (SDLC).},
  booktitle = {Proceedings of the 33rd ACM International Conference on the Foundations of Software Engineering},
  pages = {1376–1380},
}

@inproceedings{10.1145/3733006.3733035,
  author = {Huang, Siqi and Li, Shengzhu},
  title = {The Impact of Government Subsidies on the Diffusion of Artificial Intelligence in the Hospital Market},
  year = {2025},
  isbn = {9798400713446},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3733006.3733035},
  doi = {10.1145/3733006.3733035},
  abstract = {This study looks into the best approaches for government subsidies to promote the spread of artificial intelligence (AI) technology in healthcare. It examines two different subsidy strategies and creates an evolutionary game model of AI medical technology diffusion in the context of a Barab\'{a}si-Albert (BA) network. The two subsidy strategies are analyzed in detail, factoring in the maturity of the technology, the initial proportion of firms, and the initial proportion of hospitals. The study finds that: 1) Government subsidies fixed in advance negatively affect technology diffusion when the maturity of the technology is uncertain. 2) Increased proportion of high-share hospitals increased the spread of AI technology because the enhanced market share benefits the firms using AI technology which causes them to attract other firms to copy that strategy. 3) From the perspective of the uncertainty of effectiveness, the adaptive subsidies perform better than fixed subsidies across all scenarios. 4) A higher initial proportion of hospitals using AI results in greater spreads of AI technology. The governments are recommended to use adaptive subsidies and to focus support on hospitals in order to reduce the barriers to entry. The government should also provide infrastructure and establish more higher-level hospitals to allow for the more effective use of resources.},
  booktitle = {Proceedings of the 2025 International Conference on Health Big Data},
  pages = {175–180},
  numpages = {6},
  keywords = {Artificial Intelligence Healthcare Systems, Network-based evolutionary game, Subsidy policy, Technology diffusion},
  location = {},
}

