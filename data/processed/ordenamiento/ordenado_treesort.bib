@inproceedings{10.1145/3687311.3687335,
  author = {He, Han},
  title = {Exploration of Intelligent Evaluation of Teacher Teaching Reflection Evaluation Index System Based on Artificial Intelligence},
  year = {2024},
  isbn = {9798400709920},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3687311.3687335},
  doi = {10.1145/3687311.3687335},
  abstract = {TRE (teaching reflection evaluation) is a continuous process, which helps to improve teaching quality, stimulate students' interest, meet their needs, and support the mission and objectives of educational institutions. Intelligent assessment of teachers' teaching reflection is a method to support and enhance teachers' reflection and self-evaluation by using AI (artificial intelligence) technology. Based on the index system of teaching reflection of university teachers, this paper studies the design of intelligent evaluation system of teaching reflection of university teachers with AI as technical support and Matlab programming. The mathematical model is established by using BPNN (BP neural network) model structure, and the neural network is trained by using expert samples. According to the scores of online students on various evaluation indexes of teachers' classroom teaching, the trained neural network is used for data processing, so as to obtain the final evaluation result. The experimental data show that the training and prediction accuracy of teacher TRE model based on BPNN is completely within the acceptable range, which is a reasonable and feasible prediction model and can objectively and fairly evaluate teachers' teaching reflection.},
  booktitle = {Proceedings of the 2024 International Conference on Intelligent Education and Computer Technology},
  pages = {129–133},
  numpages = {5},
  location = {Guilin, China},
}

@inproceedings{10.1145/3652628.3652756,
  author = {Zhu, Jingyi},
  title = {Exploration on Digital Economy Data Processing System under Artificial Intelligence Algorithm},
  year = {2024},
  isbn = {9798400708831},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3652628.3652756},
  doi = {10.1145/3652628.3652756},
  abstract = {In the digital economy data processing, traditional data processing methods face the challenge of relying on manual intervention and the risk of data leakage. Data is threatened during transmission and storage, which can lead to the leakage of sensitive information, while manual operations can lead to errors and low efficiency. In contrast, the LSTM (Long Short Term Memory) algorithm based on artificial intelligence can effectively capture and process long-term dependencies in sequential data, and can automatically learn and adapt to patterns on different time scales, thus promoting the development of a series of data processing methods. Through relevant research on the development background and data processing needs of the current digital economy, and with the application of LSTM in digital economy data processing, combined with specific instructions on processing and predicting time series data, natural language processing, and user behavior analysis, this paper designed a digital economy data processing system. Through two sets of simulation experiments, the following conclusion was drawn: selecting six different types of dataset samples, the accuracy evaluation index of the digital economy data processing optimization system improved by an average of about 9.8\%, while the processing speed evaluation index improved by an average of 17.85\%. This indicated that the digital economic data processing system based on LSTM has good application effects.},
  booktitle = {Proceedings of the 4th International Conference on Artificial Intelligence and Computer Engineering},
  pages = {767–772},
  numpages = {6},
  location = {Dalian, China},
}

