 location = {},
 numpages = {5},
 pages = {83–87},
 publisher = {Association for Computing Machinery},
 series = {BIC '25},
 title = {Early Cancer Detection Combining Computer Artificial Intelligence with CT Medical Imaging: Application of Big Data Approach},
 url = {https://doi.org/10.1145/3724979.3724993},
 year = {2025}
}

@inproceedings{10.1145/3724979.3725056,
 abstract = {In recent years, mental health issues among college students have been on the rise, with frequent concerns such as anxiety, depression, and heightened stress. As a result, there is an increasing interest in and demand for psychological guidance and counseling. Issues related to mental wellness are particularly common during adolescence and are beginning to affect younger demographics more frequently. Among individuals aged 18-24, the risk of depression has been detected at a rate of 24.1\%. The college years represent a crucial phase in student development, characterized by academic demands, social hurdles, and shifts in self-awareness, all of which can significantly influence mental well-being. As such, it is becoming ever more crucial for colleges to prioritize the prevention of mental health challenges among their students and to offer counseling and therapeutic services. This paper delves into the significance of psychological counseling alongside the foundational concepts of artificial intelligence. By analyzing data from a sample of 566 college students, it utilizes t-tests and chi-square analyses to investigate the current implementation of AI in psychological counseling. Utilizing machine learning techniques in conjunction with predictive models powered by big data and AI algorithms, the study organizes and processes fundamental student information. Chi-square analyses are employed to explore the links between different mental health dimensions and personal characteristics, providing colleges with insights for assessing and supporting students' mental well-being.},
 address = {New York, NY, USA},
 author = {Zhou, Xuan and Sui, Huajie and Guo, Yuhong and Zou, Ziyi},
 booktitle = {Proceedings of the 2025 5th International Conference on Bioinformatics and Intelligent Computing},
