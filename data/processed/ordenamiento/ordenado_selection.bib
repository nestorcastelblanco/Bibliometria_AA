@inproceedings{10.1145/3746175.3746183,
 abstract = {Generative AI tools are increasingly embedded in creative workflows, yet their outputs often misalign with user expectations, producing results that are ambiguous, contradictory, or oddly off-mark. Rather than treating such misalignments as failures to be corrected, this paper reframes them as sources of creative ambiguity and cognitive tension. Drawing on theories of cognitive dissonance, frame conflict, and prediction error, I show how these tensions can catalyze reflection, reinterpretation, and creative reframing. Through five cross-modal cases spanning writing, design, music, code, and prototyping I demonstrate how creative ambiguity and cognitive tension function as generative forces in human–AI interaction. This work contributes to HCI and creativity support by proposing a shift from optimization toward reflection, designing not to minimize friction, but to work with it.},
 address = {New York, NY, USA},
 articleno = {4},
 author = {Dalsgaard, Peter},
 booktitle = {Proceedings of the 36th Annual Conference of the European Association of Cognitive Ergonomics},
 doi = {10.1145/3746175.3746183},
 isbn = {9798400720338},
 keywords = {Creativity, Human-Computer Interaction, Generative Artificial Intelligence, Creative Ambiguity, Cognitive Tension, Creativity-Support Tools, Design},
 location = {},
 numpages = {10},
 publisher = {Association for Computing Machinery},
 series = {ECCE '25},
 title = {Creative Ambiguity and Cognitive Tension in Generative AI Tools},
 url = {https://doi.org/10.1145/3746175.3746183},
 year = {2025}
}

@inproceedings{10.1145/3746175.3746199,
 abstract = {With ‘Generative Artificial Intelligence’ (GenAI), a new momentum has swept public opinion and, as with the Times front page on 27 February 2023, which describes an ‘arms race’ among major companies. While the GenAI in the workplace is new, the few studies suggest several benefits of such systems in work contexts. However, these results focus on the quantitative indicators, and further literature on the implementation of generative AI systems at work is still scarce. In this context, it is important to specify how to integrate a GenAI-based tool into an existing socio-technical organisation, as such integration always deeply transforms the employees’ activity. In this paper, we discuss the human-centred design of GenAI-based tools on the basis of an ergonomics intervention for the Ombudsman's team of an energy utility company. We explore how a participatory design approach – grounded in the preliminary analysis of the work – can help inform the development of a GenAI-based tool. We discuss the relevance of this methodology and the intermediary role of the ergonomists, given the dual challenge that end-users often lack a comprehensive understanding of GenAI's possibilities and potential applications, while data-scientists responsible for developing such tools are not trained to analyse complex work activities, which is a necessary step for selecting and training the appropriate AI models.},
 address = {New York, NY, USA},
 articleno = {18},
 author = {H\'{e}ron, Robin and Fr\'{e}jus, Myriam},
 booktitle = {Proceedings of the 36th Annual Conference of the European Association of Cognitive Ergonomics},
 doi = {10.1145/3746175.3746199},
 isbn = {9798400720338},
 keywords = {GenAI, HCAI},
 location = {},
 numpages = {8},
 publisher = {Association for Computing Machinery},
 series = {ECCE '25},
 title = {Bridging the conceptual gap between workers and data-scientists' needs in the design process of a GenAI-based tool: an ergonomics intervention for an ombudsman's team},
 url = {https://doi.org/10.1145/3746175.3746199},
 year = {2025}
}

@inproceedings{10.1145/3746175.3746208,
 abstract = {Scientific creativity (i.e., to innovate new scientific approaches or ideas) is a fundamental ability for scientists. Since the rise of artificial intelligence (AI), researchers have used specialized models to improve or automatize scientific processes and even scientific discoveries. In our study, we investigate whether proprietary large language models such as ChatGPT, though not specifically trained for scientific work, are able to convincingly perform or even surpass human scientific creativity. We asked graduate students and prompted ChatGPT to write an abbreviated version of a research proposal for a fictious scenario from the field of computer science/ robotics. Four specific assignments were given (i.e., formulating a hypothesis, designing an experiment, listing the required equipment, and justifying the chosen method). Human and AI-generated answers were rated by two scientists from the field, following a peer-review approach. Our results indicate that ChatGPT reached significantly higher overall scores relative to the human sample. However, closer inspection of the data and the rating procedure revealed that the evaluation of performance especially in regards to the originality of ideas differed considerably between the two raters. We evaluate these findings in more detail, as well as qualitative differences between human and AI- generated responses. Lastly, we discuss the impact of raters’ knowledge about and attitudes towards Generative AI and emphasize the importance of meta-cognitive skills to identify Generative AI.},
 address = {New York, NY, USA},
 articleno = {41},
 author = {Eymann, Vera and Lachmann, Thomas and Hekele, Felix and Ashok, Ashita and Berns, Karsten and Czernochowski, Daniela},
 booktitle = {Proceedings of the 36th Annual Conference of the European Association of Cognitive Ergonomics},
 doi = {10.1145/3746175.3746208},
 isbn = {9798400720338},
 keywords = {Scientific Creativity; Artificial Intelligence; Creativity; Generative AI},
 location = {},
 numpages = {5},
 publisher = {Association for Computing Machinery},
 series = {ECCE '25},
 title = {Does ChatGPT know what robots are thinking? - Evaluation of human-created vs. AI-generated scientific ideas},
 url = {https://doi.org/10.1145/3746175.3746208},
 year = {2025}
}

@inproceedings{10.1145/3746175.3747192,
 abstract = {Prompts are central to creative work with generative AI systems, yet they are often treated as simple and static interface components. This paper proposes a different view: prompts function as cognitive interfaces that mediate between user intent and algorithmic generation. Integrating theories of external cognition, interface semiotics, and pragmatist theories of situated inquiry, the paper provides a theoretical framing of prompting along three interrelated dimensions: semantic projection, compositional structuring, and interpretive negotiation. Through an empirical vignette from branding communication, I show how prompting can be seen as a dynamic representational activity, deeply embedded in cognitive processes of exploration, articulation, and iterative refinement. By reframing the prompt as a site of cognitive labor rather than as a static interface, this expands our understanding of interaction with generative AI as epistemic, semiotic, and situated.},
 address = {New York, NY, USA},
 articleno = {24},
 author = {Dalsgaard, Peter},
 booktitle = {Proceedings of the 36th Annual Conference of the European Association of Cognitive Ergonomics},
 doi = {10.1145/3746175.3747192},
