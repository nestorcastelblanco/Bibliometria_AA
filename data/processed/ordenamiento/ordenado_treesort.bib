@inproceedings{10.1145/3671151.3671290,
  author = {Lee, Siyuan},
  title = {Fundamental Trends and Hot-Spots in Public Governance and Artificial Intelligence Research: A Bibliometric Study Analysis From 2013 to 2023},
  year = {2024},
  isbn = {9798400718106},
