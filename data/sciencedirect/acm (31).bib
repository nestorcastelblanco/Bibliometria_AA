@article{10.1145/3649453,
author = {Langford, Michael Austin and Zilberman, Sol and Cheng, Betty},
title = {Anunnaki: A Modular Framework for Developing Trusted Artificial Intelligence},
year = {2024},
issue_date = {September 2024},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
volume = {19},
number = {3},
issn = {1556-4665},
url = {https://doi.org/10.1145/3649453},
doi = {10.1145/3649453},
abstract = {Trustworthy artificial intelligence (Trusted AI) is of utmost importance when learning-enabled components (LECs) are used in autonomous, safety-critical systems. When reliant on deep learning, these systems need to address the reliability, robustness, and interpretability of learning models. In addition to developing strategies to address these concerns, appropriate software architectures are needed to coordinate LECs and ensure they deliver acceptable behavior even under uncertain conditions. This work describes Anunnaki, a model-driven framework comprising loosely-coupled modular services designed to monitor and manage LECs with respect to Trusted AI assurance concerns when faced with different sources of uncertainty. More specifically, the Anunnaki framework supports the composition of independent, modular services to assess and improve the resilience and robustness of AI systems. The design of Annunaki was guided by several key software engineering principles (e.g., modularity, composability, and reusability) in order to facilitate its use and maintenance to support different aggregate monitoring and assurance analysis tools for LESs and their respective data sets. We demonstrate Anunnaki on two autonomous platforms, a terrestrial rover, and an unmanned aerial vehicle. Our studies show how Anunnaki can be used to manage the operations of different autonomous learning-enabled systems with vision-based LECs while exposed to uncertain environmental conditions.},
journal = {ACM Trans. Auton. Adapt. Syst.},
month = sep,
articleno = {17},
numpages = {34},
keywords = {Software engineering, models at run time, self-adaptive systems, artificial intelligence, deep learning}
}

@inproceedings{10.1145/3689236.3689240,
author = {Tao, Yuan and Chang, Yue},
title = {Research on Deep Forgery Defense Technology Based on Artificial Intelligence Security},
year = {2024},
isbn = {9798400718137},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3689236.3689240},
doi = {10.1145/3689236.3689240},
abstract = {Deep forgery usually refers to the technique of multimedia forgery using deep learning. This technique if applied in illegal ways will threaten the security of individuals and society, in order to ensure the sustainable development of artificial intelligence technology and reduce its negative impact aspects, this study proposes an innovative detection model that aims to improve the accuracy of deep forgery content detection by combining the advantages of convolutional networks and capsule networks-BIGRU. The research methodology consists of first extracting spatial features of the image using a convolutional neural network (CNN), followed by capturing the positional information of the object using a capsule network, and capturing temporal features between video frames using a gated recurrent unit (GRU). The combined application of these techniques is extensively tested on multiple sample datasets. The experimental results show that the present model outperforms existing mainstream approaches in detecting deeply forged content, which not only proves the effectiveness of the model, but also demonstrates its potential in improving the security performance of AI. Ultimately, this study concludes that the accuracy of deep forgery detection can be significantly improved by comprehensively utilizing CNNs, capsule networks, and GRUs, which provides strong technical support for the healthy development of AI technology.},
booktitle = {Proceedings of the 2024 9th International Conference on Cyber Security and Information Engineering},
pages = {7–11},
numpages = {5},
keywords = {Artificial intelligence security, BIGRU, Capsule network, Deep forgery, Deep learning},
location = {
},
series = {ICCSIE '24}
}

@article{10.1145/3675392,
author = {chander, Bhanu and John, Chinju and Warrier, Lekha and Gopalakrishnan, Kumaravelan},
title = {Toward Trustworthy Artificial Intelligence (TAI) in the Context of Explainability and Robustness},
year = {2025},
issue_date = {June 2025},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
volume = {57},
number = {6},
issn = {0360-0300},
url = {https://doi.org/10.1145/3675392},
doi = {10.1145/3675392},
abstract = {From the innovation, Artificial Intelligence (AI) materialized as one of the noticeable research areas in various technologies and has almost expanded into every aspect of modern human life. However, nowadays, the development of AI is unpredictable with the stated values of those developing them; hence, the risk of misbehaving AI increases continuously. Therefore, there are uncertainties about endorsing that the development and deploying of AI are favorable and not unfavorable to humankind. In addition, AI holds a black-box pattern, which results in a lack of understanding of how systems can work based on the raised concerns. From the above discussion, trustworthy AI is vital for the extensive adoption of AI in many applications, with strong attention to humankind and the need to focus on AI systems developing into the system outline at the time of system design. In this survey, we discuss compound materials on trustworthy AI and the present state-of-the-art of trustworthy AI technologies, revealing new perspectives, bridging knowledge gaps, and paving the way for potential advances of robustness, and explainability rules which play a proactive role in designing AI systems. Systems that are reliable and secure and mimic human behavior significantly impact the technological AI ecosystem. We provide various contemporary technologies to build explainability and robustness for AI-based solutions, so AI works in a safer and more trustworthy manner. Finally, we conclude our survey article with high-end opportunities, challenges, and future research directions for trustworthy AI to investigate in the future.},
journal = {ACM Comput. Surv.},
month = feb,
articleno = {144},
numpages = {49},
keywords = {Artificial intelligence, machine learning, deep learning, trustworthy AI, explainability, robustness, decision-making, fairness, data privacy}
}

@inbook{10.1145/3745238.3745246,
author = {Ding, Mingyi and Zheng, Qidong},
title = {Research on Performance Assessment and Business-Finance Integration Based on Generative Adversarial Network Model},
year = {2025},
isbn = {9798400712791},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3745238.3745246},
abstract = {Against the backdrop of the rapid development of digital economy and artificial intelligence technology, performance appraisal and business-finance integration (BFI) have become key issues in improving enterprise management efficiency and scientific decision-making. Traditional performance appraisal methods often have problems such as single data dimension and subjective setting of appraisal indicators, making it difficult to effectively evaluate the synergy between finance and business. To this end, this paper proposes an intelligent performance evaluation method based on generative adversarial network (GAN) to improve the intelligence level of the appraisal system. The study uses conditional generative adversarial network (CGAN) to construct a performance evaluation framework, establishes a high-dimensional performance scoring model through the collaborative input of business data and financial data, and optimizes the rationality and accuracy of the scoring results under the adversarial training mechanism. Experiments show that compared with traditional linear regression models and decision tree models, the performance appraisal scoring error generated by GAN is reduced by 18.4\%, and the performance prediction accuracy is increased by 21.7\%. This method can effectively improve the scientific nature of enterprise performance appraisal, promote the in-depth development of business-finance integration, and provide theoretical support and practical guidance for enterprise intelligent management.},
booktitle = {Proceedings of the 2nd Guangdong-Hong Kong-Macao Greater Bay Area International Conference on Digital Economy and Artificial Intelligence},
pages = {38–42},
numpages = {5}
}

