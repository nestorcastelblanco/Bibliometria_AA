@inproceedings{10.1145/3632971.3632996,
  author = {Xiao, Xiao Cheng},
  title = {Research on the Application of Artificial Intelligence in Comparative Analysis of Man-machine Translation},
  year = {2024},
  isbn = {9798400707704},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3632971.3632996},
  doi = {10.1145/3632971.3632996},
  abstract = {Due to the inherent differences in grammar structures, cultural customs, and social contexts among various languages, the current efficacy of machine translation still falls short of meeting individuals' diverse and nuanced needs. Merely isolated training in translation skills proves inadequate in nurturing the caliber of high-quality translation professionals required today. In this regard, a comprehensive translation education must encompass practical skills and a strong foundation in theoretical knowledge. Consequently, there arises a necessity for the integration of vocational translation education with a substantial theoretical and academic framework. In light of this, the present study delves into the utilization of Artificial Intelligence (AI) technology for a comparative analysis between human and machine translations. The research findings underscore the promising potential of the proposed AI-driven approach, showcasing a relatively robust accuracy rate of 73.96\% across human-machine translation evaluations. These outcomes undeniably highlight the efficacy of the method elucidated in the study's framework, particularly in the realm of human-machine translation evaluations. The implications of these findings are far-reaching, potentially smoothing the transition into the field of translation, especially for individuals endowed with broad-ranging knowledge and adept oral communication skills. Such proficiencies not only constitute fundamental requisites for advanced interpretation training but also serve as pivotal prerequisites for cultivating highly proficient English-Chinese communicators.},
  booktitle = {Proceedings of the 2023 International Joint Conference on Robotics and Artificial Intelligence},
  pages = {48–52},
  numpages = {5},
  keywords = {Artificial intelligence, Contrastive analysis, Man-machine translation},
  location = {Shanghai, China},
}

@inproceedings{10.1145/3691016.3691059,
  author = {Teng, Chao and Mao, Fengshan},
  title = {Research on the Application of Artificial Intelligence in Underwater DCM Intensity Prediction},
  year = {2024},
  isbn = {9798400710285},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3691016.3691059},
  doi = {10.1145/3691016.3691059},
  abstract = {After the construction of Deep Cement Mixing (DCM) piles underwater, due to the formed strength, it is difficult to repair any unqualified pile. The only option is to supplement with additional piles in the nearby area. However, in some construction areas with high requirements for project quality and settlement, full-scale reinforcement is needed, leaving no room even for supplementary piling, posing challenges to quality control. There is a large amount of research on the strength prediction of cement soil indoor mix ratio tests both domestically and internationally, but they fail to consider the parameter changes during actual construction. In this paper, based on real-time raw data from inspection piles of actual construction projects, a "survey-construction-inspection" associated data set is established. Quality prediction is performed using three methods: BP neural network, random forest, and support vector machine regression. Among them, the random forest has the best prediction effect, with a mean square error of 0.86MPa in the test set and an average relative error of 15.8\%. Based on this technology, the quality of the formed piles can be predicted immediately after construction. Pile sections at risk of poor quality can be timely grouted and mixed to improve project quality.},
  booktitle = {Proceedings of the 2024 International Conference on Image Processing, Intelligent Control and Computer Engineering},
  pages = {261–266},
  numpages = {6},
  location = {Qingdao, China},
}

