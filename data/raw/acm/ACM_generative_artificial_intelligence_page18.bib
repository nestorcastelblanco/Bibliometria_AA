doi = {10.1145/3745238.3745449},
abstract = {Socio-technological developments are driving the profound impact of artificial intelligence (AI) technologies on education. Driven by the “disruptive technology revolution” in different fields, AI is reshaping knowledge acquisition, teaching and learning, from personalized learning systems to the development of brain-computer interfaces (BCIs), the traditional “factory” mode of education is difficult to adapt to personalized learning development needs. Learning development needs, and artificial intelligence-driven systems in support of adaptive learning, intelligent tutoring and automatic assessment, in addition to greatly improve the learning efficiency “liberation of productivity”, but also to change the way of knowledge transfer, affect human cognition and independent learning. It is foreseeable that the competition for education in the age of artificial intelligence may shift from knowledge accumulation to computational power domination. That is to say, individuals with more computing power will have access to better learning resources, which will definitely increase the “digital education gap” on the other hand. This paper analyzes the current situation, challenges, and future trends of AI in education, and explores the impact of AI on self-directed learning, cognitive processes, and the possible evolution of education towards computational competitiveness. Using research methods such as literature review, case study, and technology trend prediction, this study combines the theories of cognitive science, philosophy of AI, and pedagogy to assess the educational impact of AI and propose policy recommendations to promote equity and optimize AI cooperation.},
booktitle = {Proceedings of the 2nd Guangdong-Hong Kong-Macao Greater Bay Area International Conference on Digital Economy and Artificial Intelligence},
pages = {1355–1360},
numpages = {6},
keywords = {artificial intelligence, computing power competition, educational equity, personalized learning},
location = {
},
series = {DEAI '25}
}

@article{10.1145/3759243,
author = {Chen, Long and Huang, Yuzhi and Dong, Junyu and Xu, Qi and Kwong, Sam and Lu, Huimin and Lu, Huchuan and Li, Chongyi},
