 articleno = {26},
 author = {Storey, Veda Catherine},
 doi = {10.1145/3719209},
 issn = {2158-656X},
 issue_date = {September 2025},
 journal = {ACM Trans. Manage. Inf. Syst.},
 keywords = {Knowledge management, generative AI (GenAI), novice knowledge workers, expert knowledge workers, Framework for Knowledge Management in the GenAI Era, risk assessment, organizational knowledge, Knowledge shifting},
 month = {September},
 number = {3},
 numpages = {14},
 publisher = {Association for Computing Machinery},
 title = {Knowledge Management in a World of Generative AI: Impact and Implications},
 url = {https://doi.org/10.1145/3719209},
 volume = {16},
 year = {2025}
}

@inproceedings{10.1145/3719236.3719242,
 abstract = {This paper explores the conceptual implementation of an application design, previously developed by communication design students. In an increasingly digitalised world, making cultural heritage accessible and approachable to diverse visitors is critical. The concept idea centres on an interactive video call with the patron of a Renaissance sandstone altar exhibited at the museum. The technical implementation includes generative AI tools for text, audio and animation production. Issues such as ensuring the accuracy of information, maintaining user privacy and preventing biases in AI-generated content are identified. The importance of co-creation with experts and adherence to ethical standards are highlighted to create a reliable educational tool. This implementation proposal forms the foundation for further development and future deployment as part of the museum’s permanent exhibition, aiming to transform how visitors experience and interact with cultural heritage artefacts.},
