@inproceedings{10.1145/3727353.3727393,
  author = {Li, Baohong},
  title = {Lntelligent drive marketing: Big data and artificial intelligence precise strategy decoding},
  year = {2025},
  isbn = {9798400712425},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
