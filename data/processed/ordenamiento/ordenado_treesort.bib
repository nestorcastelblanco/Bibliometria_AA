@inproceedings{10.1145/3746469.3746605,
  author = {Chang, Le},
  title = {Reconstruction of Writing instruction with Generative AI in SAMR Framework: A Multimodal Empirical Study},
  year = {2025},
  isbn = {9798400713811},
  publisher = {Association for Computing Machinery},
  address = {New York, NY, USA},
  url = {https://doi.org/10.1145/3746469.3746605},
  doi = {10.1145/3746469.3746605},
  abstract = {As a case of education digital transformation, traditional English writing pedagogy is difficult to truly encourage the interest of the students on writing and develop writing ability. A joint of generative AI and SAMR model offers innovation potential to writing teaching, but fewer studies exist on its implementation in vocational English writing teaching. This study enlisted students from two comparable classes of writing courses in a vocational school. The control group was instructed in generative AI supported writing instruction based on the SAMR model, while the control group was instructed in traditional writing instruction. Writing tests were given before and after teaching, and score data were collected and analyzed in terms of several aspects. The test results showed that the experimental group students' writing scores improved significantly with a dense distribution of scores and the number of students scoring high. The improvement in the control group was smaller, with a diffuse distribution and wide fluctuation. Independent sample t-tests also confirmed the effectiveness of the new teaching method. The study provides strong evidence for the reform of English writing education in vocational colleges.},
  booktitle = {Proceedings of the 2nd Guangdong-Hong Kong-Macao Greater Bay Area Education Digitalization and Computer Science International Conference},
  pages = {877–881},
  numpages = {5},
  keywords = {Generative AI, Multimodal empirical research, SAMR framework, Writing instruction},
  location = {},
}

@inproceedings{10.5555/3716662.3716773,
  author = {Salavati, Chiman and Song, Shannon and Diaz, Willmar Sosa and Hale, Scott A. and Montenegro, Roberto E. and Murai, Fabricio and Dori-Hacohen, Shiri},
  title = {Reducing Biases towards Minoritized Populations in Medical Curricular Content via Artificial Intelligence for Fairer Health Outcomes},
  year = {2025},
  publisher = {AAAI Press},
  abstract = {Biased information (recently termed bisinformation) continues to be taught in medical curricula, often long after having been debunked. In this paper, we introduce Bricc, a first-in-class initiative that seeks to mitigate medical bisinformation using machine learning to systematically identify and flag text with potential biases, for subsequent review in an expert-in-the-loop fashion, thus greatly accelerating an otherwise labor-intensive process. We have developed a goldstandard Bricc dataset throughout several years containing over 12K pages of instructional materials. Medical experts meticulously annotated these documents for bias according to comprehensive coding guidelines, emphasizing gender, sex, age, geography, ethnicity, and race. Using this labeled dataset, we trained, validated, and tested medical bias classifiers. We test three classifier approaches: a binary type-specific classifier, a general bias classifier; an ensemble combining bias type-specific classifiers independently-trained; and a multitask learning (MTL) model tasked with predicting both general and type-specific biases. While MTL led to some improvement on race bias detection in terms of F1-score, it did not outperform binary classifiers trained specifically on each task. On general bias detection, the binary classifier achieves up to 0.923 of AUC, a 27.8\% improvement over the baseline. This work lays the foundations for debiasing medical curricula by exploring a novel dataset and evaluating different training model strategies. Hence, it offers new pathways for more nuanced and effective mitigation of bisinformation.},
  booktitle = {Proceedings of the 2024 AAAI/ACM Conference on AI, Ethics, and Society},
  pages = {1269–1280},
  numpages = {12},
  location = {San Jose, California, USA},
}

