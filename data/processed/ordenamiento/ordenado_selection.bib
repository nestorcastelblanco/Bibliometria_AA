 keywords = {Human-Computer Interaction, Generative Artificial Intelligence, Interaction Design, Prompts, Prompt Engineering},
 location = {},
 numpages = {6},
 publisher = {Association for Computing Machinery},
 series = {ECCE '25},
 title = {Thinking through Prompting: Cognitive Mediation in Human–AI Interaction},
 url = {https://doi.org/10.1145/3746175.3747192},
 year = {2025}
}

