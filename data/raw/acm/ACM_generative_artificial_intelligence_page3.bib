@inproceedings{10.1145/3766918.3766975,
author = {Wang, Yu and Jia, Yanrui and Guo, Fangwei},
title = {Research on the Application of Artificial Intelligence in Advertising Creativity in the Context of Metaverse Based on XGBoost},
year = {2025},
isbn = {9798400716027},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3766918.3766975},
doi = {10.1145/3766918.3766975},
abstract = {With the rapid development of information technology, the Metaverse has emerged as a new digital space. Under this perspective, advertising creativity has ushered in unprecedented changes and opportunities. The purpose of this paper is to explore how artificial intelligence (AI) empowers advertising creativity in the context of meta-universe and promotes its innovation and development. This study constructs a theoretical model with the AI participation effect as the independent variable, consumer perception as the mediator variable, and advertising expectation as the dependent variable to explore the specific influence mechanism of AI-enabled advertising creativity. Meanwhile, the XGBoost algorithm was used to construct a classification model. After data cleaning, standardization, and dividing the dataset into training and testing sets in a 7:3 ratio, parameters were optimized by combining 5-fold cross-validation. The model performance was evaluated using metrics such as accuracy and precision, and SHAP was employed to analyze feature importance. The results of the study show that the better the AI participation effect, the higher the consumer perception, and the better the advertising expectation, also consumer perception plays a mediating role between the AI participation effect and advertising expectation. On this basis, the new situation of future advertising creativity is proposed, namely, the polarization and dynamic generation of personalized ads, the paradigm change of intelligent creative production, and the intelligent placement and scenario expansion of ads.},
booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence for Business},
pages = {338–343},
numpages = {6},
keywords = {Advertising creativity, Advertising expectation, Artificial Intelligence, Consumer perception, XGBoost},
location = {
},
series = {GAIB '25}
}

@inproceedings{10.1145/3746469.3746516,
author = {Cheng, Guo and Liu, Peng},
title = {Research on the Intelligent Development Framework of Martial Arts Cultural Products Based on Generative AI and Multi modal Interaction},
year = {2025},
isbn = {9798400713811},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3746469.3746516},
doi = {10.1145/3746469.3746516},
